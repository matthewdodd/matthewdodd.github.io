               <p>532. 
               <persName id="t18070916-10-defend122" type="defendantName">
                  SOPHIA 
                  HUBBARD
               <interp inst="t18070916-10-defend122" type="surname" value="HUBBARD"></interp>
                     <interp inst="t18070916-10-defend122" type="given" value="SOPHIA"></interp>
