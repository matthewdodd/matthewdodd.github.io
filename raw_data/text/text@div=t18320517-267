             and 
               
                  <persName id="t18320517-267-defend1939" type="defendantName">
                     JOHN 
                     DAVIS
                  <interp inst="t18320517-267-defend1939" type="surname" value="DAVIS"></interp>
                     <interp inst="t18320517-267-defend1939" type="given" value="JOHN"></interp>
