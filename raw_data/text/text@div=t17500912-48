                           <interp inst="t17500912-48-person366" type="gender" value="male"></interp>
                        </persName>
                        
                        <interp inst="t17500912-48-victim365" type="surname" value="Boyd"></interp>
                        <interp inst="t17500912-48-victim365" type="given" value="Augustus"></interp>
                        <interp inst="t17500912-48-victim365" type="gender" value="male"></interp>
                     </persName>
                  , <rs id="t17500912-48-viclabel205" type="occupation">Esquires</rs>
                     <join result="persNameOccupation" targOrder="Y" targets="t17500912-48-person366 t17500912-48-viclabel205"></join>
