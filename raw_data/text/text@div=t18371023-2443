                        <join result="offenceCrimeDate" targOrder="Y" targets="t18371023-2443-offence-1 t18371023-cd-232"></join>20th of June</rs>, 1 spoon, value 2<hi rend="italic">s</hi>. 6<hi rend="italic">d</hi>.; 3 handkerchiefs, value 4<hi rend="italic">s</hi>.; 1 ring, value 6<hi rend="italic">d</hi>.; 1 medal, value 1<hi rend="italic">d</hi>.; 1 pair of scales and case, value 1<hi rend="italic">s</hi>.; 2 marbles, value 6<hi rend="italic">d</hi>.; 1 slipper, value 3<hi rend="italic">d</hi>.; 3 towels, value 3<hi rend="italic">s</hi>.; 1 d'oiley, value 16<hi rend="italic">d</hi>.; 1 shift, value 5<hi rend="italic">s</hi>.; 1 pair of stockings, value 1<hi rend="italic">s</hi>.; 1 tin box, value 1<hi rend="italic">s</hi>. 6<hi rend="italic">d</hi>.; 1 pincushion, value 6<hi rend="italic">d</hi>.; 3/4 of a yard of ribbon, value 6<hi rend="italic">d</hi>.; and 1 pillow case, value 1<hi rend="italic">s</hi>.; the goods of <persName id="t18371023-name-1554" type="victimName">
                        <interp inst="t18371023-name-1554" type="gender" value="male"></interp>
                        <interp inst="t18371023-name-1554" type="surname" value="HOPKINS"></interp>
