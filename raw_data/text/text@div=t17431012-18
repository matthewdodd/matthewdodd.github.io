            ; and 
               
                  <persName id="t17431012-18-defend193" type="defendantName">
                     Mary 
                     Hurst 
                  <interp inst="t17431012-18-defend193" type="surname" value="Hurst"></interp>
                     <interp inst="t17431012-18-defend193" type="given" value="Mary"></interp>
                     <interp inst="t17431012-18-defend193" type="gender" value="female"></interp>
