                     <join result="offenceCrimeDate" targOrder="Y" targets="t18150405-6-off27 t18150405-6-cd28"></join>, was <rs id="t18150405-6-deflabel29" type="occupation">servant</rs>
                     <join result="persNameOccupation" targOrder="Y" targets="t18150405-6-defend102 t18150405-6-deflabel29"></join> to 
                     
                        <persName id="t18150405-6-victim104" type="victimName">
                           Daniel 
                           Gadane
