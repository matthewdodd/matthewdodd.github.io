            . I am a <rs id="t17670909-79-viclabel474" type="occupation">shoemaker</rs>
                  <join result="persNameOccupation" targOrder="Y" targets="t17670909-79-victim869 t17670909-79-viclabel474"></join> in <placeName id="t17670909-79-crimeloc475">Pennington-street</placeName>
                  <interp inst="t17670909-79-crimeloc475" type="placeName" value="Pennington-street"></interp>
                  <interp inst="t17670909-79-crimeloc475" type="type" value="crimeLocation"></interp>
