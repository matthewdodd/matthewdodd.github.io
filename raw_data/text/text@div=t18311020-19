                     <interp inst="t18311020-19-off105" type="offenceCategory" value="theft"></interp>
                     <interp inst="t18311020-19-off105" type="offenceSubcategory" value="pocketpicking"></interp>
               stealing, on the <rs id="t18311020-19-cd106" type="crimeDate">10th of October</rs>
                     <join result="offenceCrimeDate" targOrder="Y" targets="t18311020-19-off105 t18311020-19-cd106"></join>, 1 handkerchief, value 2s. 6d., the goods of 
                     
                        <persName id="t18311020-19-victim258" type="victimName">
                           Lionel Prager 
                           Goldsmid
                        <interp inst="t18311020-19-victim258" type="surname" value="Goldsmid"></interp>
