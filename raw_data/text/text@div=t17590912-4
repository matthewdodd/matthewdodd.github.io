                  <persName id="t17590912-4-person79">
                  Peter 
                  Gascoyne 
               <interp inst="t17590912-4-person79" type="surname" value="Gascoyne"></interp>
                     <interp inst="t17590912-4-person79" type="given" value="Peter"></interp>
                     <interp inst="t17590912-4-person79" type="gender" value="male"></interp>
