                     <interp inst="def2-1876-18480821" type="gender" value="male"></interp>
                     <interp inst="def2-1876-18480821" type="age" value="17"></interp>
                     <interp inst="def2-1876-18480821" type="surname" value="WILLIAMSON"></interp>
                     <interp inst="def2-1876-18480821" type="given" value="JOHN"></interp>
