            , <rs id="t18060917-146-viclabel800" type="occupation">esq.</rs>
                  <join result="persNameOccupation" targOrder="Y" targets="t18060917-146-victim1190 t18060917-146-viclabel800"></join> 
            
                  <persName id="t18060917-146-victim1191" type="victimName">
