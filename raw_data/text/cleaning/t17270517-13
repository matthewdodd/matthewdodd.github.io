                        <join result="offenceVictim" targOrder="Y" targets="t17270517-13-off69 t17270517-13-victim98"></join>
                     </persName>
                  , (now 
                     <persName id="t17270517-13-victim99" type="victimName">
                        <rs id="t17270517-13-viclabel71" type="occupation">Earl</rs>
                        <join result="persNameOccupation" targOrder="Y" targets="t17270517-13-victim98 t17270517-13-viclabel71"></join>
