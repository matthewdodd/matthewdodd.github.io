                  <persName id="t17700117-7-person121">
                  William
                   Squire
               <interp inst="t17700117-7-person121" type="surname" value="Squire"></interp>
                     <interp inst="t17700117-7-person121" type="given" value="William"></interp>
                     <interp inst="t17700117-7-person121" type="gender" value="male"></interp>
