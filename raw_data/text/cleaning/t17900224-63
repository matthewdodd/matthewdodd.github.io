                     <interp inst="t17900224-63-person592" type="gender" value="male"></interp>
                  </persName>
            , 
               
                  <persName id="t17900224-63-victim594" type="victimName">
                     Thomas 
                     Harris
                  <interp inst="t17900224-63-victim594" type="surname" value="Harris"></interp>
