                        <interp inst="t18190113-45-victim475" type="gender" value="male"></interp>
                     </persName>
                     
                  ; two gowns, value 13 s.; one spencer, value 8 s., one apron, value 6 d., the goods of 
                     
                        <persName id="t18190113-45-victim477" type="victimName">
                           Hannah 
                           Carney
                        <interp inst="t18190113-45-victim477" type="surname" value="Carney"></interp>
