                     <interp inst="t18260622-161-off802" type="offenceCategory" value="theft"></interp>
                     <interp inst="t18260622-161-off802" type="offenceSubcategory" value="grandLarceny"></interp>
               stealing, on the <rs id="t18260622-161-cd803" type="crimeDate">20th of June</rs>
                     <join result="offenceCrimeDate" targOrder="Y" targets="t18260622-161-off802 t18260622-161-cd803"></join>, 1 pair of boots, value 3s.
            </rs>, the goods of 
               <persName id="t18260622-161-victim923" type="victimName">
                  John 
                  Howard
               <interp inst="t18260622-161-victim923" type="surname" value="Howard"></interp>
                     <interp inst="t18260622-161-victim923" type="given" value="John"></interp>
                     <interp inst="t18260622-161-victim923" type="gender" value="male"></interp>
                     <join result="offenceVictim" targOrder="Y" targets="t18260622-161-off802 t18260622-161-victim923"></join>
