                  <persName id="t18261207-148-person877">
                  HENRIETTA 
                  SPARSHOTT
               <interp inst="t18261207-148-person877" type="surname" value="SPARSHOTT"></interp>
                     <interp inst="t18261207-148-person877" type="given" value="HENRIETTA"></interp>
