               <p>176. 
               
                  <persName id="t18100221-28-defend281" type="defendantName">
                     THOMAS 
                     COX
                  <interp inst="t18100221-28-defend281" type="surname" value="COX"></interp>
                     <interp inst="t18100221-28-defend281" type="given" value="THOMAS"></interp>
