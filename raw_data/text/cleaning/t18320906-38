               <p>7164. 
               
                  <persName id="t18320906-38-defend462" type="defendantName">
                     JOHN 
                     WHITING
                  <interp inst="t18320906-38-defend462" type="surname" value="WHITING"></interp>
                     <interp inst="t18320906-38-defend462" type="given" value="JOHN"></interp>
