                     <interp inst="t18280911-189-off969" type="offenceCategory" value="theft"></interp>
                     <interp inst="t18280911-189-off969" type="offenceSubcategory" value="simpleLarceny"></interp>
               stealing, on the <rs id="t18280911-189-cd970" type="crimeDate">31st of July</rs>
                     <join result="offenceCrimeDate" targOrder="Y" targets="t18280911-189-off969 t18280911-189-cd970"></join>, 1 silver pocket communion-service, value 50s.
            </rs>, the goods of 
               <persName id="t18280911-189-victim1102" type="victimName">
                  Mary 
                  Gilbert
               <interp inst="t18280911-189-victim1102" type="surname" value="Gilbert"></interp>
                     <interp inst="t18280911-189-victim1102" type="given" value="Mary"></interp>
                     <interp inst="t18280911-189-victim1102" type="gender" value="female"></interp>
                     <join result="offenceVictim" targOrder="Y" targets="t18280911-189-off969 t18280911-189-victim1102"></join>
