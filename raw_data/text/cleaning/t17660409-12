                  <persName id="t17660409-12-person120">
                  Edward 
                  Thornton 
               <interp inst="t17660409-12-person120" type="surname" value="Thornton"></interp>
                     <interp inst="t17660409-12-person120" type="given" value="Edward"></interp>
