            , the <rs id="t18040111-35-viclabel215" type="occupation">elder</rs>
                  <join result="persNameOccupation" targOrder="Y" targets="t18040111-35-victim319 t18040111-35-viclabel215"></join>, and 
               
                  <persName id="t18040111-35-victim321" type="victimName">
                     William 
                     Burrage
                  <interp inst="t18040111-35-victim321" type="surname" value="Burrage"></interp>
