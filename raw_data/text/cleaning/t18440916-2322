                     <interp inst="def2-2322-18440916" type="given" value="MARY"></interp>
                     <hi rend="largeCaps">MARY HOWE</hi>
                  </persName>,<persName id="def3-2322-18440916" type="defendantName">
                     <interp inst="def3-2322-18440916" type="gender" value="female"></interp>
                     <interp inst="def3-2322-18440916" type="surname" value="HOWE"></interp>
