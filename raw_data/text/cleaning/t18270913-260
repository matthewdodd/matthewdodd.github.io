                     </persName>
                   and 
                     <persName id="t18270913-260-victim2122" type="victimName">Thomas Wood<interp inst="t18270913-260-victim2122" type="gender" value="male"></interp>
                     </persName>
                  , from the person of 
                     <persName id="t18270913-260-victim2123" type="victimName">
                        Charles 
                        Lovegrove
                     <interp inst="t18270913-260-victim2123" type="surname" value="Lovegrove"></interp>
