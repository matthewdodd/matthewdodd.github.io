                  ; and 1 basket, value 1s., the goods of 
                     
                        <persName id="t18310106-154-victim1367" type="victimName">
                           William 
                           Ashbolt
                        <interp inst="t18310106-154-victim1367" type="surname" value="Ashbolt"></interp>
