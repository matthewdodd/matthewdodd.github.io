                  </persName>
            , 
               <persName id="t17710703-61-person725">
                  James
                   Doddrington
               <interp inst="t17710703-61-person725" type="surname" value="Doddrington"></interp>
                     <interp inst="t17710703-61-person725" type="given" value="James"></interp>
