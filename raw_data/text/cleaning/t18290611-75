            ; 
               
                  <persName id="t18290611-75-defend598" type="defendantName">
                     CHARLES 
                     COOKE
                  <interp inst="t18290611-75-defend598" type="surname" value="COOKE"></interp>
                     <interp inst="t18290611-75-defend598" type="given" value="CHARLES"></interp>
