               <p>849. 
               <persName id="t18111030-46-defend426" type="defendantName">
                  EMANUEL 
                  LEVY
               <interp inst="t18111030-46-defend426" type="surname" value="LEVY"></interp>
                     <interp inst="t18111030-46-defend426" type="given" value="EMANUEL"></interp>
