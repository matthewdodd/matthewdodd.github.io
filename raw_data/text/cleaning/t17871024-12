                  <persName id="t17871024-12-person154">
                  WILLIAM
                   WALLER
               <interp inst="t17871024-12-person154" type="surname" value="WALLER"></interp>
                     <interp inst="t17871024-12-person154" type="given" value="WILLIAM"></interp>
                     <interp inst="t17871024-12-person154" type="gender" value="male"></interp>
