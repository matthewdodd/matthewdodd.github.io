               <p>
                  <xptr type="pageFacsimile" doc="180301120003"></xptr>101. 
               
                  <persName id="t18030112-1-defend48" type="defendantName">
                     WILLIAM 
                     BARNSTAPLE
                  <interp inst="t18030112-1-defend48" type="surname" value="BARNSTAPLE"></interp>
                     <interp inst="t18030112-1-defend48" type="given" value="WILLIAM"></interp>
