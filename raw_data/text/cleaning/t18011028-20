             and 
               <persName id="t18011028-20-defend237" type="defendantName">
                  ABRAHAM-JOSEPH 
                  HAMMERTON
               <interp inst="t18011028-20-defend237" type="surname" value="HAMMERTON"></interp>
                     <interp inst="t18011028-20-defend237" type="given" value="ABRAHAM-JOSEPH"></interp>
                     <interp inst="t18011028-20-defend237" type="gender" value="male"></interp>
                     <interp inst="t18011028-20-defend237" type="age" value="28"></interp>
                  </persName>
             were indicted for <rs id="t18011028-20-off108" type="offenceDescription">
                     <interp inst="t18011028-20-off108" type="offenceCategory" value="theft"></interp>
                     <interp inst="t18011028-20-off108" type="offenceSubcategory" value="grandLarceny"></interp>
               feloniously stealing, on the <rs id="t18011028-20-cd109" type="crimeDate">12th of October</rs>
                     <join result="offenceCrimeDate" targOrder="Y" targets="t18011028-20-off108 t18011028-20-cd109"></join>, a hempen sack, value 1s. 6d. and four bushels of barley, value 22s.
            </rs> the property of 
               <persName id="t18011028-20-victim238" type="victimName">
                  John 
                  Cook
               <interp inst="t18011028-20-victim238" type="surname" value="Cook"></interp>
