                        <join result="offenceCrimeDate" targOrder="Y" targets="t18361128-87-offence-1 t18361128-cd-101"></join>3rd of November</rs>, at <placeName id="t18361128-geo-15">
                        <join result="offencePlace" targOrder="Y" targets="t18361128-87-offence-1 t18361128-geo-15"></join>St. Andrew, Holborn</placeName>, 1 watch, value 5<hi rend="italic">l</hi>.; 2 seals, value 3<hi rend="italic">l</hi>. 10<hi rend="italic">s</hi>.; 1 watch-key, value 10<hi rend="italic">s</hi>.; 1 split-ring, value 10<hi rend="italic">s</hi>.; 1 guard-chain, value 9<hi rend="italic">s</hi>.; and 1 breast-pin, value 1<hi rend="italic">s</hi>.; the goods of <persName id="t18361128-name-720" type="victimName">
                        <interp inst="t18361128-name-720" type="gender" value="male"></interp>
