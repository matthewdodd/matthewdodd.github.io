               <persName id="t18210110-74-person775">
                  James 
                  Smith
               <interp inst="t18210110-74-person775" type="surname" value="Smith"></interp>
                     <interp inst="t18210110-74-person775" type="given" value="James"></interp>
                     <interp inst="t18210110-74-person775" type="gender" value="male"></interp>
