                     <interp inst="t18440408-1254-offence-1" type="offenceSubcategory" value="theftFromPlace"></interp> stealing 110 rings, value 60<hi rend="italic">l</hi>.; 73 brooches, 38<hi rend="italic">l</hi>.; 16 lockets, 10<hi rend="italic">l</hi>.; 44 swivels, 8<hi rend="italic">l</hi>. 10<hi rend="italic">s</hi>.; 180 tubes, 9<hi rend="italic">l</hi>.; 38 ring-tips, 4<hi rend="italic">l</hi>.; 42 brooches, 4<hi rend="italic">l</hi>.; 13 seals, 5<hi rend="italic">l</hi>.; 3 chains, 2<hi rend="italic">l</hi>.; 26 watch<lb></lb>hooks, 4<hi rend="italic">l</hi>.; 1145 watch-keys, 43<hi rend="italic">l</hi>.; 4 jewel-boxes, 3<hi rend="italic">l</hi>. 5<hi rend="italic">s</hi>.; 4 watch-guard chains, 5<hi rend="italic">l</hi>.; and 2 bags, 1<hi rend="italic">s</hi>.; the goods of <persName id="t18440408-name-1233" type="victimName">
                        <interp inst="t18440408-name-1233" type="gender" value="male"></interp>
                        <interp inst="t18440408-name-1233" type="surname" value="SMITH"></interp>
