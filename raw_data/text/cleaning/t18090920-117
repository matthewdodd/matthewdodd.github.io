                  <persName id="t18090920-117-person1192">
                  JOHN 
                  IVORY
               <interp inst="t18090920-117-person1192" type="surname" value="IVORY"></interp>
                     <interp inst="t18090920-117-person1192" type="given" value="JOHN"></interp>
