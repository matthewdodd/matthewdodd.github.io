                        <join result="offenceCrimeDate" targOrder="Y" targets="t18350406-902a-offence-1 t18350406-cd-28"></join>17th of March</rs>, 1 apron, value 4<hi rend="italic">d</hi>.; 2 sheets, value 10<hi rend="italic">s</hi>.; 1 counterpane, value 13<hi rend="italic">s</hi>.; 3 towels, value 1<hi rend="italic">s</hi>. 6<hi rend="italic">d</hi>.; 2 dusters, value 6<hi rend="italic">d</hi>.; 3 shirts, value 18<hi rend="italic">s</hi>.; the goods coat, value 6<hi rend="italic">s</hi>.; 1 flannel shirt, value 1<hi rend="italic">s</hi>.; 1 basket, value 1<hi rend="italic">s</hi>.; the goods of <persName id="t18350406-name-256" type="victimName">
                        <interp inst="t18350406-name-256" type="gender" value="female"></interp>
