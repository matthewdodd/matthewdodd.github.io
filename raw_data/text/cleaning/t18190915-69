               <p>1151. 
               
                  <persName id="t18190915-69-defend610" type="defendantName">
                     MARY ANN 
                     BUTCHER
                  <interp inst="t18190915-69-defend610" type="surname" value="BUTCHER"></interp>
                     <interp inst="t18190915-69-defend610" type="given" value="MARY ANN"></interp>
                     <interp inst="t18190915-69-defend610" type="gender" value="female"></interp>
                     <interp inst="t18190915-69-defend610" type="age" value="28"></interp>
                  </persName>
               
             was indicted for <rs id="t18190915-69-off359" type="offenceDescription">
                     <interp inst="t18190915-69-off359" type="offenceCategory" value="theft"></interp>
                     <interp inst="t18190915-69-off359" type="offenceSubcategory" value="grandLarceny"></interp>
               stealing, on the <rs id="t18190915-69-cd360" type="crimeDate">3d of June</rs>
                     <join result="offenceCrimeDate" targOrder="Y" targets="t18190915-69-off359 t18190915-69-cd360"></join>, one gallon of wine, value 10 s.
            </rs>, the property of 
               <persName id="t18190915-69-person611">
                  Robert 
                  Seppings
               <interp inst="t18190915-69-person611" type="surname" value="Seppings"></interp>
                     <interp inst="t18190915-69-person611" type="given" value="Robert"></interp>
                     <interp inst="t18190915-69-person611" type="gender" value="male"></interp>
                  </persName>
            , <rs id="t18190915-69-viclabel361" type="occupation">Esq.</rs>
                  <join result="persNameOccupation" targOrder="Y" targets="t18190915-69-victim613 t18190915-69-viclabel361"></join> now <rs id="t18190915-69-viclabel362" type="occupation">Sir</rs>
                  <join result="persNameOccupation" targOrder="Y" targets="t18190915-69-victim613 t18190915-69-viclabel362"></join> 
            
               
                  <persName id="t18190915-69-victim613" type="victimName">
                     Robert 
                     Seppings
                  <interp inst="t18190915-69-victim613" type="surname" value="Seppings"></interp>
