                  <persName id="t17611021-9-person57">
                  Henry 
                  Clinton 
               <interp inst="t17611021-9-person57" type="surname" value="Clinton"></interp>
                     <interp inst="t17611021-9-person57" type="given" value="Henry"></interp>
                     <interp inst="t17611021-9-person57" type="gender" value="male"></interp>
