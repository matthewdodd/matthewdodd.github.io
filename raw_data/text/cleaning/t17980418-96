               <p>331. 
               
                  <persName id="t17980418-96-defend803" type="defendantName">
                     JANE 
                     PATTERSON
                  <interp inst="t17980418-96-defend803" type="surname" value="PATTERSON"></interp>
                     <interp inst="t17980418-96-defend803" type="given" value="JANE"></interp>
