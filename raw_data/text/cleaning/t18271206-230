               <p>231. 
               <persName id="t18271206-230-defend1250" type="defendantName">
                  JOHN 
                  SMITH
               <interp inst="t18271206-230-defend1250" type="surname" value="SMITH"></interp>
                     <interp inst="t18271206-230-defend1250" type="given" value="JOHN"></interp>
