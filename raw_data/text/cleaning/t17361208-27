                  <interp inst="t17361208-27-crimeloc160" type="placeName" value="Brook-street, Grosvenor-Square"></interp>
                  <interp inst="t17361208-27-crimeloc160" type="type" value="crimeLocation"></interp>
                  <join result="offencePlace" targOrder="Y" targets="t17361208-27-off152 t17361208-27-crimeloc160"></join>
