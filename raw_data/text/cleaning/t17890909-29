               <p>602. 
               
                  <persName id="t17890909-29-defend270" type="defendantName">
                     ROBERT 
                     DANIELS
                  <interp inst="t17890909-29-defend270" type="surname" value="DANIELS"></interp>
                     <interp inst="t17890909-29-defend270" type="given" value="ROBERT"></interp>
                     <interp inst="t17890909-29-defend270" type="gender" value="male"></interp>
