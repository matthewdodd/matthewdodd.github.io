                     <interp inst="t18480612-1600-offence-1" type="offenceCategory" value="theft"></interp>
                     <interp inst="t18480612-1600-offence-1" type="offenceSubcategory" value="pocketpicking"></interp>, stealing 1 watch, value 10<hi rend="italic">l</hi>.,—the goods of <persName id="t18480612-name-813" type="victimName">
                        <interp inst="t18480612-name-813" type="gender" value="male"></interp>
                        <interp inst="t18480612-name-813" type="surname" value="MERRITT"></interp>
                        <interp inst="t18480612-name-813" type="given" value="WILLIAM EASTGATE"></interp>
                        <join result="offenceVictim" targOrder="Y" targets="t18480612-1600-offence-1 t18480612-name-813"></join>William Eastgate Merritt</persName>, from his person.</rs>
