               <p>1242. 
               <persName id="t18320517-117-defend1018" type="defendantName">
                  JAMES 
                  CONNOR
               <interp inst="t18320517-117-defend1018" type="surname" value="CONNOR"></interp>
                     <interp inst="t18320517-117-defend1018" type="given" value="JAMES"></interp>
