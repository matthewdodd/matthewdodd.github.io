            , and 
               <persName id="t18091101-104-victim829" type="victimName">
                  John 
                  Drinkald, <rs id="t18091101-104-viclabel649" type="occupation">junior</rs>
                     <join result="persNameOccupation" targOrder="Y" targets="t18091101-104-victim829 t18091101-104-viclabel649"></join>
                     <interp inst="t18091101-104-victim829" type="surname" value="Drinkald"></interp>
