               <p>
            
                  <persName id="t18210606-51-person558">
                  BENJAMIN 
                  ALLEN STONE
               <interp inst="t18210606-51-person558" type="surname" value="ALLEN STONE"></interp>
                     <interp inst="t18210606-51-person558" type="given" value="BENJAMIN"></interp>
                     <interp inst="t18210606-51-person558" type="gender" value="male"></interp>
