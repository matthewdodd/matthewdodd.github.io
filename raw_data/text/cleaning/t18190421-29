                        <interp inst="t18190421-29-victim341" type="gender" value="male"></interp>
                     </persName>
                     
                   and 
                     
                        <persName id="t18190421-29-victim343" type="victimName">
                           Mary 
                           Davis
                        <interp inst="t18190421-29-victim343" type="surname" value="Davis"></interp>
                        <interp inst="t18190421-29-victim343" type="given" value="Mary"></interp>
                        <interp inst="t18190421-29-victim343" type="gender" value="female"></interp>
                     </persName>
                     
                  ; one basket, value 1 s., the goods of the said 
                     <persName id="t18190421-29-person344">
                        Benjamin 
                        Hanson
                     <interp inst="t18190421-29-person344" type="surname" value="Hanson"></interp>
