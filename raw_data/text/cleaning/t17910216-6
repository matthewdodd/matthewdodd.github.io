               <p>
            
                  <persName id="t17910216-6-person100">
                  ELIZABETH
                   BAILEY
               <interp inst="t17910216-6-person100" type="surname" value="BAILEY"></interp>
                     <interp inst="t17910216-6-person100" type="given" value="ELIZABETH"></interp>
