             and 
               
                  <persName id="t18320105-133-defend1256" type="defendantName">
                     JAMES 
                     WOOD
                  <interp inst="t18320105-133-defend1256" type="surname" value="WOOD"></interp>
                     <interp inst="t18320105-133-defend1256" type="given" value="JAMES"></interp>
                     <interp inst="t18320105-133-defend1256" type="gender" value="male"></interp>
