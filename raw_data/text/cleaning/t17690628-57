            , <rs id="t17690628-57-viclabel316" type="occupation">the younger</rs>
                  <join result="persNameOccupation" targOrder="Y" targets="t17690628-57-victim552 t17690628-57-viclabel316"></join>, and 
               <persName id="t17690628-57-person553">
                  John 
                  More 
               <interp inst="t17690628-57-person553" type="surname" value="More"></interp>
                     <interp inst="t17690628-57-person553" type="given" value="John"></interp>
                     <interp inst="t17690628-57-person553" type="gender" value="male"></interp>
                  </persName>
