               <p>68. 
               
                  <persName id="t18151206-69-defend681" type="defendantName">
                     ELIZABETH 
                     JOHNSON
                  <interp inst="t18151206-69-defend681" type="surname" value="JOHNSON"></interp>
                     <interp inst="t18151206-69-defend681" type="given" value="ELIZABETH"></interp>
