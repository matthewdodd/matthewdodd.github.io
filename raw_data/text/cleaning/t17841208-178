                     <interp inst="t17841208-178-person1498" type="gender" value="male"></interp>
                  </persName>
            , 
               <persName id="t17841208-178-person1499">
                  Charles
                   Younge
               <interp inst="t17841208-178-person1499" type="surname" value="Younge"></interp>
