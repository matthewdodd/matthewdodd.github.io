               <p>1277. 
               
                  <persName id="t18211024-6-defend97" type="defendantName">
                     CATHERINE 
                     JONES
                  <interp inst="t18211024-6-defend97" type="surname" value="JONES"></interp>
                     <interp inst="t18211024-6-defend97" type="given" value="CATHERINE"></interp>
