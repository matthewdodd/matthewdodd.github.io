                     <interp inst="t18280110-94-off473" type="offenceCategory" value="theft"></interp>
                     <interp inst="t18280110-94-off473" type="offenceSubcategory" value="simpleLarceny"></interp>
               stealing, on the <rs id="t18280110-94-cd474" type="crimeDate">29th of November</rs>
                     <join result="offenceCrimeDate" targOrder="Y" targets="t18280110-94-off473 t18280110-94-cd474"></join>, 5 stockings, value 1s. 3d.
            </rs>, the goods of 
               <persName id="t18280110-94-victim653" type="victimName">
                  Ursula 
                  Grady
               <interp inst="t18280110-94-victim653" type="surname" value="Grady"></interp>
                     <interp inst="t18280110-94-victim653" type="given" value="Ursula"></interp>
                     <interp inst="t18280110-94-victim653" type="gender" value="female"></interp>
                     <join result="offenceVictim" targOrder="Y" targets="t18280110-94-off473 t18280110-94-victim653"></join>
