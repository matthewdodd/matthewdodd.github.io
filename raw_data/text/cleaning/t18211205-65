            , and 
               
                  <persName id="t18211205-65-defend820" type="defendantName">
                     JOHN 
                     HITCHINGS
                  <interp inst="t18211205-65-defend820" type="surname" value="HITCHINGS"></interp>
                     <interp inst="t18211205-65-defend820" type="given" value="JOHN"></interp>
