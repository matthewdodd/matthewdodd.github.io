            , and 
               
                  <persName id="t17860426-102-defend1059" type="defendantName">
                     ANN 
                     DUTTON
                  <interp inst="t17860426-102-defend1059" type="surname" value="DUTTON"></interp>
                     <interp inst="t17860426-102-defend1059" type="given" value="ANN"></interp>
                     <interp inst="t17860426-102-defend1059" type="gender" value="female"></interp>
