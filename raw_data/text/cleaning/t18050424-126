             and 
               
                  <persName id="t18050424-126-defend1178" type="defendantName">
                     JANE 
                     TOWERS
                  <interp inst="t18050424-126-defend1178" type="surname" value="TOWERS"></interp>
                     <interp inst="t18050424-126-defend1178" type="given" value="JANE"></interp>
                     <interp inst="t18050424-126-defend1178" type="gender" value="female"></interp>
