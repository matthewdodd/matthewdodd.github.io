                  <persName id="t17880227-72-person746">
                  ELIZABETH
                   GREEN
               <interp inst="t17880227-72-person746" type="surname" value="GREEN"></interp>
                     <interp inst="t17880227-72-person746" type="given" value="ELIZABETH"></interp>
                     <interp inst="t17880227-72-person746" type="gender" value="female"></interp>
