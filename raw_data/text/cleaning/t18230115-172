                  <persName id="t18230115-172-victim1432" type="victimName">
                     JOHN 
                     KEMP
                  <interp inst="t18230115-172-victim1432" type="surname" value="KEMP"></interp>
                     <interp inst="t18230115-172-victim1432" type="given" value="JOHN"></interp>
