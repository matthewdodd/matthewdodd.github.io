             and 
               <persName id="t17650710-43-defend412" type="defendantName">
                  
                     Ann 
                     Horsley 
                  , otherwise called Ann, wife of 
                     Thomas 
                     <rs id="t17650710-43-alias-4" type="alias">
                        <join result="nameAlias" targOrder="Y" targets="t17650710-43-defend412 t17650710-43-alias-4"></join>Brown</rs>
                  
