                  <persName id="t17730908-92-victim895" type="victimName">
                     William
                      Sangrove
                  <interp inst="t17730908-92-victim895" type="surname" value="Sangrove"></interp>
                     <interp inst="t17730908-92-victim895" type="given" value="William"></interp>
