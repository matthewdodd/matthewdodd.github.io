                        <join result="offenceCrimeDate" targOrder="Y" targets="t18390708-2091-offence-1 t18390708-cd-64"></join>28th of June</rs>, 6 shifts, value 2<hi rend="italic">s</hi>.; 2 shirts, value 1<hi rend="italic">s</hi>. 6<hi rend="italic">d</hi>.; 2 pairs of trowsers, value 1<hi rend="italic">s</hi>.; 1 table-cloth, value 6<hi rend="italic">d</hi>.; 1 night-gown, value 6<hi rend="italic">d</hi>.; 1 petticoat, value 3<hi rend="italic">d</hi>.; 1 pinafore, value 8<hi rend="italic">d</hi>.; 1 apron, value 3<hi rend="italic">d</hi>.; 1 pair of socks, value 3<hi rend="italic">d</hi>.; and 1 handkerchief, value 1<hi rend="italic">d</hi>.; the goods of <persName id="t18390708-name-467" type="victimName">
                        <interp inst="t18390708-name-467" type="gender" value="male"></interp>
                        <interp inst="t18390708-name-467" type="surname" value="IDLE"></interp>
