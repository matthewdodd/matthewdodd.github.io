                  <persName id="t18190421-174-defend1680" type="defendantName">
                     GEORGE 
                     COLLIER
                  <interp inst="t18190421-174-defend1680" type="surname" value="COLLIER"></interp>
                     <interp inst="t18190421-174-defend1680" type="given" value="GEORGE"></interp>
