                        <join result="offenceCrimeDate" targOrder="Y" targets="t18390408-1182-offence-1 t18390408-cd-33"></join>2nd of April</rs>, 1 shift, value 3<hi rend="italic">s</hi>.; 6 pictures framed and glazed, value 2<hi rend="italic">s</hi>.; 2 shirts, value 8<hi rend="italic">s</hi>.; 1 counterpane, value 8<hi rend="italic">s</hi>.; 8 petti-coats, value 5<hi rend="italic">s</hi>. 6<hi rend="italic">d</hi>.; 1 pair of stockings, value 2<hi rend="italic">s</hi>.; 2 pillow-cases, value 2<hi rend="italic">s</hi>.; 1 bolster-case, value 2<hi rend="italic">s</hi>.; and 1 nightcap, value 1<hi rend="italic">s</hi>. 6<hi rend="italic">d</hi>.; the goods of <persName id="t18390408-name-249" type="victimName">
                        <interp inst="t18390408-name-249" type="gender" value="male"></interp>
                        <interp inst="t18390408-name-249" type="surname" value="ENSOR"></interp>
