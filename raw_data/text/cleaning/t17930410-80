                  </persName>
            , 
               <persName id="t17930410-80-person756">
                  James
                   Curtis
               <interp inst="t17930410-80-person756" type="surname" value="Curtis"></interp>
                     <interp inst="t17930410-80-person756" type="given" value="James"></interp>
