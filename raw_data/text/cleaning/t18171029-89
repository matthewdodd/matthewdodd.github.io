            ; and 
               <persName id="t18171029-89-defend558" type="defendantName">
                  SARAH (the <rs id="t18171029-89-deflabel473" type="occupation">wife</rs>
                     <join result="persNameOccupation" targOrder="Y" targets="t18171029-89-defend558 t18171029-89-deflabel473"></join> of William) TROUP
               <interp inst="t18171029-89-defend558" type="surname" value="TROUP"></interp>
                     <interp inst="t18171029-89-defend558" type="given" value="SARAH"></interp>
