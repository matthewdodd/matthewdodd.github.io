               <p>
                  <hi rend="italic">
                     <rs id="t18430612-1937-punishment-200" type="punishmentDescription">
                        <interp inst="t18430612-1937-punishment-200" type="punishmentCategory" value="transport"></interp>
                        <join result="defendantPunishment" targOrder="Y" targets="def1-1937-18430612 t18430612-1937-punishment-200"></join>
