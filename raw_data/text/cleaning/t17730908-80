                  <persName id="t17730908-80-person740">
                  Elizabeth
                   Gower
               <interp inst="t17730908-80-person740" type="surname" value="Gower"></interp>
                     <interp inst="t17730908-80-person740" type="given" value="Elizabeth"></interp>
