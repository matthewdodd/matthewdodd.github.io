                     <interp inst="t18351026-2137-offence-1" type="offenceCategory" value="theft"></interp>
                     <interp inst="t18351026-2137-offence-1" type="offenceSubcategory" value="simpleLarceny"></interp> for stealing, on the <rs id="t18351026-cd-18" type="crimeDate">
                        <join result="offenceCrimeDate" targOrder="Y" targets="t18351026-2137-offence-1 t18351026-cd-18"></join>22nd of September</rs>, 2 caps, value 1<hi rend="italic">s</hi>.; 1 shift, value 1<hi rend="italic">s</hi>.; 1 shift, value 8<hi rend="italic">s</hi>.; 1 pair of bellows, value 1<hi rend="italic">s</hi>. 6<hi rend="italic">d</hi>.; 3 plates, value 4<hi rend="italic">d</hi>.; and 3 spoons, value 4<hi rend="italic">d</hi>.; the goods of <persName id="t18351026-name-175" type="victimName">
                        <interp inst="t18351026-name-175" type="gender" value="male"></interp>
                        <interp inst="t18351026-name-175" type="surname" value="SCHOLEY"></interp>
