                     <hi rend="largeCaps">HANNAH M'CARTHY</hi>
                  </persName>
                  <rs id="t18480131-668-offence-1" type="offenceDescription">
                     <interp inst="t18480131-668-offence-1" type="offenceCategory" value="theft"></interp>
                     <interp inst="t18480131-668-offence-1" type="offenceSubcategory" value="simpleLarceny"></interp>, stealing 1 handkerchief, value 3<hi rend="italic">s</hi>.; 1 jacket and 1 waistcoat, 15<hi rend="italic">s</hi>.; the goods of <persName id="t18480131-name-676" type="victimName">
                        <interp inst="t18480131-name-676" type="gender" value="male"></interp>
                        <interp inst="t18480131-name-676" type="surname" value="JOHNSON"></interp>
