               <persName id="t17550116-5-person82">
                  Thomas 
                  Gayler 
               <interp inst="t17550116-5-person82" type="surname" value="Gayler"></interp>
                     <interp inst="t17550116-5-person82" type="given" value="Thomas"></interp>
