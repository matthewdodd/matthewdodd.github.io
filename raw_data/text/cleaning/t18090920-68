               <p>
            
                  <persName id="t18090920-68-person665">
                  HENRY 
                  MOORE
               <interp inst="t18090920-68-person665" type="surname" value="MOORE"></interp>
                     <interp inst="t18090920-68-person665" type="given" value="HENRY"></interp>
