             and 
               
                  <persName id="t18240114-177-defend1534" type="defendantName">
                     MARGARET 
                     BRYAN
                  <interp inst="t18240114-177-defend1534" type="surname" value="BRYAN"></interp>
                     <interp inst="t18240114-177-defend1534" type="given" value="MARGARET"></interp>
