             and 
               <persName id="t17750426-14-defend169" type="defendantName">
                  MARY, the <rs id="t17750426-14-deflabel66" type="occupation">wife</rs>
                     <join result="persNameOccupation" targOrder="Y" targets="t17750426-14-defend169 t17750426-14-deflabel66"></join> of 
