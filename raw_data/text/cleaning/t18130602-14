             and 
               
                  <persName id="t18130602-14-defend171" type="defendantName">
                     JOHN 
                     BRIANT
                  <interp inst="t18130602-14-defend171" type="surname" value="BRIANT"></interp>
                     <interp inst="t18130602-14-defend171" type="given" value="JOHN"></interp>
