                  <persName id="t18190217-167-person1415">
                  GEORGE 
                  WILSON
               <interp inst="t18190217-167-person1415" type="surname" value="WILSON"></interp>
                     <interp inst="t18190217-167-person1415" type="given" value="GEORGE"></interp>
                     <interp inst="t18190217-167-person1415" type="gender" value="male"></interp>
