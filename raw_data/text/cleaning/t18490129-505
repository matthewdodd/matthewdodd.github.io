               <p>
                  <hi rend="largeCaps">TRACEY</hi> pleaded <hi rend="smallCaps">
                     <rs id="t18490129-505-verdict-1" type="verdictDescription">
                        <interp inst="t18490129-505-verdict-1" type="verdictCategory" value="guilty"></interp>
