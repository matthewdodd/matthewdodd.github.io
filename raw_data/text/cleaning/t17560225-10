            , <rs id="t17560225-10-deflabel49" type="occupation">widow</rs>
                  <join result="persNameOccupation" targOrder="Y" targets="t17560225-10-defend134 t17560225-10-deflabel49"></join>, was indicted for <rs id="t17560225-10-off50" type="offenceDescription">
