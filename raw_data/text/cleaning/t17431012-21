               <persName id="t17431012-21-person221">
                  Mary 
                  Wilson 
               <interp inst="t17431012-21-person221" type="surname" value="Wilson"></interp>
                     <interp inst="t17431012-21-person221" type="given" value="Mary"></interp>
                     <interp inst="t17431012-21-person221" type="gender" value="female"></interp>
