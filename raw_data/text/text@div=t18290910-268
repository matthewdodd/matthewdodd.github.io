                  <xptr type="pageFacsimile" doc="182909100136"></xptr>
            
                  <persName id="t18290910-268-person2512">
                  STEPHEN 
                  BARTLETT
               <interp inst="t18290910-268-person2512" type="surname" value="BARTLETT"></interp>
