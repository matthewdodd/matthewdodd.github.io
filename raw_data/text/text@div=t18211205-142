             was indicted for <rs id="t18211205-142-off875" type="offenceDescription">
                     <interp inst="t18211205-142-off875" type="offenceCategory" value="theft"></interp>
                     <interp inst="t18211205-142-off875" type="offenceSubcategory" value="grandLarceny"></interp>
               stealing on the <rs id="t18211205-142-cd876" type="crimeDate">10th of November</rs>
                     <join result="offenceCrimeDate" targOrder="Y" targets="t18211205-142-off875 t18211205-142-cd876"></join>
                     <join result="offenceCrimeDate" targOrder="Y" targets="t18211205-142-off877 t18211205-142-cd876"></join>
                     <join result="offenceCrimeDate" targOrder="Y" targets="t18211205-142-off881 t18211205-142-cd876"></join>, five pieces of timber, value 10 l.
            </rs> the goods of 
               
                  <persName id="t18211205-142-victim1499" type="victimName">
                     Thomas 
                     Brockelbank
                  <interp inst="t18211205-142-victim1499" type="surname" value="Brockelbank"></interp>
                     <interp inst="t18211205-142-victim1499" type="given" value="Thomas"></interp>
                     <interp inst="t18211205-142-victim1499" type="gender" value="male"></interp>
                  </persName>
               
            : and 
               
                  <persName id="t18211205-142-defend1501" type="defendantName">
                     GEORGE 
                     DUNKLEY
                  <interp inst="t18211205-142-defend1501" type="surname" value="DUNKLEY"></interp>
