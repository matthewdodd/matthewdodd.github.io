            , and 
               
                  <persName id="t18170219-145-defend1088" type="defendantName">
                     GEORGE 
                     BIRD
                  <interp inst="t18170219-145-defend1088" type="surname" value="BIRD"></interp>
                     <interp inst="t18170219-145-defend1088" type="given" value="GEORGE"></interp>
