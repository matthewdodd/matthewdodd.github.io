                  <persName id="t18220703-134-person1144">
                  JAMES 
                  WILKINSON
               <interp inst="t18220703-134-person1144" type="surname" value="WILKINSON"></interp>
                     <interp inst="t18220703-134-person1144" type="given" value="JAMES"></interp>
                     <interp inst="t18220703-134-person1144" type="gender" value="male"></interp>
