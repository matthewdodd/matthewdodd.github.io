                     <interp inst="t18260216-171-off906" type="offenceCategory" value="theft"></interp>
                     <interp inst="t18260216-171-off906" type="offenceSubcategory" value="grandLarceny"></interp>
               stealing, on the <rs id="t18260216-171-cd907" type="crimeDate">30th of January</rs>
                     <join result="offenceCrimeDate" targOrder="Y" targets="t18260216-171-off906 t18260216-171-cd907"></join>, 1 ham, value 15s.
            </rs>, the goods of 
               <persName id="t18260216-171-victim1065" type="victimName">
                  John 
                  Eustace
               <interp inst="t18260216-171-victim1065" type="surname" value="Eustace"></interp>
