                     </persName>
                  
               
                  </rs>; and 
               
                  <persName id="t17660903-28-defend241" type="defendantName">
                     Elizabeth 
                     Norman 
                  <interp inst="t17660903-28-defend241" type="surname" value="Norman"></interp>
