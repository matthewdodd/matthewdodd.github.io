                     <interp inst="t17840421-21-person413" type="gender" value="female"></interp>
                  </persName>
            , <rs id="t17840421-21-verdict118" type="verdictDescription">
                     <interp inst="t17840421-21-verdict118" type="verdictCategory" value="guilty"></interp>
               GUILTY
            </rs> 
