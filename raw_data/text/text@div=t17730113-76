               <p>263. (L.) 
               
                  <persName id="t17730113-76-defend728" type="defendantName">
                     JAMES
                      WATSON
                  <interp inst="t17730113-76-defend728" type="surname" value="WATSON"></interp>
                     <interp inst="t17730113-76-defend728" type="given" value="JAMES"></interp>
                     <interp inst="t17730113-76-defend728" type="gender" value="male"></interp>
                  </persName>
               
             was indicted <rs id="t17730113-76-off416" type="offenceDescription">
                     <interp inst="t17730113-76-off416" type="offenceCategory" value="deception"></interp>
                     <interp inst="t17730113-76-off416" type="offenceSubcategory" value="fraud"></interp>
               together with 
                     <persName id="t17730113-76-person729">
                        William
                         Reddish
                     <interp inst="t17730113-76-person729" type="surname" value="Reddish"></interp>
