                     <interp inst="t17190225-29-off158" type="offenceSubcategory" value="grandLarceny"></interp>
               feloniously stealing certain Tea, Chocolate, &amp;c
            </rs>. from 
               
                  <persName id="t17190225-29-victim187" type="victimName">
                     Samuel 
                     Hawkins 
                  <interp inst="t17190225-29-victim187" type="surname" value="Hawkins"></interp>
                     <interp inst="t17190225-29-victim187" type="given" value="Samuel"></interp>
