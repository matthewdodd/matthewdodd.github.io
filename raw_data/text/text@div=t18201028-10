                  <persName id="t18201028-10-person149">
                  HARRIET 
                  DAVIS
               <interp inst="t18201028-10-person149" type="surname" value="DAVIS"></interp>
                     <interp inst="t18201028-10-person149" type="given" value="HARRIET"></interp>
