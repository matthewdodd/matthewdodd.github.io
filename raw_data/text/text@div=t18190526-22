               <p>
            
                  <persName id="t18190526-22-person251">
                  THOMAS 
                  WEEKLEY
               <interp inst="t18190526-22-person251" type="surname" value="WEEKLEY"></interp>
                     <interp inst="t18190526-22-person251" type="given" value="THOMAS"></interp>
                     <interp inst="t18190526-22-person251" type="gender" value="male"></interp>
