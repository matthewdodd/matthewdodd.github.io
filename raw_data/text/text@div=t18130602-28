                  <persName id="t18130602-28-person276">
                  SAMUEL 
                  JONES
               <interp inst="t18130602-28-person276" type="surname" value="JONES"></interp>
                     <interp inst="t18130602-28-person276" type="given" value="SAMUEL"></interp>
