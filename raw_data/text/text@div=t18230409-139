                        <interp inst="t18230409-139-person1240" type="age" value="24"></interp>
                     </persName>
                   THE <rs id="t18230409-139-deflabel738" type="occupation">WIFE</rs>
                     <join result="persNameOccupation" targOrder="Y" targets="t18230409-139-defend1239 t18230409-139-deflabel738"></join>
                     <join result="persNameOccupation" targOrder="Y" targets="t18230409-139-person1240 t18230409-139-deflabel738"></join> OF BRYAN M'KENNA alias <rs id="t18230409-139-alias-1" type="alias">
                        <join result="nameAlias" targOrder="Y" targets="t18230409-139-defend1239 t18230409-139-alias-1"></join>M'CORMICK</rs>
                     <interp inst="t18230409-139-defend1239" type="surname" value="M'KENNA"></interp>
