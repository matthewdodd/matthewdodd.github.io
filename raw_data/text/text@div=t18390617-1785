               <p>
                  <hi rend="largeCaps">COLLINS</hi> pleaded <hi rend="largeCaps">
                     <rs id="t18390617-1785-verdict-2" type="verdictDescription">
                        <interp inst="t18390617-1785-verdict-2" type="verdictCategory" value="guilty"></interp>
