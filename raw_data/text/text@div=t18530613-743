                     <hi rend="largeCaps">GEORGE THOMPSON</hi>, <hi rend="italic">alias<rs id="t18530613-alias-1" type="alias">
                           <join result="nameAlias" targOrder="Y" targets="def1-743-18530613 t18530613-alias-1"></join> Cole</rs>
                     </hi>
                  </persName>, and <persName id="def2-743-18530613" type="defendantName">
                     <interp inst="def2-743-18530613" type="gender" value="male"></interp>
                     <interp inst="def2-743-18530613" type="age" value="21"></interp>
                     <interp inst="def2-743-18530613" type="surname" value="FITZGERALD"></interp>
