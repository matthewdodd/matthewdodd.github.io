                  <persName id="t18190421-239-person2238">
                  WILLIAM 
                  HAGGETT
               <interp inst="t18190421-239-person2238" type="surname" value="HAGGETT"></interp>
                     <interp inst="t18190421-239-person2238" type="given" value="WILLIAM"></interp>
                     <interp inst="t18190421-239-person2238" type="gender" value="male"></interp>
