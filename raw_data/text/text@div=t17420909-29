                  <persName id="t17420909-29-person294">
                  Frances 
                  Coverley 
               <interp inst="t17420909-29-person294" type="surname" value="Coverley"></interp>
                     <interp inst="t17420909-29-person294" type="given" value="Frances"></interp>
