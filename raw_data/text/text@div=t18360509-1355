                     <interp inst="t18360509-1355-offence-1" type="offenceCategory" value="theft"></interp>
                     <interp inst="t18360509-1355-offence-1" type="offenceSubcategory" value="stealingFromMaster"></interp> for stealing on the <rs id="t18360509-cd-198" type="crimeDate">
                        <join result="offenceCrimeDate" targOrder="Y" targets="t18360509-1355-offence-1 t18360509-cd-198"></join>27th of April</rs>, 2 sheets, value 4<hi rend="italic">s</hi>.; 1 bed-ground, value 1<hi rend="italic">s</hi>. 6<hi rend="italic">d</hi>.; 3 towels, value 1<hi rend="italic">s</hi>.; 1 pillow-case, value 6<hi rend="italic">d</hi>.; 1 cap, value 5<hi rend="italic">s</hi>.; and 3 handkerchiefs, value 2<hi rend="italic">s</hi>., the goods of <persName id="t18360509-name-1357" type="victimName">
                        <interp inst="t18360509-name-1357" type="gender" value="female"></interp>
                        <interp inst="t18360509-name-1357" type="surname" value="WARNER"></interp>
                        <interp inst="t18360509-name-1357" type="given" value="ELIZABETH"></interp>
                        <join result="offenceVictim" targOrder="Y" targets="t18360509-1355-offence-1 t18360509-name-1357"></join>Elizabeth Warner</persName> her mistress; and 1 coat, value 3<hi rend="italic">l</hi>. 10<hi rend="italic">s</hi>.; 1 waistcoat, value 20<hi rend="italic">s</hi>.; and 1 pair of boots, value 20<hi rend="italic">s</hi>., the goods of <persName id="t18360509-name-1358" type="victimName">
                        <interp inst="t18360509-name-1358" type="gender" value="male"></interp>
