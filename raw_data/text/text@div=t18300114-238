            , and 
               
                  <persName id="t18300114-238-defend1609" type="defendantName">
                     JOHN DAVID 
                     OUSLEY
                  <interp inst="t18300114-238-defend1609" type="surname" value="OUSLEY"></interp>
                     <interp inst="t18300114-238-defend1609" type="given" value="JOHN DAVID"></interp>
                     <interp inst="t18300114-238-defend1609" type="gender" value="male"></interp>
                  </persName>
               
            , were indicted for <rs id="t18300114-238-off1270" type="offenceDescription">
                     <interp inst="t18300114-238-off1270" type="offenceCategory" value="theft"></interp>
                     <interp inst="t18300114-238-off1270" type="offenceSubcategory" value="pocketpicking"></interp>
               stealing, on the <rs id="t18300114-238-cd1271" type="crimeDate">26th of December</rs>
                     <join result="offenceCrimeDate" targOrder="Y" targets="t18300114-238-off1270 t18300114-238-cd1271"></join>, 1 watch, value 2l., the goods of 
                     
                        <persName id="t18300114-238-victim1611" type="victimName">
                           Thomas 
                           Lee
                        <interp inst="t18300114-238-victim1611" type="surname" value="Lee"></interp>
