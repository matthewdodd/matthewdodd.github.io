                     <interp inst="t18480403-1121-offence-1" type="offenceCategory" value="theft"></interp>
                     <interp inst="t18480403-1121-offence-1" type="offenceSubcategory" value="simpleLarceny"></interp>, stealing 8 shirts, and other articles, value 5<hi rend="italic">l</hi>. 12<hi rend="italic">s</hi>. 6<hi rend="italic">d</hi>.; the goods of <persName id="t18480403-name-788" type="victimName">
                        <interp inst="t18480403-name-788" type="gender" value="male"></interp>
