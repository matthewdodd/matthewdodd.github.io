             and 
               <persName id="t18320517-251-defend1857" type="defendantName">
                  HENRY 
                  McGEE
               <interp inst="t18320517-251-defend1857" type="surname" value="McGEE"></interp>
                     <interp inst="t18320517-251-defend1857" type="given" value="HENRY"></interp>
                     <interp inst="t18320517-251-defend1857" type="gender" value="male"></interp>
                     <interp inst="t18320517-251-defend1857" type="age" value="18"></interp>
                  </persName>
             were indicted for <rs id="t18320517-251-off1445" type="offenceDescription">
                     <interp inst="t18320517-251-off1445" type="offenceCategory" value="theft"></interp>
                     <interp inst="t18320517-251-off1445" type="offenceSubcategory" value="pocketpicking"></interp>
               stealing, on the <rs id="t18320517-251-cd1446" type="crimeDate">1st of May</rs>
                     <join result="offenceCrimeDate" targOrder="Y" targets="t18320517-251-off1445 t18320517-251-cd1446"></join>, 1 necklace, value 8s., the goods of 
                     
                        <persName id="t18320517-251-victim1859" type="victimName">
                           Charles 
                           Barnes
                        <interp inst="t18320517-251-victim1859" type="surname" value="Barnes"></interp>
