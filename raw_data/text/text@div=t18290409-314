               <p>993. 
               
                  <persName id="t18290409-314-defend2860" type="defendantName">
                     GEORGE FREDERICK 
                     BROWN
                  <interp inst="t18290409-314-defend2860" type="surname" value="BROWN"></interp>
                     <interp inst="t18290409-314-defend2860" type="given" value="GEORGE FREDERICK"></interp>
