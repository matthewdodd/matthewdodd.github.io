               <p>527. 
               
                  <persName id="t18160529-52-defend532" type="defendantName">
                     PHILIP 
                     GLYNN
                  <interp inst="t18160529-52-defend532" type="surname" value="GLYNN"></interp>
                     <interp inst="t18160529-52-defend532" type="given" value="PHILIP"></interp>
