                        <interp inst="def3-1168-18480403" type="surname" value="BARRETT"></interp>
                        <interp inst="def3-1168-18480403" type="given" value="WILLIAM"></interp> WILLIAM BARRETT</persName>,<persName id="def4-1168-18480403" type="defendantName">
                        <interp inst="def4-1168-18480403" type="gender" value="male"></interp>
                        <interp inst="def4-1168-18480403" type="surname" value="PROPHETT"></interp>
                        <interp inst="def4-1168-18480403" type="given" value="BENJAMIN"></interp> BENJAMIN PROPHETT</persName>,<persName id="def5-1168-18480403" type="defendantName">
                        <interp inst="def5-1168-18480403" type="gender" value="male"></interp>
