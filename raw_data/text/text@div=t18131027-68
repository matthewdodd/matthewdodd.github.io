            , and 
               
                  <persName id="t18131027-68-victim734" type="victimName">
                     Thomas Seward 
                     Beachcroft
                  <interp inst="t18131027-68-victim734" type="surname" value="Beachcroft"></interp>
                     <interp inst="t18131027-68-victim734" type="given" value="Thomas Seward"></interp>
                     <interp inst="t18131027-68-victim734" type="gender" value="male"></interp>
