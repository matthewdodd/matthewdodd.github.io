<div1 type="trialAccount" id="t17471014-28">
               <interp inst="t17471014-28" type="collection" value="BAILEY"></interp>
               <interp inst="t17471014-28" type="year" value="1747"></interp>
               <interp inst="t17471014-28" type="uri" value="sessionsPapers/17471014"></interp>
