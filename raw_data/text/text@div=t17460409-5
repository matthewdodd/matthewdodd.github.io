                     <interp inst="t17460409-5-person118" type="gender" value="female"></interp>
                  </persName>
             
                  <rs id="t17460409-5-verdict22" type="verdictDescription">
                     <interp inst="t17460409-5-verdict22" type="verdictCategory" value="notGuilty"></interp>
               Acquitted
            </rs>.</p>
               <p>Darby <rs id="t17460409-5-verdict23" type="verdictDescription">
                     <interp inst="t17460409-5-verdict23" type="verdictCategory" value="guilty"></interp>
               Guilty
            </rs>.</p>
