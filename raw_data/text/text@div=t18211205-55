               <p>55. 
               
                  <persName id="t18211205-55-defend729" type="defendantName">
                     SARAH 
                     SMITH
                  <interp inst="t18211205-55-defend729" type="surname" value="SMITH"></interp>
                     <interp inst="t18211205-55-defend729" type="given" value="SARAH"></interp>
