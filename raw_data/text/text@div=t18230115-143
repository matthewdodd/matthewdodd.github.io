             and 
               
                  <persName id="t18230115-143-defend1233" type="defendantName">
                     DENNIS 
                     DONOVAN
                  <interp inst="t18230115-143-defend1233" type="surname" value="DONOVAN"></interp>
                     <interp inst="t18230115-143-defend1233" type="given" value="DENNIS"></interp>
                     <interp inst="t18230115-143-defend1233" type="gender" value="male"></interp>
                     <interp inst="t18230115-143-defend1233" type="age" value="17"></interp>
                  </persName>
               
             were indicted for <rs id="t18230115-143-off779" type="offenceDescription">
                     <interp inst="t18230115-143-off779" type="offenceCategory" value="theft"></interp>
                     <interp inst="t18230115-143-off779" type="offenceSubcategory" value="pocketpicking"></interp>
               stealing, on the <rs id="t18230115-143-cd780" type="crimeDate">12th of January</rs>
                     <join result="offenceCrimeDate" targOrder="Y" targets="t18230115-143-off779 t18230115-143-cd780"></join>, a handkerchief, value 18 d. the goods of 
                     
                        <persName id="t18230115-143-victim1235" type="victimName">
                           John 
                           Buttfield
                        <interp inst="t18230115-143-victim1235" type="surname" value="Buttfield"></interp>
