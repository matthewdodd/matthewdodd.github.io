                     <interp inst="t18320405-223-off1318" type="offenceCategory" value="theft"></interp>
                     <interp inst="t18320405-223-off1318" type="offenceSubcategory" value="simpleLarceny"></interp>
               stealing, on the <rs id="t18320405-223-cd1319" type="crimeDate">20th of February</rs>
                     <join result="offenceCrimeDate" targOrder="Y" targets="t18320405-223-off1318 t18320405-223-cd1319"></join>, 1 lace collar, value 7s.
            </rs>, the goods of 
               
                  <persName id="t18320405-223-victim1928" type="victimName">
                     Ruth 
                     Bruce
                  <interp inst="t18320405-223-victim1928" type="surname" value="Bruce"></interp>
