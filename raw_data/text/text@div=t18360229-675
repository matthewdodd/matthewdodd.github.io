                     <interp inst="def1-675-18360229" type="given" value="JOHN"></interp>
                     <hi rend="largeCaps">JOHN JONES</hi>
                  </persName> and <persName id="def2-675-18360229" type="defendantName">
                     <interp inst="def2-675-18360229" type="gender" value="male"></interp>
                     <interp inst="def2-675-18360229" type="age" value="18"></interp>
