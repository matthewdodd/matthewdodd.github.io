               <p>214. 
               
                  <persName id="t18220417-90-defend841" type="defendantName">
                     GEORGE 
                     BIGGS
                  <interp inst="t18220417-90-defend841" type="surname" value="BIGGS"></interp>
                     <interp inst="t18220417-90-defend841" type="given" value="GEORGE"></interp>
