             and 
               
                  <persName id="t18220417-90-defend843" type="defendantName">
                     RICHARD 
                     BIGGS
                  <interp inst="t18220417-90-defend843" type="surname" value="BIGGS"></interp>
                     <interp inst="t18220417-90-defend843" type="given" value="RICHARD"></interp>
