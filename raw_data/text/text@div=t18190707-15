               <persName id="t18190707-15-person190">
                  William 
                  Vardon
               <interp inst="t18190707-15-person190" type="surname" value="Vardon"></interp>
                     <interp inst="t18190707-15-person190" type="given" value="William"></interp>
                     <interp inst="t18190707-15-person190" type="gender" value="male"></interp>
