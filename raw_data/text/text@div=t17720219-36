                  </persName>
             and 
               <persName id="t17720219-36-person282">
                  Margaret
                   Michener
               <interp inst="t17720219-36-person282" type="surname" value="Michener"></interp>
                     <interp inst="t17720219-36-person282" type="given" value="Margaret"></interp>
