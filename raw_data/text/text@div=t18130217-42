             and 
               
                  <persName id="t18130217-42-defend390" type="defendantName">
                     HANNAH 
                     SMITH
                  <interp inst="t18130217-42-defend390" type="surname" value="SMITH"></interp>
                     <interp inst="t18130217-42-defend390" type="given" value="HANNAH"></interp>
