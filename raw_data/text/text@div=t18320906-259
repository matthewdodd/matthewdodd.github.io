                  ; and 1 watch, value 1l., the goods of 
                     
                        <persName id="t18320906-259-victim2123" type="victimName">
                           Mary 
                           Donovan
                        <interp inst="t18320906-259-victim2123" type="surname" value="Donovan"></interp>
