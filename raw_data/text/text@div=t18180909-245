               <p>
            
                  <persName id="t18180909-245-person1821">
                  GEORGE 
                  CARTER
               <interp inst="t18180909-245-person1821" type="surname" value="CARTER"></interp>
                     <interp inst="t18180909-245-person1821" type="given" value="GEORGE"></interp>
