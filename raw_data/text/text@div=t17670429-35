                  <persName id="t17670429-35-person304">
                  William 
                  Batcheller 
               <interp inst="t17670429-35-person304" type="surname" value="Batcheller"></interp>
                     <interp inst="t17670429-35-person304" type="given" value="William"></interp>
                     <interp inst="t17670429-35-person304" type="gender" value="male"></interp>
