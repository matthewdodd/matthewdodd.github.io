                  <persName id="t18190421-15-person212">
                  THOMAS 
                  WILLIAM LEATHWICK
               <interp inst="t18190421-15-person212" type="surname" value="WILLIAM LEATHWICK"></interp>
                     <interp inst="t18190421-15-person212" type="given" value="THOMAS"></interp>
                     <interp inst="t18190421-15-person212" type="gender" value="male"></interp>
