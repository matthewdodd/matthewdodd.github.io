               <p>891. 
               <persName id="t18280410-99-defend672" type="defendantName">
                  ELIZEBETH 
                  COOPER
               <interp inst="t18280410-99-defend672" type="surname" value="COOPER"></interp>
                     <interp inst="t18280410-99-defend672" type="given" value="ELIZEBETH"></interp>
