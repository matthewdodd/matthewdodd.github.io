               <join result="criminalCharge" id="t18321129-142-off805-c1021" targOrder="Y" targets="t18321129-142-defend1346 t18321129-142-off805 t18321129-142-verdict812"></join>
         
               <p>143. 
               
                  <persName id="t18321129-142-defend1340" type="defendantName">
                     ELEANOR 
                     GOODRICH
                  <interp inst="t18321129-142-defend1340" type="surname" value="GOODRICH"></interp>
                     <interp inst="t18321129-142-defend1340" type="given" value="ELEANOR"></interp>
