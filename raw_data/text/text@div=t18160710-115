               <p>
            
                  <persName id="t18160710-115-person824">
                  THOMAS 
                  NIGHTINGALE
               <interp inst="t18160710-115-person824" type="surname" value="NIGHTINGALE"></interp>
                     <interp inst="t18160710-115-person824" type="given" value="THOMAS"></interp>
                     <interp inst="t18160710-115-person824" type="gender" value="male"></interp>
