            , and 
               
                  <persName id="t18200918-120-defend1362" type="defendantName">
                     DENNIS 
                     SWEENEY
                  <interp inst="t18200918-120-defend1362" type="surname" value="SWEENEY"></interp>
                     <interp inst="t18200918-120-defend1362" type="given" value="DENNIS"></interp>
