               <p>505. 
               <persName id="t18170219-142-defend1068" type="defendantName">
                  MARY 
                  SMITH
               <interp inst="t18170219-142-defend1068" type="surname" value="SMITH"></interp>
                     <interp inst="t18170219-142-defend1068" type="given" value="MARY"></interp>
