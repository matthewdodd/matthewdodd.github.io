                  <persName id="def2-573-18510203" type="defendantName">
                     <interp inst="def2-573-18510203" type="gender" value="female"></interp>
                     <interp inst="def2-573-18510203" type="age" value="19"></interp>
                     <interp inst="def2-573-18510203" type="surname" value="MURPHY"></interp>
