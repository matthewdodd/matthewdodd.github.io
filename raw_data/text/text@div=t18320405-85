               <persName id="t18320405-85-person854">
                  William 
                  Dyer
               <interp inst="t18320405-85-person854" type="surname" value="Dyer"></interp>
                     <interp inst="t18320405-85-person854" type="given" value="William"></interp>
                     <interp inst="t18320405-85-person854" type="gender" value="male"></interp>
