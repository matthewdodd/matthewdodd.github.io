                  <persName id="t17440728-3-victim54" type="victimName">
                     Thomas 
                     Mynet 
                  <interp inst="t17440728-3-victim54" type="surname" value="Mynet"></interp>
                     <interp inst="t17440728-3-victim54" type="given" value="Thomas"></interp>
