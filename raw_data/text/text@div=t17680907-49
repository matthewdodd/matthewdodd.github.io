                  <persName id="t17680907-49-victim516" type="victimName">
                     John 
                     Glaister 
                  <interp inst="t17680907-49-victim516" type="surname" value="Glaister"></interp>
                     <interp inst="t17680907-49-victim516" type="given" value="John"></interp>
