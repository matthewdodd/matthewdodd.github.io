               <persName id="t18140216-16-person188">
                  Thomas 
                  Brandram
               <interp inst="t18140216-16-person188" type="surname" value="Brandram"></interp>
                     <interp inst="t18140216-16-person188" type="given" value="Thomas"></interp>
