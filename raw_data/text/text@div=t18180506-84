               <interp inst="t18180506-84-person752" type="surname" value="Laycock"></interp>
                     <interp inst="t18180506-84-person752" type="given" value="Richard"></interp>
                     <interp inst="t18180506-84-person752" type="gender" value="male"></interp>
                  </persName>
            , 
               <persName id="t18180506-84-person753">
                  William 
                  White
               <interp inst="t18180506-84-person753" type="surname" value="White"></interp>
                     <interp inst="t18180506-84-person753" type="given" value="William"></interp>
                     <interp inst="t18180506-84-person753" type="gender" value="male"></interp>
                  </persName>
            , and myself, as <rs id="t18180506-84-viclabel470" type="occupation">trustee</rs>
                  <join result="persNameOccupation" targOrder="Y" targets="t18180506-84-victim733 t18180506-84-viclabel470"></join>
                  <join result="persNameOccupation" targOrder="Y" targets="t18180506-84-victim735 t18180506-84-viclabel470"></join>
