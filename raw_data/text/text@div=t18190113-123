                  <persName id="t18190113-123-person1089">
                  WILLIAM 
                  LEE
               <interp inst="t18190113-123-person1089" type="surname" value="LEE"></interp>
                     <interp inst="t18190113-123-person1089" type="given" value="WILLIAM"></interp>
                     <interp inst="t18190113-123-person1089" type="gender" value="male"></interp>
