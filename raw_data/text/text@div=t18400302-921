                        <join result="offenceVictim" targOrder="Y" targets="t18400302-921-offence-1 t18400302-name-738"></join>Thomas Daniel Legg Goozee</persName>: 1 oil-stone, value 3<hi rend="italic">s</hi>.; 1 square, value 1<hi rend="italic">s</hi>. 6<hi rend="italic">d</hi>.; 1 spoke-shave, value 1<hi rend="italic">s</hi>.; 2 gimlets, value 8<hi rend="italic">d</hi>.; 3 bradawls, value 6<hi rend="italic">d</hi>.; 7 chisels, value 5<hi rend="italic">s</hi>.; 1 pair of compasses, value 6<hi rend="italic">d</hi>.; 1 punch, value 4<hi rend="italic">d</hi>.; 1 hammer, value 1<hi rend="italic">s</hi>.; 2 screw-drivers, value 4<hi rend="italic">s</hi>.; and 1 basket, value 2<hi rend="italic">s</hi>. 6<hi rend="italic">d</hi>.; the goods of <persName id="t18400302-name-739" type="victimName">
                        <interp inst="t18400302-name-739" type="gender" value="male"></interp>
