               <p>
            
                  <persName id="t18320216-172-person1438">
                  GEORGE 
                  THURSTON
               <interp inst="t18320216-172-person1438" type="surname" value="THURSTON"></interp>
                     <interp inst="t18320216-172-person1438" type="given" value="GEORGE"></interp>
                     <interp inst="t18320216-172-person1438" type="gender" value="male"></interp>
