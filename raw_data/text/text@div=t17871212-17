                     <interp inst="t17871212-17-person259" type="gender" value="female"></interp>
                  </persName>
            , 
               <persName id="t17871212-17-person260">
                  MARY
                   READING
               <interp inst="t17871212-17-person260" type="surname" value="READING"></interp>
                     <interp inst="t17871212-17-person260" type="given" value="MARY"></interp>
