            , and 
               <persName id="t18000709-94-defend691" type="defendantName">
                  PEARCE 
                  COLLETT
               <interp inst="t18000709-94-defend691" type="surname" value="COLLETT"></interp>
                     <interp inst="t18000709-94-defend691" type="given" value="PEARCE"></interp>
