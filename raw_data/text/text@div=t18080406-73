                     <interp inst="t18080406-73-person698" type="gender" value="male"></interp>
                  </persName>
            . I am <rs id="t18080406-73-viclabel392" type="occupation">clerk</rs>
                  <join result="persNameOccupation" targOrder="Y" targets="t18080406-73-victim697 t18080406-73-viclabel392"></join> to Mr. 
               <persName id="t18080406-73-person699">
                  John 
                  Robins
               <interp inst="t18080406-73-person699" type="surname" value="Robins"></interp>
                     <interp inst="t18080406-73-person699" type="given" value="John"></interp>
                     <interp inst="t18080406-73-person699" type="gender" value="male"></interp>
                  </persName>
            , <rs id="t18080406-73-viclabel393" type="occupation">auctioneer</rs>
                  <join result="persNameOccupation" targOrder="Y" targets="t18080406-73-victim693 t18080406-73-viclabel393"></join>, Covent garden; 
               <persName id="t18080406-73-person700">
                  John 
                  Robins
               <interp inst="t18080406-73-person700" type="surname" value="Robins"></interp>
                     <interp inst="t18080406-73-person700" type="given" value="John"></interp>
                     <interp inst="t18080406-73-person700" type="gender" value="male"></interp>
