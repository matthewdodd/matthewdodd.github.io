                     <interp inst="t18361212-295-offence-1" type="offenceCategory" value="theft"></interp>
                     <interp inst="t18361212-295-offence-1" type="offenceSubcategory" value="simpleLarceny"></interp> stealing, on the <rs id="t18361212-cd-57" type="crimeDate">
                        <join result="offenceCrimeDate" targOrder="Y" targets="t18361212-295-offence-1 t18361212-cd-57"></join>24th of November</rs>, 1 bed, value 34<hi rend="italic">s</hi>.; 1 blanket, value 6<hi rend="italic">s</hi>. 1 sheet, value 3<hi rend="italic">s</hi>.; 3 pillows, value 5<hi rend="italic">s</hi>.; 1 counterpane, value 8<hi rend="italic">s</hi>.; 2 curtains, value 1<hi rend="italic">s</hi>.; 1 carpet, value 1<hi rend="italic">s</hi>.; 1 looking-glass and frame, value 1<hi rend="italic">l</hi>.; 1 tea-pot, value 1<hi rend="italic">s</hi>.; 1 candlestick, value 1<hi rend="italic">s</hi>.; 1 pail, value 1<hi rend="italic">s</hi>.; 1 saucepan. value 3<hi rend="italic">s</hi>.; 1 kettle, value 1<hi rend="italic">s</hi>.; 6<hi rend="italic">d</hi>.; and 1 flat-iron, value 1<hi rend="italic">s</hi>.; the goods of <persName id="t18361212-name-505" type="victimName">
                        <interp inst="t18361212-name-505" type="gender" value="male"></interp>
                        <interp inst="t18361212-name-505" type="surname" value="LAKIE"></interp>
