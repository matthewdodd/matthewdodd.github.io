                        <interp inst="t18470510-name-914" type="given" value="BENJAMIN"></interp>
                        <join result="offenceVictim" targOrder="Y" targets="t18470510-1263-offence-1 t18470510-name-914"></join>Benjamin Hicklin</persName>
                  </rs>: and <persName id="def4-1263-18470510" type="defendantName">
                     <interp inst="def4-1263-18470510" type="gender" value="male"></interp>
