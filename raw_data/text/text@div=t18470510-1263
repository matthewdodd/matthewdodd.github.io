                        <join result="offenceVictim" targOrder="Y" targets="t18470510-1263-offence-1 t18470510-name-913"></join>James William Weaver</persName> and <persName id="t18470510-name-914" type="victimName">
                        <interp inst="t18470510-name-914" type="gender" value="male"></interp>
