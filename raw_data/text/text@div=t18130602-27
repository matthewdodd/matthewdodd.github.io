               <p>
            
                  <persName id="t18130602-27-person270">
                  SAMUEL 
                  JONES
               <interp inst="t18130602-27-person270" type="surname" value="JONES"></interp>
                     <interp inst="t18130602-27-person270" type="given" value="SAMUEL"></interp>
                     <interp inst="t18130602-27-person270" type="gender" value="male"></interp>
