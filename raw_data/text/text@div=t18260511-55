               <p>953. 
               <persName id="t18260511-55-defend412" type="defendantName">
                  JOHN 
                  CARTER
               <interp inst="t18260511-55-defend412" type="surname" value="CARTER"></interp>
                     <interp inst="t18260511-55-defend412" type="given" value="JOHN"></interp>
