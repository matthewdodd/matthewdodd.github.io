                        <interp inst="t18210912-141-victim1501" type="gender" value="male"></interp>
                     </persName>
                     
                  , son of 
                     <persName id="t18210912-141-victim1502" type="victimName">
                        
                              
                                 <persName id="t18210912-141-victim1504" type="victimName">
                                    John 
                                    Brady
                                 <interp inst="t18210912-141-victim1504" type="surname" value="Brady"></interp>
                           <interp inst="t18210912-141-victim1504" type="given" value="John"></interp>
                           <interp inst="t18210912-141-victim1504" type="gender" value="male"></interp>
                        </persName>
                              
                           , and Norah his <rs id="t18210912-141-viclabel820" type="occupation">wife</rs>
                        <join result="persNameOccupation" targOrder="Y" targets="t18210912-141-victim1502 t18210912-141-viclabel820"></join>
                        <interp inst="t18210912-141-victim1502" type="surname" value="Brady"></interp>
