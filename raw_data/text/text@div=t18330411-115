                  <persName id="t18330411-115-person915">
                  GEORGE 
                  WILSON
               <interp inst="t18330411-115-person915" type="surname" value="WILSON"></interp>
                     <interp inst="t18330411-115-person915" type="given" value="GEORGE"></interp>
                     <interp inst="t18330411-115-person915" type="gender" value="male"></interp>
