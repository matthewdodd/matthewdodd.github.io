             and 
               
                  <persName id="t17970426-41-defend348" type="defendantName">
                     THOMAS 
                     JONES
                  <interp inst="t17970426-41-defend348" type="surname" value="JONES"></interp>
                     <interp inst="t17970426-41-defend348" type="given" value="THOMAS"></interp>
