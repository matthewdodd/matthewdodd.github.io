                  <persName id="t18290409-210-person1995">
                  ELIZA 
                  WALKER
               <interp inst="t18290409-210-person1995" type="surname" value="WALKER"></interp>
                     <interp inst="t18290409-210-person1995" type="given" value="ELIZA"></interp>
                     <interp inst="t18290409-210-person1995" type="gender" value="female"></interp>
