                     <interp inst="t18030216-39-victim240" type="gender" value="male"></interp>
                  </persName>
            , 
               <persName id="t18030216-39-victim241" type="victimName">
                  Edward 
                  Sage
               <interp inst="t18030216-39-victim241" type="surname" value="Sage"></interp>
