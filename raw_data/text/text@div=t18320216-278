               <p>786. 
               
                  <persName id="t18320216-278-defend2296" type="defendantName">
                     JAMES 
                     PHELPS
                  <interp inst="t18320216-278-defend2296" type="surname" value="PHELPS"></interp>
                     <interp inst="t18320216-278-defend2296" type="given" value="JAMES"></interp>
