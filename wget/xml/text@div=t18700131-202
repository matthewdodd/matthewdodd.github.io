                        <interp inst="t18700131-202-verdict-1" type="verdictSubcategory" value="no_subcategory"></interp>GUILTY</rs>
                  </hi>—<hi rend="italic">
                     <rs id="t18700131-202-punishment-14" type="punishmentDescription">
                        <interp inst="t18700131-202-punishment-14" type="punishmentCategory" value="imprison"></interp>
