               <p>766. 
               
                  <persName id="t18170416-229-defend1678" type="defendantName">
                     JAMES 
                     PEELE
                  <interp inst="t18170416-229-defend1678" type="surname" value="PEELE"></interp>
                     <interp inst="t18170416-229-defend1678" type="given" value="JAMES"></interp>
