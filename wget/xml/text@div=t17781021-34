                  <persName id="t17781021-34-defend284" type="defendantName">
                     CATHERINE
                      DAVIS, otherwise <rs id="t17781021-34-alias-2" type="alias">
                        <join result="nameAlias" targOrder="Y" targets="t17781021-34-defend284 t17781021-34-alias-2"></join>SIMPSON</rs>
