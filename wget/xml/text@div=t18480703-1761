                        <interp inst="t18480703-name-685" type="surname" value="STILL"></interp>
                        <interp inst="t18480703-name-685" type="given" value="WILLIAM SAMUEL"></interp>
                        <join result="offenceVictim" targOrder="Y" targets="t18480703-1761-offence-1 t18480703-name-685"></join>William Samuel Still</persName>.</rs>
