                        <interp inst="t18651120-16-verdict-1" type="verdictSubcategory" value="no_subcategory"></interp>GUILTY</rs>
                  </hi>.—<hi rend="italic">
                     <rs id="t18651120-16-punishment-16" type="punishmentDescription">
