                  <persName id="t18211024-69-person694">
                  MARY 
                  ATKINS
               <interp inst="t18211024-69-person694" type="surname" value="ATKINS"></interp>
                     <interp inst="t18211024-69-person694" type="given" value="MARY"></interp>
                     <interp inst="t18211024-69-person694" type="gender" value="female"></interp>
