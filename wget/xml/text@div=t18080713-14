               <p>488. 
               
                  <persName id="t18080713-14-defend187" type="defendantName">
                     ANN 
                     CARTER, alias <rs id="t18080713-14-alias-1" type="alias">
                        <join result="nameAlias" targOrder="Y" targets="t18080713-14-defend187 t18080713-14-alias-1"></join>FARMER</rs>
                     <interp inst="t18080713-14-defend187" type="surname" value="CARTER"></interp>
