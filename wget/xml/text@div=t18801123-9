                        <interp inst="t18801123-9-verdict-1" type="verdictSubcategory" value="no_subcategory"></interp>GUILTY</rs>
                  </hi>.—<hi rend="italic">
                     <rs id="t18801123-9-punishment-7" type="punishmentDescription">
                        <interp inst="t18801123-9-punishment-7" type="punishmentCategory" value="imprison"></interp>
