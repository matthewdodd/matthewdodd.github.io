<div1 type="trialAccount" id="t17671209-25">
               <interp inst="t17671209-25" type="collection" value="BAILEY"></interp>
               <interp inst="t17671209-25" type="year" value="1767"></interp>
               <interp inst="t17671209-25" type="uri" value="sessionsPapers/17671209"></interp>
