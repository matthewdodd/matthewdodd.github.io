                        <interp inst="t18700228-283-verdict-1" type="verdictSubcategory" value="no_subcategory"></interp>GUILTY</rs>
                  </hi>—<hi rend="italic">
                     <rs id="t18700228-283-punishment-39" type="punishmentDescription">
