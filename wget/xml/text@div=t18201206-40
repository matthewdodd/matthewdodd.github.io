             and 
               
                  <persName id="t18201206-40-defend429" type="defendantName">
                     SAMUEL 
                     CHANDLER
                  <interp inst="t18201206-40-defend429" type="surname" value="CHANDLER"></interp>
                     <interp inst="t18201206-40-defend429" type="given" value="SAMUEL"></interp>
