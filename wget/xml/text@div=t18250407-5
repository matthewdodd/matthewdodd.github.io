                   and SARAH, HIS <rs id="t18250407-5-deflabel24" type="occupation">WIFE</rs>
                     <join result="persNameOccupation" targOrder="Y" targets="t18250407-5-defend121 t18250407-5-deflabel24"></join>
                     <interp inst="t18250407-5-defend121" type="surname" value="DARBY"></interp>
