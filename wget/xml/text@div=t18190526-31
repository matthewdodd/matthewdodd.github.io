                  <persName id="t18190526-31-person318">
                  ANN 
                  DOWSE
               <interp inst="t18190526-31-person318" type="surname" value="DOWSE"></interp>
                     <interp inst="t18190526-31-person318" type="given" value="ANN"></interp>
                     <interp inst="t18190526-31-person318" type="gender" value="female"></interp>
