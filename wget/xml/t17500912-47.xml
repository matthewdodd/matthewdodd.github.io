               <join result="criminalCharge" id="t17500912-47-off201-c262" targOrder="Y" targets="t17500912-47-defend356 t17500912-47-off201 t17500912-47-verdict203"></join>
               <join result="criminalCharge" id="t17500912-47-off201-c263" targOrder="Y" targets="t17500912-47-defend357 t17500912-47-off201 t17500912-47-verdict203"></join>
