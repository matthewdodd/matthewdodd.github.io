                  <persName id="t18210912-208-person2022">
                  GEORGE 
                  WHITFIELD ROYER
               <interp inst="t18210912-208-person2022" type="surname" value="WHITFIELD ROYER"></interp>
                     <interp inst="t18210912-208-person2022" type="given" value="GEORGE"></interp>
