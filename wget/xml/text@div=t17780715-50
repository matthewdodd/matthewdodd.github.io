                  <persName id="t17780715-50-person497">
                  ISAAC
                   HAWLEY
               <interp inst="t17780715-50-person497" type="surname" value="HAWLEY"></interp>
                     <interp inst="t17780715-50-person497" type="given" value="ISAAC"></interp>
                     <interp inst="t17780715-50-person497" type="gender" value="male"></interp>
