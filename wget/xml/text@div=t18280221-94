               <p>575. 
               <persName id="t18280221-94-defend673" type="defendantName">
                  JOHN 
                  KENNEY
               <interp inst="t18280221-94-defend673" type="surname" value="KENNEY"></interp>
                     <interp inst="t18280221-94-defend673" type="given" value="JOHN"></interp>
