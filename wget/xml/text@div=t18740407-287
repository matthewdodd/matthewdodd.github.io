                        <interp inst="t18740407-287-verdict-1" type="verdictSubcategory" value="no_subcategory"></interp>GUILTY</rs>
                  </hi>.—<hi rend="italic">
                     <rs id="t18740407-287-punishment-47" type="punishmentDescription">
                        <interp inst="t18740407-287-punishment-47" type="punishmentCategory" value="imprison"></interp>
