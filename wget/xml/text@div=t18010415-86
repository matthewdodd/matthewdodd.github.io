               <p>379. 
               
                  <persName id="t18010415-86-defend926" type="defendantName">
                     JOHN 
                     TURNER
                  <interp inst="t18010415-86-defend926" type="surname" value="TURNER"></interp>
                     <interp inst="t18010415-86-defend926" type="given" value="JOHN"></interp>
