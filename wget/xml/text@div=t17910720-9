                     <interp inst="t17910720-9-off47" type="offenceSubcategory" value="highwayRobbery"></interp>
               feloniously assaulting 
                     
                        <persName id="t17910720-9-victim134" type="victimName">
                           Benedict 
                           Debar
                        <interp inst="t17910720-9-victim134" type="surname" value="Debar"></interp>
