<div1 type="trialAccount" id="t17470604-14">
               <interp inst="t17470604-14" type="collection" value="BAILEY"></interp>
               <interp inst="t17470604-14" type="year" value="1747"></interp>
               <interp inst="t17470604-14" type="uri" value="sessionsPapers/17470604"></interp>
