                        <interp inst="t18710605-481-verdict-1" type="verdictSubcategory" value="no_subcategory"></interp>GUILTY</rs>
                  </hi>—<hi rend="italic">
                     <rs id="t18710605-481-punishment-53" type="punishmentDescription">
                        <interp inst="t18710605-481-punishment-53" type="punishmentCategory" value="imprison"></interp>
