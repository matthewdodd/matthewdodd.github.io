<div1 type="trialAccount" id="t16880222-12">
               <interp inst="t16880222-12" type="collection" value="BAILEY"></interp>
               <interp inst="t16880222-12" type="year" value="1688"></interp>
               <interp inst="t16880222-12" type="uri" value="sessionsPapers/16880222"></interp>
