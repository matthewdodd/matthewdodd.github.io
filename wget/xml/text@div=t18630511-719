                        <interp inst="t18630511-719-verdict-1" type="verdictSubcategory" value="no_subcategory"></interp>GUILTY</rs>
                  </hi>.—<hi rend="italic">
                     <rs id="t18630511-719-punishment-64" type="punishmentDescription">
                        <interp inst="t18630511-719-punishment-64" type="punishmentCategory" value="imprison"></interp>
