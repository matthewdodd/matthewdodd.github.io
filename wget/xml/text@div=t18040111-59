                        <interp inst="t18040111-59-victim571" type="gender" value="female"></interp>
                        <join result="offenceVictim" targOrder="Y" targets="t18040111-59-off353 t18040111-59-victim571"></join>
                     </persName>
                     
                  , <rs id="t18040111-59-viclabel355" type="occupation">spinster</rs>
                     <join result="persNameOccupation" targOrder="Y" targets="t18040111-59-victim571 t18040111-59-viclabel355"></join>, in the dwelling-house of 
                     <persName id="t18040111-59-person572">
                        Ann 
                        Hopkins
                     <interp inst="t18040111-59-person572" type="surname" value="Hopkins"></interp>
