                     <hi rend="largeCaps">JAMES McDONALD</hi> (24)</persName>, and <persName id="def2-69-18701212" type="defendantName">
                     <interp inst="def2-69-18701212" type="gender" value="male"></interp>
                     <interp inst="def2-69-18701212" type="age" value="18"></interp>
