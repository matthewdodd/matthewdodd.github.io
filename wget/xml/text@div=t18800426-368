                        <interp inst="t18800426-368-verdict-1" type="verdictSubcategory" value="no_subcategory"></interp>GUILTY</rs>
                  </hi>.—<hi rend="italic">
                     <rs id="t18800426-368-punishment-14" type="punishmentDescription">
                        <interp inst="t18800426-368-punishment-14" type="punishmentCategory" value="imprison"></interp>
