                  <persName id="t18220220-123-person1215">
                  WILLIAM 
                  LOVITT
               <interp inst="t18220220-123-person1215" type="surname" value="LOVITT"></interp>
                     <interp inst="t18220220-123-person1215" type="given" value="WILLIAM"></interp>
                     <interp inst="t18220220-123-person1215" type="gender" value="male"></interp>
