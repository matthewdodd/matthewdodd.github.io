               <persName id="t17320419-27-person231">
                  Henry
                   Dodd, alias <rs id="t17320419-27-alias-4" type="alias">
                        <join result="nameAlias" targOrder="Y" targets="t17320419-27-person231 t17320419-27-alias-4"></join>Ford</rs>
