                     <hi rend="largeCaps">HENRY WEEDON</hi> (26)</persName>, <persName id="def2-509-18800524" type="defendantName">
                     <interp inst="def2-509-18800524" type="gender" value="female"></interp>
                     <interp inst="def2-509-18800524" type="age" value="23"></interp>
                     <interp inst="def2-509-18800524" type="surname" value="NEALE"></interp>
                     <interp inst="def2-509-18800524" type="given" value="GERTRUDE"></interp>
                     <hi rend="largeCaps">GERTRUDE NEALE</hi> (23)</persName>, and <persName id="def3-509-18800524" type="defendantName">
                     <interp inst="def3-509-18800524" type="gender" value="male"></interp>
                     <interp inst="def3-509-18800524" type="age" value="22"></interp>
