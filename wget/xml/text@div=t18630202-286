               <interp inst="t18630202-286" type="date" value="18630202"></interp>
               <join result="criminalCharge" id="t18630202-286-charge-1" targOrder="Y" targets="def1-286-18630202 t18630202-286-offence-1 t18630202-286-verdict-1"></join>
