                     <join result="defendantPunishment" targOrder="Y" targets="t17900113-27-defend341 t17900113-27-punish150"></join>
               Whipped
            </rs>, and <rs id="t17900113-27-punish151" type="punishmentDescription">
                     <interp inst="t17900113-27-punish151" type="punishmentCategory" value="imprison"></interp>
                     <join result="defendantPunishment" targOrder="Y" targets="t17900113-27-defend341 t17900113-27-punish151"></join>
