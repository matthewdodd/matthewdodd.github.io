                  <persName id="t18240715-110-person1000">
                  JOHN 
                  CARRUTHERS
               <interp inst="t18240715-110-person1000" type="surname" value="CARRUTHERS"></interp>
                     <interp inst="t18240715-110-person1000" type="given" value="JOHN"></interp>
