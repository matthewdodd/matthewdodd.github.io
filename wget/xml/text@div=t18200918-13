                     <interp inst="t18200918-13-off69" type="offenceCategory" value="violentTheft"></interp>
                     <interp inst="t18200918-13-off69" type="offenceSubcategory" value="highwayRobbery"></interp>
               feloniously assaulting 
                     
                        <persName id="t18200918-13-victim188" type="victimName">
                           Joseph 
                           Welham
                        <interp inst="t18200918-13-victim188" type="surname" value="Welham"></interp>
