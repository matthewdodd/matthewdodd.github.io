                     <interp inst="t18231022-125-defend1079" type="given" value="JOHN"></interp>
                     <interp inst="t18231022-125-defend1079" type="gender" value="male"></interp>
                  </persName>
               
             was indicted for <rs id="t18231022-125-off667" type="offenceDescription">
                     <interp inst="t18231022-125-off667" type="offenceCategory" value="theft"></interp>
                     <interp inst="t18231022-125-off667" type="offenceSubcategory" value="receiving"></interp>
               feloniously receiving, on the <rs id="t18231022-125-cd668" type="crimeDate">14th of July</rs>
                     <join result="offenceCrimeDate" targOrder="Y" targets="t18231022-125-off667 t18231022-125-cd668"></join>, a book, value 2 s., the goods of our Lord 
                     <persName id="t18231022-125-victim1080" type="victimName">
                        <rs id="t18231022-125-viclabel669" type="occupation">the King</rs>
                        <join result="persNameOccupation" targOrder="Y" targets="t18231022-125-victim1080 t18231022-125-viclabel669"></join>
