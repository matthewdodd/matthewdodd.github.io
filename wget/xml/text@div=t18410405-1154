                     <interp inst="t18410405-1154-offence-1" type="offenceCategory" value="theft"></interp>
                     <interp inst="t18410405-1154-offence-1" type="offenceSubcategory" value="simpleLarceny"></interp> stealing, on the <rs id="t18410405-cd-137" type="crimeDate">
                        <join result="offenceCrimeDate" targOrder="Y" targets="t18410405-1154-offence-1 t18410405-cd-137"></join>15th of March</rs>, 1 counterpane, value 10<hi rend="italic">s</hi>.; 1 blanket, value 9<hi rend="italic">s</hi>.; 1 gown, value 9<hi rend="italic">s</hi>.; 1 candlestick, value 2<hi rend="italic">s</hi>.; 1 pair of shoes, value 2<hi rend="italic">s</hi>.; 1 table-cloth, value 2<hi rend="italic">s</hi>. 6<hi rend="italic">d</hi>.; 2 yards of crape, value 1<hi rend="italic">s</hi>. 6<hi rend="italic">d</hi>. and 4 yards of lace, value 9<hi rend="italic">d</hi>.; the goods of <persName id="t18410405-name-928" type="victimName">
                        <interp inst="t18410405-name-928" type="gender" value="male"></interp>
                        <interp inst="t18410405-name-928" type="surname" value="JACQUES"></interp>
