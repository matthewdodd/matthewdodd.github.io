                  <persName id="t18180114-187-person1279">
                  WILLIAM 
                  NICHOLLS
               <interp inst="t18180114-187-person1279" type="surname" value="NICHOLLS"></interp>
                     <interp inst="t18180114-187-person1279" type="given" value="WILLIAM"></interp>
