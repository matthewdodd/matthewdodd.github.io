                  ; and a half handkerchief, value 1 s., the goods of 
                     
                        <persName id="t18240114-38-victim404" type="victimName">
                           Elizabeth 
                           Moody
                        <interp inst="t18240114-38-victim404" type="surname" value="Moody"></interp>
