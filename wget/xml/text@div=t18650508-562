                     <rs id="t18650508-562-verdict-1" type="verdictDescription">
                        <interp inst="t18650508-562-verdict-1" type="verdictCategory" value="guilty"></interp>
                        <interp inst="t18650508-562-verdict-1" type="verdictSubcategory" value="no_subcategory"></interp>GUILTY</rs>
                  </hi>.—** <hi rend="italic">
                     <rs id="t18650508-562-punishment-57" type="punishmentDescription">
                        <interp inst="t18650508-562-punishment-57" type="punishmentCategory" value="imprison"></interp>
