                        <join result="offenceVictim" targOrder="Y" targets="t18460511-1108-offence-1 t18460511-name-680"></join>Henry Benjamin</persName>, her master: and </rs>
                  <persName id="def2-1108-18460511" type="defendantName">
                     <interp inst="def2-1108-18460511" type="gender" value="female"></interp>
                     <interp inst="def2-1108-18460511" type="age" value="35"></interp>
                     <interp inst="def2-1108-18460511" type="surname" value="M'CARTHY"></interp>
