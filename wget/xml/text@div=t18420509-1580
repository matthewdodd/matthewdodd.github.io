                        <interp inst="t18420509-name-822" type="gender" value="male"></interp>
                        <interp inst="t18420509-name-822" type="surname" value="COVE"></interp>
                        <interp inst="t18420509-name-822" type="given" value="HENRY"></interp>HENRY COVE</persName>
