               <p>
            
                  <persName id="t18190707-161-person1362">
                  RICHARD 
                  EDWARD SCURRAH
               <interp inst="t18190707-161-person1362" type="surname" value="EDWARD SCURRAH"></interp>
                     <interp inst="t18190707-161-person1362" type="given" value="RICHARD"></interp>
                     <interp inst="t18190707-161-person1362" type="gender" value="male"></interp>
