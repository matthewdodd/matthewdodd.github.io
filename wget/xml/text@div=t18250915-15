               <p>1237. 
               <persName id="t18250915-15-defend186" type="defendantName">
                  RUTH 
                  BROMLEY
               <interp inst="t18250915-15-defend186" type="surname" value="BROMLEY"></interp>
                     <interp inst="t18250915-15-defend186" type="given" value="RUTH"></interp>
