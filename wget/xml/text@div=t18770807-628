               <p>
                  <hi rend="largeCaps">LAING</hi> 
                  <hi rend="smallCaps">
                     <rs id="t18770807-628-verdict-1" type="verdictDescription">
                        <interp inst="t18770807-628-verdict-1" type="verdictCategory" value="guilty"></interp>
                        <interp inst="t18770807-628-verdict-1" type="verdictSubcategory" value="pleadedGuilty"></interp>PLEADED GUILTY</rs>
