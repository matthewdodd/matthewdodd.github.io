                     <interp inst="t17280228-82-off353" type="offenceCategory" value="theft"></interp>
                     <interp inst="t17280228-82-off353" type="offenceSubcategory" value="extortion"></interp>
               extorting Money from 
                     <persName id="t17280228-82-victim460" type="victimName">
                        Rodolphus 
                        Blank 
                     <interp inst="t17280228-82-victim460" type="surname" value="Blank"></interp>
