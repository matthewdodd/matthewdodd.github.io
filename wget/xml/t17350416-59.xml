               <p>65. 
               
                  <persName id="t17350416-59-defend404" type="defendantName">
                     John 
                     Tucker 
                  <interp inst="t17350416-59-defend404" type="surname" value="Tucker"></interp>
                     <interp inst="t17350416-59-defend404" type="given" value="John"></interp>
                     <interp inst="t17350416-59-defend404" type="gender" value="male"></interp>
                  </persName>
               
            , was indicted for <rs id="t17350416-59-off258" type="offenceDescription">
                     <interp inst="t17350416-59-off258" type="offenceCategory" value="sexual"></interp>
                     <interp inst="t17350416-59-off258" type="offenceSubcategory" value="bigamy"></interp>
               marrying 
                     
                        <persName id="t17350416-59-victim406" type="victimName">
                           Dorothy 
                           Chancellor 
                        <interp inst="t17350416-59-victim406" type="surname" value="Chancellor"></interp>
                        <interp inst="t17350416-59-victim406" type="given" value="Dorothy"></interp>
                        <interp inst="t17350416-59-victim406" type="gender" value="female"></interp>
                     </persName>
                      
