               <p>480. 
               
                  <persName id="t18110529-101-defend846" type="defendantName">
                     JOHN 
                     MANNING
                  <interp inst="t18110529-101-defend846" type="surname" value="MANNING"></interp>
                     <interp inst="t18110529-101-defend846" type="given" value="JOHN"></interp>
