                        <interp inst="t18711211-99-verdict-1" type="verdictSubcategory" value="no_subcategory"></interp>GUILTY</rs>
                  </hi>.—<hi rend="italic">
                     <rs id="t18711211-99-punishment-27" type="punishmentDescription">
                        <interp inst="t18711211-99-punishment-27" type="punishmentCategory" value="imprison"></interp>
