               <persName id="t17991030-38-person398">
                  William
                   Wilkins
               <interp inst="t17991030-38-person398" type="surname" value="Wilkins"></interp>
                     <interp inst="t17991030-38-person398" type="given" value="William"></interp>
                     <interp inst="t17991030-38-person398" type="gender" value="male"></interp>
