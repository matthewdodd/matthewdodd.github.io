               <p>1206. 
               
                  <persName id="t18220911-61-defend663" type="defendantName">
                     FRANCES 
                     WADDELL
                  <interp inst="t18220911-61-defend663" type="surname" value="WADDELL"></interp>
                     <interp inst="t18220911-61-defend663" type="given" value="FRANCES"></interp>
