            </rs> from 
               <persName id="t16890516-28-victim139" type="victimName">
                     <rs id="t16890516-28-viclabel127" type="occupation">Esquire</rs>
                     <join result="persNameOccupation" targOrder="Y" targets="t16890516-28-victim139 t16890516-28-viclabel127"></join> 
