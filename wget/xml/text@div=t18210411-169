                     <interp inst="t18210411-169-defend1672" type="gender" value="male"></interp>
                     <interp inst="t18210411-169-defend1672" type="age" value="16"></interp>
                  </persName>
               
             were indicted for <rs id="t18210411-169-off1043" type="offenceDescription">
                     <interp inst="t18210411-169-off1043" type="offenceCategory" value="theft"></interp>
                     <interp inst="t18210411-169-off1043" type="offenceSubcategory" value="grandLarceny"></interp>
               stealing, on the <rs id="t18210411-169-cd1044" type="crimeDate">5th of April</rs>
                     <join result="offenceCrimeDate" targOrder="Y" targets="t18210411-169-off1043 t18210411-169-cd1044"></join>, four fenders, value 30 s.
            </rs>, the goods of 
               
                  <persName id="t18210411-169-victim1674" type="victimName">
                     George 
                     Bishop
                  <interp inst="t18210411-169-victim1674" type="surname" value="Bishop"></interp>
