                  <hi rend="largeCaps">ANDREWS</hi> 
                  <hi rend="italic">and</hi> 
                  <hi rend="largeCaps">JOHN WINSLOW</hi> 
                  <hi rend="italic">also</hi> 
                  <hi rend="smallCaps">PLEADED GUILTY</hi> 
                  <hi rend="italic">to previous convictions of felony.</hi>
               </p>
