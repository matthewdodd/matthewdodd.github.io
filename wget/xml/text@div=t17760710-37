                      and MARGARET
                   his 
                  <rs id="t17760710-37-deflabel170" type="occupation">Wife</rs>
                     <join result="persNameOccupation" targOrder="Y" targets="t17760710-37-defend324 t17760710-37-deflabel170"></join>
