               <p>
            
                  <persName id="t18290409-78-person780">
                  WILLIAM 
                  USHER LEATHER
               <interp inst="t18290409-78-person780" type="surname" value="USHER LEATHER"></interp>
                     <interp inst="t18290409-78-person780" type="given" value="WILLIAM"></interp>
                     <interp inst="t18290409-78-person780" type="gender" value="male"></interp>
