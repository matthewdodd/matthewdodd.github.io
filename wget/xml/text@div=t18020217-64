               <p>237. 
               
                  <persName id="t18020217-64-defend734" type="defendantName">
                     HENRIETTA 
                     STOCKHAM
                  <interp inst="t18020217-64-defend734" type="surname" value="STOCKHAM"></interp>
                     <interp inst="t18020217-64-defend734" type="given" value="HENRIETTA"></interp>
