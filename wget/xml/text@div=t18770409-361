                  <rs id="t18770409-361-offence-1" type="offenceDescription">
                     <interp inst="t18770409-361-offence-1" type="offenceCategory" value="theft"></interp>
                     <interp inst="t18770409-361-offence-1" type="offenceSubcategory" value="housebreaking"></interp>, Feloniously breaking and entering the dwelling-house of <persName id="t18770409-name-141" type="victimName">
                        <interp inst="t18770409-name-141" type="gender" value="female"></interp>
                        <interp inst="t18770409-name-141" type="surname" value="BARLOW"></interp>
                        <interp inst="t18770409-name-141" type="given" value="ELIZABETH"></interp>
                        <join result="offenceVictim" targOrder="Y" targets="t18770409-361-offence-1 t18770409-name-141"></join>Elizabeth Barlow</persName> with intent to steal.</rs>
