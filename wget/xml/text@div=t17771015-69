                  <persName id="t17771015-69-person627">
                  SAWYER
                   SPENCE
               <interp inst="t17771015-69-person627" type="surname" value="SPENCE"></interp>
                     <interp inst="t17771015-69-person627" type="given" value="SAWYER"></interp>
                     <interp inst="t17771015-69-person627" type="gender" value="male"></interp>
