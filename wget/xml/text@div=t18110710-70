               <p>
            
                  <persName id="t18110710-70-person702">
                  JAMES 
                  GORDON
               <interp inst="t18110710-70-person702" type="surname" value="GORDON"></interp>
                     <interp inst="t18110710-70-person702" type="given" value="JAMES"></interp>
                     <interp inst="t18110710-70-person702" type="gender" value="male"></interp>
