                     <hi rend="largeCaps">ELIZABETH DAVISON</hi> (17)</persName>, <persName id="def2-471-18640411" type="defendantName">
                     <interp inst="def2-471-18640411" type="gender" value="male"></interp>
                     <interp inst="def2-471-18640411" type="age" value="17"></interp>
