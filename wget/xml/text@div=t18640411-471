                     <interp inst="def2-471-18640411" type="given" value="CHARLES"></interp>
                     <hi rend="largeCaps">CHARLES SMITH</hi> (17)</persName>, and <persName id="def3-471-18640411" type="defendantName">
                     <interp inst="def3-471-18640411" type="gender" value="male"></interp>
                     <interp inst="def3-471-18640411" type="age" value="17"></interp>
