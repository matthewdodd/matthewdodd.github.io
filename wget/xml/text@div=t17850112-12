               <persName id="t17850112-12-person226">
                  ROBERT
                   BROWN
               <interp inst="t17850112-12-person226" type="surname" value="BROWN"></interp>
                     <interp inst="t17850112-12-person226" type="given" value="ROBERT"></interp>
                     <interp inst="t17850112-12-person226" type="gender" value="male"></interp>
