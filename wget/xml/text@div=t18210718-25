                     <interp inst="t18210718-25-defend256" type="gender" value="male"></interp>
                     <interp inst="t18210718-25-defend256" type="age" value="35"></interp>
                  </persName>
               
             was indicted for <rs id="t18210718-25-off141" type="offenceDescription">
                     <interp inst="t18210718-25-off141" type="offenceCategory" value="theft"></interp>
                     <interp inst="t18210718-25-off141" type="offenceSubcategory" value="grandLarceny"></interp>
               stealing, on the <rs id="t18210718-25-cd142" type="crimeDate">8th of June</rs>
                     <join result="offenceCrimeDate" targOrder="Y" targets="t18210718-25-off141 t18210718-25-cd142"></join>, 11 lbs of silk, value 18 l.
            </rs> the goods of 
               
                  <persName id="t18210718-25-victim258" type="victimName">
                     James 
                     Jones
                  <interp inst="t18210718-25-victim258" type="surname" value="Jones"></interp>
