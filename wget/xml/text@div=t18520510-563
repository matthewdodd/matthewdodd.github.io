                  <rs id="t18520510-563-offence-1" type="offenceDescription">
                     <interp inst="t18520510-563-offence-1" type="offenceCategory" value="theft"></interp>
                     <interp inst="t18520510-563-offence-1" type="offenceSubcategory" value="simpleLarceny"></interp>, stealing 1 coat, value 18<hi rend="italic">s</hi>.; the goods of <persName id="t18520510-name-547" type="victimName">
                        <interp inst="t18520510-name-547" type="gender" value="male"></interp>
                        <interp inst="t18520510-name-547" type="surname" value="HENDERSON"></interp>
