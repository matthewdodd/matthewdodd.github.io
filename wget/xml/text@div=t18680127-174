                        <interp inst="t18680127-174-verdict-1" type="verdictSubcategory" value="no_subcategory"></interp>GUILTY</rs>
                  </hi>.—<hi rend="italic">
                     <rs id="t18680127-174-punishment-28" type="punishmentDescription">
                        <interp inst="t18680127-174-punishment-28" type="punishmentCategory" value="imprison"></interp>
