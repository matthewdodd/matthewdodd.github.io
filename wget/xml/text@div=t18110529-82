                        <interp inst="t18110529-82-victim709" type="gender" value="male"></interp>
                     </persName>
                     
                  , 
                     
                        <persName id="t18110529-82-victim711" type="victimName">
                           Charles 
                           Luck
                        <interp inst="t18110529-82-victim711" type="surname" value="Luck"></interp>
                        <interp inst="t18110529-82-victim711" type="given" value="Charles"></interp>
                        <interp inst="t18110529-82-victim711" type="gender" value="male"></interp>
                     </persName>
                     
                  , 
                     
                        <persName id="t18110529-82-victim713" type="victimName">
                           Alexander 
                           Kemp
                        <interp inst="t18110529-82-victim713" type="surname" value="Kemp"></interp>
