                  <rs id="t18760918-397-offence-1" type="offenceDescription">
                     <interp inst="t18760918-397-offence-1" type="offenceCategory" value="violentTheft"></interp>
                     <interp inst="t18760918-397-offence-1" type="offenceSubcategory" value="robbery"></interp>, Robbery on <persName id="t18760918-name-73" type="victimName">
                        <interp inst="t18760918-name-73" type="gender" value="male"></interp>
                        <interp inst="t18760918-name-73" type="surname" value="SMITH"></interp>
