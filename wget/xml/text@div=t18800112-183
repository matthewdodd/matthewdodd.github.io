                        <interp inst="t18800112-183-verdict-1" type="verdictSubcategory" value="no_subcategory"></interp>GUILTY</rs>
                  </hi>.—<hi rend="italic">
                     <rs id="t18800112-183-punishment-38" type="punishmentDescription">
                        <interp inst="t18800112-183-punishment-38" type="punishmentCategory" value="imprison"></interp>
