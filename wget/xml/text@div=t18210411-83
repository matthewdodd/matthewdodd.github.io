               <p>570. 
               
                  <persName id="t18210411-83-defend952" type="defendantName">
                     DANIEL 
                     DUNN
                  <interp inst="t18210411-83-defend952" type="surname" value="DUNN"></interp>
                     <interp inst="t18210411-83-defend952" type="given" value="DANIEL"></interp>
                     <interp inst="t18210411-83-defend952" type="gender" value="male"></interp>
