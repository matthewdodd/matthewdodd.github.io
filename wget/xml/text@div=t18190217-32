                  <persName id="t18190217-32-person376">
                  JAMES 
                  SQUIRES
               <interp inst="t18190217-32-person376" type="surname" value="SQUIRES"></interp>
                     <interp inst="t18190217-32-person376" type="given" value="JAMES"></interp>
                     <interp inst="t18190217-32-person376" type="gender" value="male"></interp>
