                  </persName>
               
             and 
               
                  <persName id="t18290219-158-defend1471" type="defendantName">
                     JUDITH 
                     DONOVAN
                  <interp inst="t18290219-158-defend1471" type="surname" value="DONOVAN"></interp>
                     <interp inst="t18290219-158-defend1471" type="given" value="JUDITH"></interp>
                     <interp inst="t18290219-158-defend1471" type="gender" value="female"></interp>
                  </persName>
               
             were indicted for <rs id="t18290219-158-off907" type="offenceDescription">
                     <interp inst="t18290219-158-off907" type="offenceCategory" value="violentTheft"></interp>
                     <interp inst="t18290219-158-off907" type="offenceSubcategory" value="highwayRobbery"></interp>
               felonlously assaulting 
                     
                        <persName id="t18290219-158-victim1473" type="victimName">
                           Jeremiah 
                           Conner
                        <interp inst="t18290219-158-victim1473" type="surname" value="Conner"></interp>
                        <interp inst="t18290219-158-victim1473" type="given" value="Jeremiah"></interp>
                        <interp inst="t18290219-158-victim1473" type="gender" value="male"></interp>
                        <join result="offenceVictim" targOrder="Y" targets="t18290219-158-off907 t18290219-158-victim1473"></join>
