<div1 type="trialAccount" id="t16900717-4">
               <interp inst="t16900717-4" type="collection" value="BAILEY"></interp>
               <interp inst="t16900717-4" type="year" value="1690"></interp>
               <interp inst="t16900717-4" type="uri" value="sessionsPapers/16900717"></interp>
