                  <persName id="t17991030-41-person418">
                  JOHN
                   TANNER
               <interp inst="t17991030-41-person418" type="surname" value="TANNER"></interp>
                     <interp inst="t17991030-41-person418" type="given" value="JOHN"></interp>
                     <interp inst="t17991030-41-person418" type="gender" value="male"></interp>
