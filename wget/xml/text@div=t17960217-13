                     <join result="defendantPunishment" targOrder="Y" targets="t17960217-13-defend171 t17960217-13-punish74"></join>
               Privately whipped
            </rs> and <rs id="t17960217-13-punish75" type="punishmentDescription">
                     <interp inst="t17960217-13-punish75" type="punishmentCategory" value="imprison"></interp>
                     <join result="defendantPunishment" targOrder="Y" targets="t17960217-13-defend171 t17960217-13-punish75"></join>
