               <p>497. 
               
                  <persName id="t18340220-183-defend1723" type="defendantName">
                     THOMAS 
                     JENKINS
                  <interp inst="t18340220-183-defend1723" type="surname" value="JENKINS"></interp>
                     <interp inst="t18340220-183-defend1723" type="given" value="THOMAS"></interp>
                     <interp inst="t18340220-183-defend1723" type="gender" value="male"></interp>
                  </persName>
               
             was indicted for <rs id="t18340220-183-off1042" type="offenceDescription">
                     <interp inst="t18340220-183-off1042" type="offenceCategory" value="theft"></interp>
                     <interp inst="t18340220-183-off1042" type="offenceSubcategory" value="simpleLarceny"></interp>
               stealing, on the <rs id="t18340220-183-cd1043" type="crimeDate">13th of February</rs>
                     <join result="offenceCrimeDate" targOrder="Y" targets="t18340220-183-off1042 t18340220-183-cd1043"></join>, 1 hat-case, value 30s., the goods of the <rs id="t18340220-183-viclabel1044" type="occupation">Honourable</rs>
                     <join result="persNameOccupation" targOrder="Y" targets="t18340220-183-victim1725 t18340220-183-viclabel1044"></join> 
                  
                     
                        <persName id="t18340220-183-victim1725" type="victimName">
                           George 
                           Anson
                        <interp inst="t18340220-183-victim1725" type="surname" value="Anson"></interp>
                        <interp inst="t18340220-183-victim1725" type="given" value="George"></interp>
                        <interp inst="t18340220-183-victim1725" type="gender" value="male"></interp>
                     </persName>
                     
                  ; and 1 hat, value 10s., and 1 glove, value 6d., the goods of 
                     
                        <persName id="t18340220-183-victim1727" type="victimName">
                           John 
                           Wilson
                        <interp inst="t18340220-183-victim1727" type="surname" value="Wilson"></interp>
                        <interp inst="t18340220-183-victim1727" type="given" value="John"></interp>
