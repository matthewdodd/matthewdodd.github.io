                  <persName id="t18230910-113-person1177">
                  THOMAS 
                  SPIVEY
               <interp inst="t18230910-113-person1177" type="surname" value="SPIVEY"></interp>
                     <interp inst="t18230910-113-person1177" type="given" value="THOMAS"></interp>
                     <interp inst="t18230910-113-person1177" type="gender" value="male"></interp>
