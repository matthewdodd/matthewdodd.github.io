               <p>616. 
               
                  <persName id="t17900915-26-defend281" type="defendantName">
                     CHARLOTTE 
                     WALKER
                  <interp inst="t17900915-26-defend281" type="surname" value="WALKER"></interp>
                     <interp inst="t17900915-26-defend281" type="given" value="CHARLOTTE"></interp>
                     <interp inst="t17900915-26-defend281" type="gender" value="female"></interp>
