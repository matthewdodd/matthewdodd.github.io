                  <persName id="t18180114-47-person395">
                  THOMAS 
                  SHEPHERD
               <interp inst="t18180114-47-person395" type="surname" value="SHEPHERD"></interp>
                     <interp inst="t18180114-47-person395" type="given" value="THOMAS"></interp>
                     <interp inst="t18180114-47-person395" type="gender" value="male"></interp>
