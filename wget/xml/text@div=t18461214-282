                        <join result="offenceVictim" targOrder="Y" targets="t18461214-282-offence-1 t18461214-name-451"></join>Frederick Marsh</persName>, her master: and </rs>
                  <persName id="def2-282-18461214" type="defendantName">
                     <interp inst="def2-282-18461214" type="gender" value="female"></interp>
                     <interp inst="def2-282-18461214" type="age" value="27"></interp>
                     <interp inst="def2-282-18461214" type="surname" value="HOARE"></interp>
                     <interp inst="def2-282-18461214" type="given" value="CATHARINE"></interp>
