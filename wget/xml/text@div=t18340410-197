                     <interp inst="t18340410-197-off1122" type="offenceCategory" value="theft"></interp>
                     <interp inst="t18340410-197-off1122" type="offenceSubcategory" value="receiving"></interp>
               receiving, on the <rs id="t18340410-197-cd1123" type="crimeDate">26th of February</rs>
                     <join result="offenceCrimeDate" targOrder="Y" targets="t18340410-197-off1122 t18340410-197-cd1123"></join>, 4 pair of gloves, value 10s., the goods of 
                     <persName id="t18340410-197-victim1278" type="victimName">
                        George 
                        Arnold
                     <interp inst="t18340410-197-victim1278" type="surname" value="Arnold"></interp>
                        <interp inst="t18340410-197-victim1278" type="given" value="George"></interp>
