                  <persName id="t18270111-171-person1607">
                  EDWIN 
                  AUGUSTUS WITHERS
               <interp inst="t18270111-171-person1607" type="surname" value="AUGUSTUS WITHERS"></interp>
                     <interp inst="t18270111-171-person1607" type="given" value="EDWIN"></interp>
