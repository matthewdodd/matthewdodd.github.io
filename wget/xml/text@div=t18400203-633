                     <interp inst="t18400203-633-offence-1" type="offenceSubcategory" value="simpleLarceny"></interp>for stealing, on the <rs id="t18400203-cd-71" type="crimeDate">
                        <join result="offenceCrimeDate" targOrder="Y" targets="t18400203-633-offence-1 t18400203-cd-71"></join>16th of January</rs>, 3 gowns, value 6<hi rend="italic">s</hi>.; 1 cloak, value 3<hi rend="italic">s</hi>.; 1 apron, value 4<hi rend="italic">d</hi>.; 1 petticoat, value 1<hi rend="italic">s</hi>.; the goods of <persName id="t18400203-name-475" type="victimName">
                        <interp inst="t18400203-name-475" type="gender" value="female"></interp>
                        <interp inst="t18400203-name-475" type="surname" value="SHEFFIELD"></interp>
