            . 
               <persName id="t17220510-38-person260">
                  Elizabeth
                   Howard, alias <rs id="t17220510-38-alias-8" type="alias">
                        <join result="nameAlias" targOrder="Y" targets="t17220510-38-person260 t17220510-38-alias-8"></join>Thomas</rs>
