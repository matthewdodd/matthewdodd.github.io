               <p>58. 
               <persName id="t17961130-58-defend501" type="defendantName">
                  
                     JOHN 
                     PURDY
                  , otherwise <rs id="t17961130-58-alias-5" type="alias">
                        <join result="nameAlias" targOrder="Y" targets="t17961130-58-defend501 t17961130-58-alias-5"></join>PILKINGTON</rs>
                     <interp inst="t17961130-58-defend501" type="surname" value="PURDY"></interp>
