            . I <rs id="t18141130-11-viclabel65" type="occupation">keep a chop-house</rs>
                  <join result="persNameOccupation" targOrder="Y" targets="t18141130-11-victim184 t18141130-11-viclabel65"></join> in <placeName id="t18141130-11-crimeloc66">Finch-lane</placeName>
