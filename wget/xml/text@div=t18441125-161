                        <join result="offenceCrimeDate" targOrder="Y" targets="t18441125-161-offence-1 t18441125-cd-53"></join>5th of Nov</rs>., 3 blankets, value 10<hi rend="italic">s</hi>.; 2 pillows, 7<hi rend="italic">s</hi>. 6<hi rend="italic">d</hi>.; 1 bolster, 2<hi rend="italic">s</hi>. 6<hi rend="italic">d</hi>.; 2 sheets, 2<hi rend="italic">s</hi>.; 5 pillow-cases, 3<hi rend="italic">s</hi>.; 2 table-cloths, 3<hi rend="italic">s</hi>. 6<hi rend="italic">d</hi>.; 1 toilet-cover, 1<hi rend="italic">s</hi>.; 2 counterpanes, 6<hi rend="italic">s</hi>.; 1 towel, 6<hi rend="italic">d</hi>.; ✗ looking-glass and stand, 3<hi rend="italic">s</hi>. 6<hi rend="italic">d</hi>.; and 1 salt-cellar, 2<hi rend="italic">s</hi>.; the goods of <persName id="t18441125-name-940" type="victimName">
                        <interp inst="t18441125-name-940" type="gender" value="male"></interp>
