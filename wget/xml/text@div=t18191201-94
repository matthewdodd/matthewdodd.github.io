                  <persName id="t18191201-94-person1037">
                  BARTHOLOMEW 
                  GINYER
               <interp inst="t18191201-94-person1037" type="surname" value="GINYER"></interp>
                     <interp inst="t18191201-94-person1037" type="given" value="BARTHOLOMEW"></interp>
