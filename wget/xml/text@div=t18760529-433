               <join result="criminalCharge" id="t18760529-433-charge-2" targOrder="Y" targets="def2-433-18760529 t18760529-433-offence-1 t18760529-433-verdict-1"></join>
               <join result="criminalCharge" id="t18760529-433-charge-3" targOrder="Y" targets="def1-433-18760529 t18760529-433-offence-2 t18760529-433-verdict-1"></join>
