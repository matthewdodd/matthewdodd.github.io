                        <interp inst="t18681214-96-verdict-1" type="verdictSubcategory" value="no_subcategory"></interp>GUILTY</rs>
                  </hi>—<hi rend="italic">
                     <rs id="t18681214-96-punishment-12" type="punishmentDescription">
