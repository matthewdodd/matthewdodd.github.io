                     <join result="persNameOccupation" targOrder="Y" targets="t18330214-212-victim1883 t18330214-212-viclabel1191"></join>, in her dwelling-house
            </rs>; and 
               
                  <persName id="t18330214-212-defend1885" type="defendantName">
                     JOHN 
                     BARNES
                  <interp inst="t18330214-212-defend1885" type="surname" value="BARNES"></interp>
