                        <interp inst="t18740504-363-verdict-2" type="verdictSubcategory" value="no_subcategory"></interp>GUILTY</rs>
                  </hi> 
                  <hi rend="italic">
                     <rs id="t18740504-363-punishment-31" type="punishmentDescription">
                        <interp inst="t18740504-363-punishment-31" type="punishmentCategory" value="imprison"></interp>
