            , the <rs id="t18190113-76-viclabel439" type="occupation">elder</rs>
                  <join result="persNameOccupation" targOrder="Y" targets="t18190113-76-victim731 t18190113-76-viclabel439"></join>, and 
               
                  <persName id="t18190113-76-victim733" type="victimName">
                     James 
                     Colville
                  <interp inst="t18190113-76-victim733" type="surname" value="Colville"></interp>
