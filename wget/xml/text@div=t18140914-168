            , <rs id="t18140914-168-viclabel946" type="occupation">widow</rs>
                  <join result="persNameOccupation" targOrder="Y" targets="t18140914-168-victim1436 t18140914-168-viclabel946"></join>, and 
               <persName id="t18140914-168-victim1437" type="victimName">
                  Thomas 
                  Gibson
               <interp inst="t18140914-168-victim1437" type="surname" value="Gibson"></interp>
                     <interp inst="t18140914-168-victim1437" type="given" value="Thomas"></interp>
                     <interp inst="t18140914-168-victim1437" type="gender" value="male"></interp>
