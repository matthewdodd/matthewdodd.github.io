               <p>73. 
               
                  <persName id="t17370420-62-defend486" type="defendantName">
                     Joseph 
                     Clapton 
                  <interp inst="t17370420-62-defend486" type="surname" value="Clapton"></interp>
                     <interp inst="t17370420-62-defend486" type="given" value="Joseph"></interp>
                     <interp inst="t17370420-62-defend486" type="gender" value="male"></interp>
                  </persName>
                
            , was indicted for <rs id="t17370420-62-off307" type="offenceDescription">
                     <interp inst="t17370420-62-off307" type="offenceCategory" value="theft"></interp>
                     <interp inst="t17370420-62-off307" type="offenceSubcategory" value="burglary"></interp>
