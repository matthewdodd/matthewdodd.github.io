                     <interp inst="t17360908-58-defend350" type="gender" value="female"></interp>
                  </persName>
                
             and 
               
                  <persName id="t17360908-58-defend352" type="defendantName">
                     Elizabeth 
                     Studder 
                  <interp inst="t17360908-58-defend352" type="surname" value="Studder"></interp>
