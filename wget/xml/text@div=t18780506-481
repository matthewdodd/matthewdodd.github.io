                        <interp inst="t18780506-481-verdict-1" type="verdictSubcategory" value="no_subcategory"></interp>GUILTY</rs>
                  </hi>.—<hi rend="italic">
                     <rs id="t18780506-481-punishment-13" type="punishmentDescription">
                        <interp inst="t18780506-481-punishment-13" type="punishmentCategory" value="imprison"></interp>
