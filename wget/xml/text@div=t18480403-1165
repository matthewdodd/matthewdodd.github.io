                        <interp inst="def5-1165-18480403" type="surname" value="DUFFY"></interp>
                        <interp inst="def5-1165-18480403" type="given" value="DAVID ANTHONY"></interp> DAVID ANTHONY DUFFY</persName>,<persName id="def6-1165-18480403" type="defendantName">
                        <interp inst="def6-1165-18480403" type="gender" value="male"></interp>
