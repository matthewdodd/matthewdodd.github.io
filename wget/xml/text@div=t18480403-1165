                        <interp inst="def6-1165-18480403" type="surname" value="LOWERS"></interp>
                        <interp inst="def6-1165-18480403" type="given" value="GEORGE"></interp> GEORGE LOWERS</persName>,<persName id="def7-1165-18480403" type="defendantName">
                        <interp inst="def7-1165-18480403" type="gender" value="male"></interp>
