                        <join result="offenceVictim" targOrder="Y" targets="t18460511-1147-offence-1 t18460511-name-850"></join>Scipio Robinson</persName>, her master; and</rs>
                  <persName id="def2-1147-18460511" type="defendantName">
                     <interp inst="def2-1147-18460511" type="gender" value="female"></interp>
                     <interp inst="def2-1147-18460511" type="age" value="28"></interp>
