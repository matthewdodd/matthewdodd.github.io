                     <interp inst="t17930109-1-person62" type="gender" value="female"></interp>
                  </persName>
            , <rs id="t17930109-1-verdict7" type="verdictDescription">
                     <interp inst="t17930109-1-verdict7" type="verdictCategory" value="guilty"></interp>
               GUILTY
            </rs>. <rs id="t17930109-1-punish8" type="punishmentDescription">
