               <persName id="t18300415-250-person2182">
                  John 
                  Ambler
               <interp inst="t18300415-250-person2182" type="surname" value="Ambler"></interp>
                     <interp inst="t18300415-250-person2182" type="given" value="John"></interp>
                     <interp inst="t18300415-250-person2182" type="gender" value="male"></interp>
