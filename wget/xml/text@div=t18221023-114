                     <interp inst="t18221023-114-off610" type="offenceCategory" value="theft"></interp>
                     <interp inst="t18221023-114-off610" type="offenceSubcategory" value="grandLarceny"></interp>
               stealing, on the <rs id="t18221023-114-cd611" type="crimeDate">9th of October</rs>
                     <join result="offenceCrimeDate" targOrder="Y" targets="t18221023-114-off610 t18221023-114-cd611"></join>, seven sovereigns, value 7 l.
            </rs>, the monies of 
               
                  <persName id="t18221023-114-victim1172" type="victimName">
                     Margaret 
                     Stokes
                  <interp inst="t18221023-114-victim1172" type="surname" value="Stokes"></interp>
                     <interp inst="t18221023-114-victim1172" type="given" value="Margaret"></interp>
                     <interp inst="t18221023-114-victim1172" type="gender" value="female"></interp>
                     <join result="offenceVictim" targOrder="Y" targets="t18221023-114-off610 t18221023-114-victim1172"></join>
