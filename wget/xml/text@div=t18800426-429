                     <hi rend="largeCaps">JAMES STENT</hi> (32)</persName>
                  <rs id="t18800426-429-offence-1" type="offenceDescription">
                     <interp inst="t18800426-429-offence-1" type="offenceCategory" value="sexual"></interp>
                     <interp inst="t18800426-429-offence-1" type="offenceSubcategory" value="bigamy"></interp>, Feloniously marrying <persName id="t18800426-name-445" type="victimName">
                        <interp inst="t18800426-name-445" type="gender" value="female"></interp>
