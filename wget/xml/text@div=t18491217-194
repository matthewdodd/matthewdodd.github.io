                  <hi rend="largeCaps">WOODWARD</hi> pleaded <hi rend="smallCaps"> 
                     <rs id="t18491217-194-verdict-1" type="verdictDescription">
                        <interp inst="t18491217-194-verdict-1" type="verdictCategory" value="guilty"></interp>
