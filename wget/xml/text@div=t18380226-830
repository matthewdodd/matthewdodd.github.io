                     <interp inst="t18380226-830-offence-1" type="offenceSubcategory" value="simpleLarceny"></interp> for stealing, on the <rs id="t18380226-cd-147" type="crimeDate">
                        <join result="offenceCrimeDate" targOrder="Y" targets="t18380226-830-offence-1 t18380226-cd-147"></join>13th of February</rs>, 2 sheets, value 3<hi rend="italic">s</hi>.; and 1 bonnet, value 2<hi rend="italic">s</hi>.; the goods of <persName id="t18380226-name-1027" type="victimName">
                        <interp inst="t18380226-name-1027" type="gender" value="male"></interp>
                        <interp inst="t18380226-name-1027" type="surname" value="ROACH"></interp>
                        <interp inst="t18380226-name-1027" type="given" value="JAMES"></interp>
                        <join result="offenceVictim" targOrder="Y" targets="t18380226-830-offence-1 t18380226-name-1027"></join>James Roach</persName>: 1 gown, value 3<hi rend="italic">s</hi>.; 1 shift, value 2<hi rend="italic">s</hi>.; and 5 caps, value 2<hi rend="italic">s</hi>.; the goods of <persName id="t18380226-name-1028" type="victimName">
                        <interp inst="t18380226-name-1028" type="gender" value="male"></interp>
