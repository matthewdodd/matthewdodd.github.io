                     <interp inst="t17280501-30-off156" type="offenceCategory" value="violentTheft"></interp>
                     <interp inst="t17280501-30-off156" type="offenceSubcategory" value="highwayRobbery"></interp>
               assaulting <rs id="t17280501-30-viclabel157" type="occupation">Mr.</rs>
                     <join result="persNameOccupation" targOrder="Y" targets="t17280501-30-victim221 t17280501-30-viclabel157"></join> 
                  
                     
