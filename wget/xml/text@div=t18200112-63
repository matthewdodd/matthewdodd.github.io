                     <interp inst="t18200112-63-off358" type="offenceCategory" value="violentTheft"></interp>
                     <interp inst="t18200112-63-off358" type="offenceSubcategory" value="highwayRobbery"></interp>
               feloniously assaulting 
                     
                        <persName id="t18200112-63-victim671" type="victimName">
                           Jane 
                           Smith
                        <interp inst="t18200112-63-victim671" type="surname" value="Smith"></interp>
