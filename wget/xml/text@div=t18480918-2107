                        <interp inst="t18480918-name-249" type="surname" value="BROOKS"></interp>
                        <interp inst="t18480918-name-249" type="given" value="JAMES"></interp>
                        <join result="offenceVictim" targOrder="Y" targets="t18480918-2107-offence-1 t18480918-name-249"></join>James Brooks</persName>, her master.</rs>
