            , and 
               
                  <persName id="t17830723-95-victim972" type="victimName">
                     Henry 
                     Newman
                  <interp inst="t17830723-95-victim972" type="surname" value="Newman"></interp>
                     <interp inst="t17830723-95-victim972" type="given" value="Henry"></interp>
                     <interp inst="t17830723-95-victim972" type="gender" value="male"></interp>
