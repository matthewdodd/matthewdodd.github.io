                     <interp inst="t18090920-57-victim560" type="gender" value="male"></interp>
                  </persName>
               
            , 
               
                  <persName id="t18090920-57-victim562" type="victimName">
                     Robert 
                     Milton
                  <interp inst="t18090920-57-victim562" type="surname" value="Milton"></interp>
                     <interp inst="t18090920-57-victim562" type="given" value="Robert"></interp>
                     <interp inst="t18090920-57-victim562" type="gender" value="male"></interp>
                  </persName>
               
            , and 
               
                  <persName id="t18090920-57-victim564" type="victimName">
                     Joseph 
                     Braithwaite
                  <interp inst="t18090920-57-victim564" type="surname" value="Braithwaite"></interp>
