                  </persName>
                  <rs id="t18520202-238-offence-1" type="offenceDescription">
                     <interp inst="t18520202-238-offence-1" type="offenceCategory" value="kill"></interp>
                     <interp inst="t18520202-238-offence-1" type="offenceSubcategory" value="manslaughter"></interp>, feloniously killing and slaying <persName id="t18520202-name-176" type="victimName">
                        <interp inst="t18520202-name-176" type="gender" value="male"></interp>
                        <interp inst="t18520202-name-176" type="surname" value="MANSELL"></interp>
