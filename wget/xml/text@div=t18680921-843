                        <interp inst="t18680921-843-verdict-1" type="verdictSubcategory" value="no_subcategory"></interp>GUILTY</rs>
                  </hi>.—<hi rend="italic">
                     <rs id="t18680921-843-punishment-70" type="punishmentDescription">
                        <interp inst="t18680921-843-punishment-70" type="punishmentCategory" value="imprison"></interp>
