                     <interp inst="t18030706-38-defend209" type="age" value="20"></interp>
                  </persName>
             and 
               <persName id="t18030706-38-defend210" type="defendantName">
                  CHARLOTTE 
                  WOOD
               <interp inst="t18030706-38-defend210" type="surname" value="WOOD"></interp>
                     <interp inst="t18030706-38-defend210" type="given" value="CHARLOTTE"></interp>
