                        <interp inst="t18761023-475-verdict-1" type="verdictSubcategory" value="no_subcategory"></interp>GUILTY</rs>
                  </hi>—<hi rend="italic">
                     <rs id="t18761023-475-punishment-7" type="punishmentDescription">
