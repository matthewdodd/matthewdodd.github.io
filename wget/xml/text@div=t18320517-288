                     <interp inst="t18320517-288-defend2065" type="gender" value="male"></interp>
                  </persName>
               
             and 
               <persName id="t18320517-288-defend2066" type="defendantName">
                  SARAH 
                  BAYLEY
               <interp inst="t18320517-288-defend2066" type="surname" value="BAYLEY"></interp>
                     <interp inst="t18320517-288-defend2066" type="given" value="SARAH"></interp>
                     <interp inst="t18320517-288-defend2066" type="gender" value="female"></interp>
                  </persName>
             were indicted for <rs id="t18320517-288-off1629" type="offenceDescription">
                     <interp inst="t18320517-288-off1629" type="offenceCategory" value="violentTheft"></interp>
                     <interp inst="t18320517-288-off1629" type="offenceSubcategory" value="robbery"></interp>
               feloniously assaulting 
                     <persName id="t18320517-288-victim2067" type="victimName">
                        Guy 
                        Pickering
                     <interp inst="t18320517-288-victim2067" type="surname" value="Pickering"></interp>
                        <interp inst="t18320517-288-victim2067" type="given" value="Guy"></interp>
                        <interp inst="t18320517-288-victim2067" type="gender" value="male"></interp>
                        <join result="offenceVictim" targOrder="Y" targets="t18320517-288-off1629 t18320517-288-victim2067"></join>
