            , 
               <persName id="t17871212-75-defend681" type="defendantName">
                  JOHN 
                  READING
               <interp inst="t17871212-75-defend681" type="surname" value="READING"></interp>
                     <interp inst="t17871212-75-defend681" type="given" value="JOHN"></interp>
