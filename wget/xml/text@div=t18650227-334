               <join result="criminalCharge" id="t18650227-334-charge-3" targOrder="Y" targets="def3-334-18650227 t18650227-334-offence-1 t18650227-334-verdict-1"></join>
               <join result="criminalCharge" id="t18650227-334-charge-4" targOrder="Y" targets="def4-334-18650227 t18650227-334-offence-1 t18650227-334-verdict-1"></join>
