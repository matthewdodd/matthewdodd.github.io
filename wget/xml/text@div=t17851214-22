               <p>(
               <persName id="t17851214-22-person373">
                  DAVID
                   LEVY
               <interp inst="t17851214-22-person373" type="surname" value="LEVY"></interp>
                     <interp inst="t17851214-22-person373" type="given" value="DAVID"></interp>
                     <interp inst="t17851214-22-person373" type="gender" value="male"></interp>
