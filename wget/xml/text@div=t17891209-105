                     <interp inst="t17891209-105-person946" type="gender" value="female"></interp>
                  </persName>
            , 
               <persName id="t17891209-105-person947">
                  ELIZABETH
                   HUGHES
               <interp inst="t17891209-105-person947" type="surname" value="HUGHES"></interp>
