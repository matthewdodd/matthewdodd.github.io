                     <interp inst="t17310714-56-off288" type="offenceCategory" value="violentTheft"></interp>
                     <interp inst="t17310714-56-off288" type="offenceSubcategory" value="highwayRobbery"></interp>
               assault 
                     
                        <persName id="t17310714-56-victim364" type="victimName">
                           Edward 
                           Allen 
