                     <interp inst="t18290611-202-off1060" type="offenceCategory" value="theft"></interp>
                     <interp inst="t18290611-202-off1060" type="offenceSubcategory" value="housebreaking"></interp>
               feloniously breaking and entering the dwelling-house of 
                     <persName id="t18290611-202-victim1537" type="victimName">
                        John 
                        Reed
                     <interp inst="t18290611-202-victim1537" type="surname" value="Reed"></interp>
                        <interp inst="t18290611-202-victim1537" type="given" value="John"></interp>
                        <interp inst="t18290611-202-victim1537" type="gender" value="male"></interp>
                        <join result="offenceVictim" targOrder="Y" targets="t18290611-202-off1060 t18290611-202-victim1537"></join>
