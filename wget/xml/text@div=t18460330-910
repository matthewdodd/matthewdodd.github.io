                     <interp inst="t18460330-910-verdict-1" type="verdictSubcategory" value="no_subcategory"></interp>
                     <hi rend="smallCaps">GUILTY</hi> 
                     <hi rend="italic">of uttering.</hi>
                  </rs>—
<hi rend="italic">
                     <rs id="t18460330-910-punishment-89" type="punishmentDescription">
