                     <interp inst="def1-927-18530815" type="given" value="JKAN AUGUSTE"></interp>
                     <hi rend="largeCaps">JKAN AUGUSTE LABAT</hi>
                  </persName> and <persName id="def2-927-18530815" type="defendantName">
                     <interp inst="def2-927-18530815" type="gender" value="male"></interp>
                     <interp inst="def2-927-18530815" type="surname" value="SAUGE"></interp>
