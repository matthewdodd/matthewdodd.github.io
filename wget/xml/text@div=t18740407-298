                  <hi rend="smallCaps">
                     <persName id="t18740407-name-363" type="witnessName">
                        <interp inst="t18740407-name-363" type="gender" value="indeterminate"></interp>
                        <interp inst="t18740407-name-363" type="surname" value="COLE"></interp>
                        <interp inst="t18740407-name-363" type="given" value="THORNE"></interp>MR. THORNE COLE</persName>
