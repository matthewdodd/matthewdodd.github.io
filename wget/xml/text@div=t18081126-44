               <p>717. 
               
                  <persName id="t18081126-44-defend398" type="defendantName">
                     ELIZABETH 
                     BICKERSTAFF
                  <interp inst="t18081126-44-defend398" type="surname" value="BICKERSTAFF"></interp>
                     <interp inst="t18081126-44-defend398" type="given" value="ELIZABETH"></interp>
