               <join result="criminalCharge" id="t18610505-431-charge-3" targOrder="Y" targets="def3-431-18610505 t18610505-431-offence-1 t18610505-431-verdict-1"></join>
               <join result="criminalCharge" id="t18610505-431-charge-4" targOrder="Y" targets="def4-431-18610505 t18610505-431-offence-1 t18610505-431-verdict-2"></join>
               <join result="criminalCharge" id="t18610505-431-charge-5" targOrder="Y" targets="def5-431-18610505 t18610505-431-offence-1 t18610505-431-verdict-2"></join>
