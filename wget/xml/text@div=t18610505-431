                        <interp inst="t18610505-431-verdict-1" type="verdictSubcategory" value="no_subcategory"></interp>GUILTY</rs>
                  </hi>.—<hi rend="italic">
                     <rs id="t18610505-431-punishment-24" type="punishmentDescription">
                        <interp inst="t18610505-431-punishment-24" type="punishmentCategory" value="imprison"></interp>
