                        <interp inst="t18610408-321-verdict-1" type="verdictSubcategory" value="no_subcategory"></interp>GUILTY</rs>
                  </hi>.—<hi rend="italic">
                     <rs id="t18610408-321-punishment-37" type="punishmentDescription">
                        <interp inst="t18610408-321-punishment-37" type="punishmentCategory" value="imprison"></interp>
