                        <interp inst="t18660226-258-verdict-1" type="verdictSubcategory" value="no_subcategory"></interp>GUILTY</rs>
                  </hi>.—<hi rend="italic">
                     <rs id="t18660226-258-punishment-11" type="punishmentDescription">
                        <interp inst="t18660226-258-punishment-11" type="punishmentCategory" value="imprison"></interp>
