                  <persName id="t18300114-251-victim1704" type="victimName">
                     JOHN 
                     CROSS
                  <interp inst="t18300114-251-victim1704" type="surname" value="CROSS"></interp>
                     <interp inst="t18300114-251-victim1704" type="given" value="JOHN"></interp>
