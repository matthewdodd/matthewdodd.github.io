               <p>320. (L.) 
               
                  <persName id="t17740413-82-defend690" type="defendantName">
                     RICHARD
                      BURNETT
                  <interp inst="t17740413-82-defend690" type="surname" value="BURNETT"></interp>
                     <interp inst="t17740413-82-defend690" type="given" value="RICHARD"></interp>
                     <interp inst="t17740413-82-defend690" type="gender" value="male"></interp>
