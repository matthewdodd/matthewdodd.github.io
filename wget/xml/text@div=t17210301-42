                     <interp inst="t17210301-42-off235" type="offenceCategory" value="theft"></interp>
                     <interp inst="t17210301-42-off235" type="offenceSubcategory" value="burglary"></interp>
               breaking the House of 
                     
                        <persName id="t17210301-42-victim248" type="victimName">
                           John 
                           Blackburn 
                        <interp inst="t17210301-42-victim248" type="surname" value="Blackburn"></interp>
                        <interp inst="t17210301-42-victim248" type="given" value="John"></interp>
                        <interp inst="t17210301-42-victim248" type="gender" value="male"></interp>
                        <join result="offenceVictim" targOrder="Y" targets="t17210301-42-off235 t17210301-42-victim248"></join>
