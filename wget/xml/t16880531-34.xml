               <p>
            
                  <persName id="t16880531-34-defend163" type="defendantName">
                  
                     
                           <persName id="t16880531-34-defend165" type="defendantName">
                              Isaac 
                              Browne
                           <interp inst="t16880531-34-defend165" type="surname" value="Browne"></interp>
                        <interp inst="t16880531-34-defend165" type="given" value="Isaac"></interp>
                        <interp inst="t16880531-34-defend165" type="gender" value="male"></interp>
                     </persName>
                        
                      and Alice his <rs id="t16880531-34-deflabel145" type="occupation">Wife</rs>
                     <join result="persNameOccupation" targOrder="Y" targets="t16880531-34-defend163 t16880531-34-deflabel145"></join>
                     <interp inst="t16880531-34-defend163" type="surname" value="Browne"></interp>
