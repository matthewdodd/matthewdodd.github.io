                        <interp inst="t18670708-635-verdict-1" type="verdictSubcategory" value="no_subcategory"></interp>GUILTY</rs>.</hi>—<hi rend="italic">

                     <rs id="t18670708-635-punishment-4" type="punishmentDescription">
                        <interp inst="t18670708-635-punishment-4" type="punishmentCategory" value="imprison"></interp>
