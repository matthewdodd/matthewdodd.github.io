                  ; 6 s. 6 d., in monies numbered, one 5 l. and eight 1 l. Bank notes, the property, of 
                     <persName id="t18201206-62-person636">
                        Thomas 
                        Davidson
                     <interp inst="t18201206-62-person636" type="surname" value="Davidson"></interp>
                        <interp inst="t18201206-62-person636" type="given" value="Thomas"></interp>
                        <interp inst="t18201206-62-person636" type="gender" value="male"></interp>
