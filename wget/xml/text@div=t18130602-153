                  <persName id="t18130602-153-person1290">
                  WILLIAM 
                  LINDUS
               <interp inst="t18130602-153-person1290" type="surname" value="LINDUS"></interp>
                     <interp inst="t18130602-153-person1290" type="given" value="WILLIAM"></interp>
                     <interp inst="t18130602-153-person1290" type="gender" value="male"></interp>
