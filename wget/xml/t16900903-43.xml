                     <interp inst="t16900903-43-off188" type="offenceSubcategory" value="grandLarceny"></interp>
               robbing one 
                     
                        <persName id="t16900903-43-victim222" type="victimName">
                           Francis 
                           Smith
                        <interp inst="t16900903-43-victim222" type="surname" value="Smith"></interp>
