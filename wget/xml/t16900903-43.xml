                     <interp inst="t16900903-43-defend220" type="gender" value="female"></interp>
                  </persName>
               
            , a <rs id="t16900903-43-deflabel187" type="occupation">Girl</rs>
                  <join result="persNameOccupation" targOrder="Y" targets="t16900903-43-defend220 t16900903-43-deflabel187"></join>, accused for <rs id="t16900903-43-off188" type="offenceDescription">
