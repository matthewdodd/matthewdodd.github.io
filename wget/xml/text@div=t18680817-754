                        <interp inst="def1-754-18680817" type="given" value="SAMUEL"></interp>SAMUEL ISRAEL</persName>,<persName id="def2-754-18680817" type="defendantName">
                        <interp inst="def2-754-18680817" type="gender" value="male"></interp>
                        <interp inst="def2-754-18680817" type="surname" value="BARNES"></interp>
                        <interp inst="def2-754-18680817" type="given" value="WILLIAM"></interp> WILLIAM BARNES</persName>
                  </hi>, and <persName id="def3-754-18680817" type="defendantName">
                     <interp inst="def3-754-18680817" type="gender" value="male"></interp>
