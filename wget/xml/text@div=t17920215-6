             and 
               
                  <persName id="t17920215-6-defend121" type="defendantName">
                     JAMES 
                     GILLTHORPE
                  <interp inst="t17920215-6-defend121" type="surname" value="GILLTHORPE"></interp>
                     <interp inst="t17920215-6-defend121" type="given" value="JAMES"></interp>
