               <p>
                  <xptr type="pageFacsimile" doc="177707020035"></xptr>
            
                  <persName id="t17770702-56-person464">
                  MARY
                   AARONS
               <interp inst="t17770702-56-person464" type="surname" value="AARONS"></interp>
                     <interp inst="t17770702-56-person464" type="given" value="MARY"></interp>
