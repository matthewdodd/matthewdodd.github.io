            , and 
               
                  <persName id="t17860719-8-victim138" type="victimName">
                     Thomas 
                     Higgins
                  <interp inst="t17860719-8-victim138" type="surname" value="Higgins"></interp>
                     <interp inst="t17860719-8-victim138" type="given" value="Thomas"></interp>
