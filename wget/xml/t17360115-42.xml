                        </persName>
                         
                        <interp inst="t17360115-42-person307" type="surname" value="Jordan"></interp>
                        <interp inst="t17360115-42-person307" type="given" value="William"></interp>
                        <interp inst="t17360115-42-person307" type="gender" value="male"></interp>
                     </persName>
                   and Elizabeth
                   his 
                  <rs id="t17360115-42-deflabel231" type="occupation">Wife</rs>
                     <join result="persNameOccupation" targOrder="Y" targets="t17360115-42-defend306 t17360115-42-deflabel231"></join>
                     <interp inst="t17360115-42-defend306" type="surname" value="Jordan"></interp>
