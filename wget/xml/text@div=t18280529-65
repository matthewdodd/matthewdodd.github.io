               <persName id="t18280529-65-defend459" type="defendantName">
                  THOMAS 
                  LACK
               <interp inst="t18280529-65-defend459" type="surname" value="LACK"></interp>
                     <interp inst="t18280529-65-defend459" type="given" value="THOMAS"></interp>
