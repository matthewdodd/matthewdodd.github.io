                     <interp inst="t17800913-57-defend561" type="given" value="ABRAHAM"></interp>
                     <interp inst="t17800913-57-defend561" type="gender" value="male"></interp>
                  </persName>
               
             and 
               
                  <persName id="t17800913-57-defend563" type="defendantName">
                     WILLIAM 
                     NEWTON
                  <interp inst="t17800913-57-defend563" type="surname" value="NEWTON"></interp>
                     <interp inst="t17800913-57-defend563" type="given" value="WILLIAM"></interp>
                     <interp inst="t17800913-57-defend563" type="gender" value="male"></interp>
                  </persName>
               
             were indicted for <rs id="t17800913-57-off337" type="offenceDescription">
                     <interp inst="t17800913-57-off337" type="offenceCategory" value="violentTheft"></interp>
                     <interp inst="t17800913-57-off337" type="offenceSubcategory" value="robbery"></interp>
               feloniously assaulting 
                     
                        <persName id="t17800913-57-victim565" type="victimName">
                           James 
                           Watts
                        <interp inst="t17800913-57-victim565" type="surname" value="Watts"></interp>
