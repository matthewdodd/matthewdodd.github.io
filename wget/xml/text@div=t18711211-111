                        <interp inst="t18711211-111-verdict-1" type="verdictSubcategory" value="no_subcategory"></interp>GUILTY</rs>
                  </hi>—<hi rend="italic">
                     <rs id="t18711211-111-punishment-39" type="punishmentDescription">
                        <interp inst="t18711211-111-punishment-39" type="punishmentCategory" value="imprison"></interp>
