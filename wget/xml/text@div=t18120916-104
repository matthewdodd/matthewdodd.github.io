               <p>727. 
               
                  <persName id="t18120916-104-defend861" type="defendantName">
                     THOMAS 
                     HITCHINSON
                  <interp inst="t18120916-104-defend861" type="surname" value="HITCHINSON"></interp>
                     <interp inst="t18120916-104-defend861" type="given" value="THOMAS"></interp>
                     <interp inst="t18120916-104-defend861" type="gender" value="male"></interp>
