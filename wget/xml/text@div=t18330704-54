               <p>1076. 
               
                  <persName id="t18330704-54-defend608" type="defendantName">
                     WILLIAM 
                     WINSPER
                  <interp inst="t18330704-54-defend608" type="surname" value="WINSPER"></interp>
                     <interp inst="t18330704-54-defend608" type="given" value="WILLIAM"></interp>
