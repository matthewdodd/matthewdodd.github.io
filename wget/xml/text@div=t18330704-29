                  <persName id="t18330704-29-defend369" type="defendantName">
                     FREDERICK 
                     BISHOP
                  <interp inst="t18330704-29-defend369" type="surname" value="BISHOP"></interp>
                     <interp inst="t18330704-29-defend369" type="given" value="FREDERICK"></interp>
