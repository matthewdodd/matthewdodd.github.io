                     <hi rend="largeCaps">RICHARD PALMER</hi> (25)</persName> and <persName id="def2-572-18670610" type="defendantName">
                     <interp inst="def2-572-18670610" type="gender" value="male"></interp>
                     <interp inst="def2-572-18670610" type="age" value="20"></interp>
