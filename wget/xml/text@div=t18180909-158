             and 
               
                  <persName id="t18180909-158-defend1244" type="defendantName">
                     CHARLES 
                     CARPENTER
                  <interp inst="t18180909-158-defend1244" type="surname" value="CARPENTER"></interp>
                     <interp inst="t18180909-158-defend1244" type="given" value="CHARLES"></interp>
                     <interp inst="t18180909-158-defend1244" type="gender" value="male"></interp>
                     <interp inst="t18180909-158-defend1244" type="age" value="20"></interp>
                  </persName>
               
             were indicted for <rs id="t18180909-158-off859" type="offenceDescription">
                     <interp inst="t18180909-158-off859" type="offenceCategory" value="theft"></interp>
                     <interp inst="t18180909-158-off859" type="offenceSubcategory" value="theftFromPlace"></interp>
               stealing, on the <rs id="t18180909-158-cd860" type="crimeDate">27th of August</rs>
                     <join result="offenceCrimeDate" targOrder="Y" targets="t18180909-158-off859 t18180909-158-cd860"></join>, 100 lbs. of lead, value 30s., the property of 
                     
                        <persName id="t18180909-158-victim1246" type="victimName">
                           Thomas 
                           Wallis
                        <interp inst="t18180909-158-victim1246" type="surname" value="Wallis"></interp>
