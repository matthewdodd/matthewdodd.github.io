                     <interp inst="t18440205-630-offence-1" type="offenceCategory" value="theft"></interp>
                     <interp inst="t18440205-630-offence-1" type="offenceSubcategory" value="simpleLarceny"></interp> for stealing 1 coat, value 8<hi rend="italic">s</hi>.; 1 violin, 8<hi rend="italic">s</hi>.; and 1 violin bow, 6<hi rend="italic">d</hi>.; the goods of <persName id="t18440205-name-678" type="victimName">
                        <interp inst="t18440205-name-678" type="gender" value="male"></interp>
                        <interp inst="t18440205-name-678" type="surname" value="JARMAN"></interp>
