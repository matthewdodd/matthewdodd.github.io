                        <interp inst="t18590919-867-verdict-1" type="verdictSubcategory" value="no_subcategory"></interp>GUILTY</rs>
                  </hi>.—<hi rend="italic">
                     <rs id="t18590919-867-punishment-63" type="punishmentDescription">
                        <interp inst="t18590919-867-punishment-63" type="punishmentCategory" value="imprison"></interp>
