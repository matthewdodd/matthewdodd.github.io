               <p>530. 
               
                  <persName id="t18220220-207-defend1846" type="defendantName">
                     ELIZA 
                     BULLARD
                  <interp inst="t18220220-207-defend1846" type="surname" value="BULLARD"></interp>
                     <interp inst="t18220220-207-defend1846" type="given" value="ELIZA"></interp>
