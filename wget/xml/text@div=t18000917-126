                  <persName id="t18000917-126-person1189">
                  WILLIAM 
                  HODGE
               <interp inst="t18000917-126-person1189" type="surname" value="HODGE"></interp>
                     <interp inst="t18000917-126-person1189" type="given" value="WILLIAM"></interp>
                     <interp inst="t18000917-126-person1189" type="gender" value="male"></interp>
