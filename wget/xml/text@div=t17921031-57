               <p>503. 
               
                  <persName id="t17921031-57-defend590" type="defendantName">
                     JAMES 
                     JONES
                  <interp inst="t17921031-57-defend590" type="surname" value="JONES"></interp>
                     <interp inst="t17921031-57-defend590" type="given" value="JAMES"></interp>
