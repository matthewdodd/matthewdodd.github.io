            , 
               
                  <persName id="t17731208-1-defend59" type="defendantName">
                     WILLIAM
                      DAVIS, otherwise <rs id="t17731208-1-alias-1" type="alias">
                        <join result="nameAlias" targOrder="Y" targets="t17731208-1-defend59 t17731208-1-alias-1"></join>SCARLET</rs>
                     <interp inst="t17731208-1-defend59" type="surname" value="DAVIS"></interp>
