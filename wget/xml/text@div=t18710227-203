                        <interp inst="t18710227-203-verdict-1" type="verdictSubcategory" value="no_subcategory"></interp>GUILTY</rs>
                  </hi>—<hi rend="italic">
                     <rs id="t18710227-203-punishment-13" type="punishmentDescription">
                        <interp inst="t18710227-203-punishment-13" type="punishmentCategory" value="imprison"></interp>
