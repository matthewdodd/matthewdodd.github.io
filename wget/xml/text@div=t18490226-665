                  <hi rend="smallCaps">
                     <persName id="t18490226-name-317" type="witnessName">
                        <interp inst="t18490226-name-317" type="gender" value="male"></interp>
                        <interp inst="t18490226-name-317" type="surname" value="ROWLEY"></interp>
                        <interp inst="t18490226-name-317" type="given" value="THOMAS"></interp>THOMAS ROWLEY</persName>
