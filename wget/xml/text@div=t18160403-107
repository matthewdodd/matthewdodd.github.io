                     <interp inst="t18160403-107-defend866" type="age" value="24"></interp>
                  </persName>
            , was indicted for <rs id="t18160403-107-off546" type="offenceDescription">
                     <interp inst="t18160403-107-off546" type="offenceCategory" value="violentTheft"></interp>
                     <interp inst="t18160403-107-off546" type="offenceSubcategory" value="highwayRobbery"></interp>
               feloniously assaulting 
                     
                        <persName id="t18160403-107-victim869" type="victimName">
                           Henry 
                           Dewdney
                        <interp inst="t18160403-107-victim869" type="surname" value="Dewdney"></interp>
                        <interp inst="t18160403-107-victim869" type="given" value="Henry"></interp>
