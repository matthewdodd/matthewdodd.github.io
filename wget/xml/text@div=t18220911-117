                  <persName id="t18220911-117-person1158">
                  THOMAS 
                  POTTER
               <interp inst="t18220911-117-person1158" type="surname" value="POTTER"></interp>
                     <interp inst="t18220911-117-person1158" type="given" value="THOMAS"></interp>
                     <interp inst="t18220911-117-person1158" type="gender" value="male"></interp>
