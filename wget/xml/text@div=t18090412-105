                  <persName id="t18090412-105-person936">
                  THOMAS 
                  GRANGER
               <interp inst="t18090412-105-person936" type="surname" value="GRANGER"></interp>
                     <interp inst="t18090412-105-person936" type="given" value="THOMAS"></interp>
                     <interp inst="t18090412-105-person936" type="gender" value="male"></interp>
