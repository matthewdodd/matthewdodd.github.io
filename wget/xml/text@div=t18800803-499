                     <hi rend="largeCaps">MARY MARTHA WOOD</hi> (21)</persName>, and <persName id="def6-499-18800803" type="defendantName">
                     <interp inst="def6-499-18800803" type="gender" value="male"></interp>
                     <interp inst="def6-499-18800803" type="surname" value="NORRIS"></interp>
