               <p>
                  <hi rend="largeCaps">SCARBOROUGH</hi> 
                  <hi rend="italic">and</hi> 
                  <hi rend="largeCaps">FULLER</hi> 
                  <hi rend="smallCaps">
                     <rs id="t18730303-233-verdict-1" type="verdictDescription">
                        <interp inst="t18730303-233-verdict-1" type="verdictCategory" value="guilty"></interp>
                        <interp inst="t18730303-233-verdict-1" type="verdictSubcategory" value="pleadedGuilty"></interp>PLEADED GUILTY</rs>
                  </hi>.—<hi rend="italic">
                     <rs id="t18730303-233-punishment-14" type="punishmentDescription">
                        <interp inst="t18730303-233-punishment-14" type="punishmentCategory" value="imprison"></interp>
                        <interp inst="t18730303-233-punishment-14" type="punishmentSubcategory" value="no_subcategory"></interp>
                        <join result="defendantPunishment" targOrder="Y" targets="def1-233-18730303 t18730303-233-punishment-14"></join>
