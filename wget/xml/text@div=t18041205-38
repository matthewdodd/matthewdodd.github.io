            , and 
               
                  <persName id="t18041205-38-defend499" type="defendantName">
                     SAMUEL 
                     COOK
                  <interp inst="t18041205-38-defend499" type="surname" value="COOK"></interp>
                     <interp inst="t18041205-38-defend499" type="given" value="SAMUEL"></interp>
