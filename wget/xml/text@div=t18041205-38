                        <interp inst="t18041205-38-victim503" type="gender" value="male"></interp>
                     </persName>
                     
                  , and 
                     
                        <persName id="t18041205-38-victim505" type="victimName">
                           William 
                           Wilkins
                        <interp inst="t18041205-38-victim505" type="surname" value="Wilkins"></interp>
