                     <persName id="t17501205-61-person528">
                        Henry 
                        Norman 
                     <interp inst="t17501205-61-person528" type="surname" value="Norman"></interp>
                        <interp inst="t17501205-61-person528" type="given" value="Henry"></interp>
                        <interp inst="t17501205-61-person528" type="gender" value="male"></interp>
