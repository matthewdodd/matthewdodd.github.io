                     <interp inst="t17501205-61-off311" type="offenceSubcategory" value="theftFromPlace"></interp>
               stealing one cloth coat, the goods of 
                     
                        <persName id="t17501205-61-victim523" type="victimName">
                           William 
                           Eliot 
                        <interp inst="t17501205-61-victim523" type="surname" value="Eliot"></interp>
                        <interp inst="t17501205-61-victim523" type="given" value="William"></interp>
                        <interp inst="t17501205-61-victim523" type="gender" value="male"></interp>
                     </persName>
                      
                  , one cloth coat the goods of 
                     
                        <persName id="t17501205-61-victim525" type="victimName">
                           Michael 
                           Perrot 
                        <interp inst="t17501205-61-victim525" type="surname" value="Perrot"></interp>
                        <interp inst="t17501205-61-victim525" type="given" value="Michael"></interp>
                        <interp inst="t17501205-61-victim525" type="gender" value="male"></interp>
                     </persName>
                      
                  , one cloth coat and a pair of boots the goods of 
                     
                        <persName id="t17501205-61-victim527" type="victimName">
                           John 
                           Wilmot 
                        <interp inst="t17501205-61-victim527" type="surname" value="Wilmot"></interp>
                        <interp inst="t17501205-61-victim527" type="given" value="John"></interp>
                        <interp inst="t17501205-61-victim527" type="gender" value="male"></interp>
                     </persName>
                      
