               <p>735. 
               
                  <persName id="t18160710-59-defend448" type="defendantName">
                     WILLIAM 
                     NICHOLSON
                  <interp inst="t18160710-59-defend448" type="surname" value="NICHOLSON"></interp>
                     <interp inst="t18160710-59-defend448" type="given" value="WILLIAM"></interp>
