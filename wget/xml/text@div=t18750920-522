               <join result="criminalCharge" id="t18750920-522-charge-3" targOrder="Y" targets="def3-522-18750920 t18750920-522-offence-2 t18750920-522-verdict-2"></join>
               <join result="criminalCharge" id="t18750920-522-charge-4" targOrder="Y" targets="def2-522-18750920 t18750920-522-offence-3 t18750920-522-verdict-3"></join>
