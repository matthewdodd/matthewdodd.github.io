                     <join result="offenceCrimeDate" targOrder="Y" targets="t18210110-18-off106 t18210110-18-cd107"></join>, at <placeName id="t18210110-18-crimeloc108">All Saints, Poplar</placeName>
                     <interp inst="t18210110-18-crimeloc108" type="placeName" value="All Saints, Poplar"></interp>
