                  <persName id="t17831029-33-person558">
                  SOLOMON
                   HART
               <interp inst="t17831029-33-person558" type="surname" value="HART"></interp>
                     <interp inst="t17831029-33-person558" type="given" value="SOLOMON"></interp>
