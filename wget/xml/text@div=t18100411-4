                     <persName id="t18100411-4-person82">
                        Margaret 
                        Craig
                     <interp inst="t18100411-4-person82" type="surname" value="Craig"></interp>
                        <interp inst="t18100411-4-person82" type="given" value="Margaret"></interp>
