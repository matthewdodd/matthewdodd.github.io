                     <hi rend="largeCaps">WILLIAM KENNETT</hi> (39)</persName>, <hi rend="italic">and</hi> 
                  <persName id="def3-422-18770507" type="defendantName">
                     <interp inst="def3-422-18770507" type="gender" value="male"></interp>
