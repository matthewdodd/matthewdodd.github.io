             and 
               
                  <persName id="t18330214-41-defend500" type="defendantName">
                     WILLIAM 
                     DUKES
                  <interp inst="t18330214-41-defend500" type="surname" value="DUKES"></interp>
                     <interp inst="t18330214-41-defend500" type="given" value="WILLIAM"></interp>
