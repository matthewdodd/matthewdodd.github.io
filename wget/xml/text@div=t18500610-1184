                  <hi rend="largeCaps">EDWARD WRIGHT</hi> pleaded 
<rs id="t18500610-1184-verdict-1" type="verdictDescription">
                     <interp inst="t18500610-1184-verdict-1" type="verdictCategory" value="guilty"></interp>
                     <interp inst="t18500610-1184-verdict-1" type="verdictSubcategory" value="pleadedGuilty"></interp>
