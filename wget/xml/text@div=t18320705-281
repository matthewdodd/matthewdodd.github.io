             and 
               
                  <persName id="t18320705-281-defend2289" type="defendantName">
                     MARY ANN 
                     FRANKLIN
                  <interp inst="t18320705-281-defend2289" type="surname" value="FRANKLIN"></interp>
                     <interp inst="t18320705-281-defend2289" type="given" value="MARY ANN"></interp>
                     <interp inst="t18320705-281-defend2289" type="gender" value="female"></interp>
                  </persName>
               
             were indicted for <rs id="t18320705-281-off1476" type="offenceDescription">
                     <interp inst="t18320705-281-off1476" type="offenceCategory" value="theft"></interp>
                     <interp inst="t18320705-281-off1476" type="offenceSubcategory" value="simpleLarceny"></interp>
               stealing, on the <rs id="t18320705-281-cd1477" type="crimeDate">27th of June</rs>
                     <join result="offenceCrimeDate" targOrder="Y" targets="t18320705-281-off1476 t18320705-281-cd1477"></join>, 1 bed, value 1l.; 1 bolster, value 4s.; 2 pillows, value 4s.; 2 blankets, value 4s.; 1 quilt, value 2s.; 1 looking-glass, value 10s, and 1 set of fire irons, value 1s. 6d.
            </rs>, the goods of 
               
                  <persName id="t18320705-281-victim2291" type="victimName">
                     John 
                     Adams
                  <interp inst="t18320705-281-victim2291" type="surname" value="Adams"></interp>
                     <interp inst="t18320705-281-victim2291" type="given" value="John"></interp>
