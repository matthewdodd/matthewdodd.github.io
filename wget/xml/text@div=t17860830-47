                  <xptr type="pageFacsimile" doc="178608300091"></xptr>
            
                  <persName id="t17860830-47-person568">
                  WILLIAM
                   RICHARDSON
               <interp inst="t17860830-47-person568" type="surname" value="RICHARDSON"></interp>
