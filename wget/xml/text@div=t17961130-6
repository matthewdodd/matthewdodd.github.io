               <p>6. 
               
                  <persName id="t17961130-6-defend119" type="defendantName">
                     FRANCIS 
                     WARD
                  <interp inst="t17961130-6-defend119" type="surname" value="WARD"></interp>
                     <interp inst="t17961130-6-defend119" type="given" value="FRANCIS"></interp>
