            , (a <rs id="t17260114-34-deflabel206" type="occupation">Girl</rs>
                  <join result="persNameOccupation" targOrder="Y" targets="t17260114-34-defend227 t17260114-34-deflabel206"></join> of <placeName id="t17260114-34-defloc207">White Chappel</placeName>
                  <interp inst="t17260114-34-defloc207" type="placeName" value="White Chappel"></interp>
                  <interp inst="t17260114-34-defloc207" type="type" value="defendantHome"></interp>
                  <join result="persNamePlace" targOrder="Y" targets="t17260114-34-defend227 t17260114-34-defloc207"></join>, was indicted for <rs id="t17260114-34-off208" type="offenceDescription">
                     <interp inst="t17260114-34-off208" type="offenceCategory" value="theft"></interp>
                     <interp inst="t17260114-34-off208" type="offenceSubcategory" value="burglary"></interp>
               breaking and entring the House of 
                     
                        <persName id="t17260114-34-victim229" type="victimName">
                           John 
                           Cordingly 
                        <interp inst="t17260114-34-victim229" type="surname" value="Cordingly"></interp>
                        <interp inst="t17260114-34-victim229" type="given" value="John"></interp>
