                  <persName id="t17871024-32-person343">
                  EPHRAIM
                   CARTER
               <interp inst="t17871024-32-person343" type="surname" value="CARTER"></interp>
                     <interp inst="t17871024-32-person343" type="given" value="EPHRAIM"></interp>
                     <interp inst="t17871024-32-person343" type="gender" value="male"></interp>
