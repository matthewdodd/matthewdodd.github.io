                     <join result="persNameOccupation" targOrder="Y" targets="t18250519-63-victim395 t18250519-63-viclabel379"></join>, in the dwelling-house of 
                     <persName id="t18250519-63-person396">
                        Thomas 
                        Butcher
                     <interp inst="t18250519-63-person396" type="surname" value="Butcher"></interp>
