               <p>409. 
               <persName id="t18120408-98-defend977" type="defendantName">
                  WILLIAM 
                  RADDICK
               <interp inst="t18120408-98-defend977" type="surname" value="RADDICK"></interp>
                     <interp inst="t18120408-98-defend977" type="given" value="WILLIAM"></interp>
