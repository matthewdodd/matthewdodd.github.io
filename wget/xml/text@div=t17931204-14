                  <persName id="t17931204-14-person210">
                  ELIZABETH
                   LAWRENCE
               <interp inst="t17931204-14-person210" type="surname" value="LAWRENCE"></interp>
                     <interp inst="t17931204-14-person210" type="given" value="ELIZABETH"></interp>
                     <interp inst="t17931204-14-person210" type="gender" value="female"></interp>
