                     </persName>
                      
                  , <rs id="t17330404-57-cd252" type="crimeDate">Jan. 3</rs>
                     <join result="offenceCrimeDate" targOrder="Y" targets="t17330404-57-off251 t17330404-57-cd252"></join>
