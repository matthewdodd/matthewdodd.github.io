                        <interp inst="t18640711-658-verdict-1" type="verdictSubcategory" value="no_subcategory"></interp>GUILTY</rs>
                  </hi>.—<hi rend="italic">
                     <rs id="t18640711-658-punishment-10" type="punishmentDescription">
                        <interp inst="t18640711-658-punishment-10" type="punishmentCategory" value="imprison"></interp>
