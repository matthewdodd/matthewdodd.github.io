               <interp inst="t17380412-55" type="uri" value="sessionsPapers/17380412"></interp>
               <interp inst="t17380412-55" type="date" value="17380412"></interp>
               <join result="criminalCharge" id="t17380412-55-off266-c250" targOrder="Y" targets="t17380412-55-defend373 t17380412-55-off266 t17380412-55-verdict269"></join>
         
               <p>71. 
               
                  <persName id="t17380412-55-defend373" type="defendantName">
                     Eleanor 
                     Onion 
                  <interp inst="t17380412-55-defend373" type="surname" value="Onion"></interp>
                     <interp inst="t17380412-55-defend373" type="given" value="Eleanor"></interp>
                     <interp inst="t17380412-55-defend373" type="gender" value="female"></interp>
                  </persName>
               
            , was indicted for <rs id="t17380412-55-off266" type="offenceDescription">
                     <interp inst="t17380412-55-off266" type="offenceCategory" value="sexual"></interp>
                     <interp inst="t17380412-55-off266" type="offenceSubcategory" value="bigamy"></interp>
               marrying 
                     
                        <persName id="t17380412-55-victim375" type="victimName">
                           George 
                           Soley 
                        <interp inst="t17380412-55-victim375" type="surname" value="Soley"></interp>
                        <interp inst="t17380412-55-victim375" type="given" value="George"></interp>
                        <interp inst="t17380412-55-victim375" type="gender" value="male"></interp>
                     </persName>
                      
                  , in the Parish of St. Brides, January 6. 1727. And afterwards, viz. on the <rs id="t17380412-55-cd267" type="crimeDate">4th of June, 1732</rs>
                     <join result="offenceCrimeDate" targOrder="Y" targets="t17380412-55-off266 t17380412-55-cd267"></join>, for marrying 
                     
                        <persName id="t17380412-55-victim377" type="victimName">
                           Abram 
                           Iliva 
                        <interp inst="t17380412-55-victim377" type="surname" value="Iliva"></interp>
                        <interp inst="t17380412-55-victim377" type="given" value="Abram"></interp>
                        <interp inst="t17380412-55-victim377" type="gender" value="male"></interp>
                     </persName>
                      
                  , in the Parish of <placeName id="t17380412-55-crimeloc268">St. Martin's Ludgate</placeName>
                     <interp inst="t17380412-55-crimeloc268" type="placeName" value="St. Martin's Ludgate"></interp>
                     <interp inst="t17380412-55-crimeloc268" type="type" value="crimeLocation"></interp>
                     <join result="offencePlace" targOrder="Y" targets="t17380412-55-off266 t17380412-55-crimeloc268"></join>, her former Husband being then alive
            </rs>. <rs id="t17380412-55-verdict269" type="verdictDescription">
                     <interp inst="t17380412-55-verdict269" type="verdictCategory" value="notGuilty"></interp>
               Acquitted
            </rs>.</p>
