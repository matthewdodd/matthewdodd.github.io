                        <interp inst="def2-304-18730407" type="given" value="JOSEPH"></interp> JOSEPH SMITH</persName>
                  </hi>, and <persName id="def3-304-18730407" type="defendantName">
                     <interp inst="def3-304-18730407" type="gender" value="male"></interp>
                     <interp inst="def3-304-18730407" type="age" value="18"></interp>
                     <interp inst="def3-304-18730407" type="surname" value="MEAD"></interp>
