                  <hi rend="smallCaps">
                     <persName id="t18381126-name-1530" type="witnessName">
                        <interp inst="t18381126-name-1530" type="gender" value="male"></interp>
                        <interp inst="t18381126-name-1530" type="surname" value="SMITH"></interp>
