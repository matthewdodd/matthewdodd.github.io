                     <interp inst="t18130602-61-defend603" type="age" value="22"></interp>
                  </persName>
               
             was indicted for <rs id="t18130602-61-off325" type="offenceDescription">
                     <interp inst="t18130602-61-off325" type="offenceCategory" value="theft"></interp>
                     <interp inst="t18130602-61-off325" type="offenceSubcategory" value="housebreaking"></interp>
               feloniously breaking and entering the dwelling-house of 
                     
                        <persName id="t18130602-61-victim605" type="victimName">
                           Samuel 
                           Ray
                        <interp inst="t18130602-61-victim605" type="surname" value="Ray"></interp>
                        <interp inst="t18130602-61-victim605" type="given" value="Samuel"></interp>
                        <interp inst="t18130602-61-victim605" type="gender" value="male"></interp>
