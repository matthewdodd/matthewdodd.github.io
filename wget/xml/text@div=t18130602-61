               <p>577. 
               
                  <persName id="t18130602-61-defend603" type="defendantName">
                     JAMES 
                     SMITH
                  <interp inst="t18130602-61-defend603" type="surname" value="SMITH"></interp>
                     <interp inst="t18130602-61-defend603" type="given" value="JAMES"></interp>
