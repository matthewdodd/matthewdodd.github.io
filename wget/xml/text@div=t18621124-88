                     <interp inst="t18621124-88-verdict-1" type="verdictSubcategory" value="no_subcategory"></interp>
                     <hi rend="smallCaps">GUILTY</hi> 
                     <hi rend="italic">on the Second Count.—</hi>
                  </rs>
                  <hi rend="italic">
                     <rs id="t18621124-88-punishment-88" type="punishmentDescription">
