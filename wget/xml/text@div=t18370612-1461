                        <interp inst="t18370612-name-500" type="gender" value="male"></interp>
                        <interp inst="t18370612-name-500" type="surname" value="HUMPHREYS"></interp>
                        <interp inst="t18370612-name-500" type="given" value="GRIFFITH"></interp>
                        <join result="offenceVictim" targOrder="Y" targets="t18370612-1461-offence-1 t18370612-name-500"></join>Griffith Hum<lb></lb>phreys</persName>.</rs>
