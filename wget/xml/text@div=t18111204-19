               <p>
            
                  <persName id="t18111204-19-person256">
                  MARY 
                  MACDONALD
               <interp inst="t18111204-19-person256" type="surname" value="MACDONALD"></interp>
                     <interp inst="t18111204-19-person256" type="given" value="MARY"></interp>
                     <interp inst="t18111204-19-person256" type="gender" value="female"></interp>
