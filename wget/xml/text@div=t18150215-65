                     <interp inst="t18150215-65-victim549" type="gender" value="male"></interp>
                  </persName>
               
            , 
               
                  <persName id="t18150215-65-victim551" type="victimName">
                     George 
                     Sills
                  <interp inst="t18150215-65-victim551" type="surname" value="Sills"></interp>
