               <p>
                  <xptr type="pageFacsimile" doc="180710280052"></xptr>
            
                  <persName id="t18071028-60-person677">
                  JAMES 
                  PALMER
               <interp inst="t18071028-60-person677" type="surname" value="PALMER"></interp>
                     <interp inst="t18071028-60-person677" type="given" value="JAMES"></interp>
