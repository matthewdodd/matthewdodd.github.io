                     <interp inst="t17870221-20-person318" type="gender" value="male"></interp>
                  </persName>
            , 
               <persName id="t17870221-20-person319">
                  JAMES
                   COLEMAN
               <interp inst="t17870221-20-person319" type="surname" value="COLEMAN"></interp>
