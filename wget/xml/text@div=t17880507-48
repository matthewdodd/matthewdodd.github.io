                  <xptr type="pageFacsimile" doc="178805070053"></xptr>377. 
               
                  <persName id="t17880507-48-defend399" type="defendantName">
                     DANIEL 
                     HANCHARD
                  <interp inst="t17880507-48-defend399" type="surname" value="HANCHARD"></interp>
