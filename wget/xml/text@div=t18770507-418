                     <hi rend="largeCaps">ELLEN MORRIS</hi> (60)</persName>, <hi rend="italic">and</hi> 
                  <persName id="def2-418-18770507" type="defendantName">
                     <interp inst="def2-418-18770507" type="gender" value="female"></interp>
