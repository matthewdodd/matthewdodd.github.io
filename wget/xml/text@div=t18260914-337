               <interp inst="t18260914-337-defend1793" type="surname" value="HAMMOND"></interp>
                     <interp inst="t18260914-337-defend1793" type="given" value="SUSAN"></interp>
                     <interp inst="t18260914-337-defend1793" type="gender" value="female"></interp>
