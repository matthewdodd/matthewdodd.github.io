                     <interp inst="def1-126-18631214" type="gender" value="male"></interp>
                     <interp inst="def1-126-18631214" type="age" value="17"></interp>
                     <interp inst="def1-126-18631214" type="surname" value="ELLMORE"></interp>
                     <interp inst="def1-126-18631214" type="given" value="GEORGE"></interp>
                     <hi rend="largeCaps">GEORGE ELLMORE</hi> (17)</persName>
                  <rs id="t18631214-126-offence-1" type="offenceDescription">
                     <interp inst="t18631214-126-offence-1" type="offenceCategory" value="breakingPeace"></interp>
                     <interp inst="t18631214-126-offence-1" type="offenceSubcategory" value="wounding"></interp>, Feloniously wounding <persName id="t18631214-name-94" type="victimName">
                        <interp inst="t18631214-name-94" type="gender" value="male"></interp>
                        <interp inst="t18631214-name-94" type="surname" value="SAVAGE"></interp>
