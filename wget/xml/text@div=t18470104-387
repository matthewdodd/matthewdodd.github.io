                        <interp inst="t18470104-387-verdict-1" type="verdictSubcategory" value="no_subcategory"></interp>GUILTY</rs>.</hi>—<hi rend="italic">
                     <rs id="t18470104-387-punishment-43" type="punishmentDescription">
                        <interp inst="t18470104-387-punishment-43" type="punishmentCategory" value="imprison"></interp>
