             and 
               
                  <persName id="t17721209-86-defend737" type="defendantName">
                     JOHN
                      MITCHELL
                  <interp inst="t17721209-86-defend737" type="surname" value="MITCHELL"></interp>
                     <interp inst="t17721209-86-defend737" type="given" value="JOHN"></interp>
                     <interp inst="t17721209-86-defend737" type="gender" value="male"></interp>
                  </persName>
               
             were indicted for <rs id="t17721209-86-off421" type="offenceDescription">
                     <interp inst="t17721209-86-off421" type="offenceCategory" value="sexual"></interp>
                     <interp inst="t17721209-86-off421" type="offenceSubcategory" value="rape"></interp>
               committing a rape on the body of 
                     
                        <persName id="t17721209-86-victim739" type="victimName">
                           Mary
                            Wilds
                        <interp inst="t17721209-86-victim739" type="surname" value="Wilds"></interp>
                        <interp inst="t17721209-86-victim739" type="given" value="Mary"></interp>
                        <interp inst="t17721209-86-victim739" type="gender" value="female"></interp>
