                  ; and 1 duck, value 2s. 6d., the goods of 
                     
                        <persName id="t18290409-47-victim533" type="victimName">
                           Hannah 
                           Stapleton
                        <interp inst="t18290409-47-victim533" type="surname" value="Stapleton"></interp>
