                        <interp inst="t18791215-93-verdict-1" type="verdictSubcategory" value="no_subcategory"></interp>GUILTY</rs>
                  </hi>.—<hi rend="italic">
                     <rs id="t18791215-93-punishment-15" type="punishmentDescription">
                        <interp inst="t18791215-93-punishment-15" type="punishmentCategory" value="imprison"></interp>
