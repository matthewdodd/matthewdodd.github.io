                     <interp inst="t18141130-6-off31" type="offenceCategory" value="violentTheft"></interp>
                     <interp inst="t18141130-6-off31" type="offenceSubcategory" value="highwayRobbery"></interp>
               feloniously making an assault upon 
                     
                        <persName id="t18141130-6-victim129" type="victimName">
                           Samuel 
                           Pettigrones
                        <interp inst="t18141130-6-victim129" type="surname" value="Pettigrones"></interp>
                        <interp inst="t18141130-6-victim129" type="given" value="Samuel"></interp>
