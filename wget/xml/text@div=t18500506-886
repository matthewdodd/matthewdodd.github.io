                     <interp inst="t18500506-886-offence-1" type="offenceCategory" value="theft"></interp>
                     <interp inst="t18500506-886-offence-1" type="offenceSubcategory" value="simpleLarceny"></interp>, stealing 1 pair of boots, value 8<hi rend="italic">s</hi>. 6<hi rend="italic">d</hi>.; the goods of <persName id="t18500506-name-141" type="victimName">
                        <interp inst="t18500506-name-141" type="gender" value="male"></interp>
                        <interp inst="t18500506-name-141" type="surname" value="BAILEY"></interp>
