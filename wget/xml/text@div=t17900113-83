                     <interp inst="t17900113-83-off442" type="offenceCategory" value="violentTheft"></interp>
                     <interp inst="t17900113-83-off442" type="offenceSubcategory" value="highwayRobbery"></interp>
               feloniously assaulting 
                     
                        <persName id="t17900113-83-victim818" type="victimName">
                           Elizabeth 
                           Stout
                        <interp inst="t17900113-83-victim818" type="surname" value="Stout"></interp>
