                     <interp inst="t18630921-1156-verdict-1" type="verdictSubcategory" value="no_subcategory"></interp>
                     <hi rend="smallCaps">GUILTY</hi>
                  </rs>.—<rs id="t18630921-1156-punishment-75" type="punishmentDescription">
