               <persName id="t16800226-14-defend57" type="defendantName">
                  John 
                  Nailor, alias <rs id="t16800226-14-alias-2" type="alias">
                        <join result="nameAlias" targOrder="Y" targets="t16800226-14-defend57 t16800226-14-alias-2"></join>Carpenter</rs>
