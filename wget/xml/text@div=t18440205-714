               <p>
                  <hi rend="smallCaps">
                     <persName id="t18440205-name-1066" type="witnessName">
                        <interp inst="t18440205-name-1066" type="gender" value="male"></interp>
                        <interp inst="t18440205-name-1066" type="surname" value="COOK"></interp>
