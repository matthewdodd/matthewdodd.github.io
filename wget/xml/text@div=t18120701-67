                  <persName id="t18120701-67-defend604" type="defendantName">
                     THOMAS 
                     PURPLE
                  <interp inst="t18120701-67-defend604" type="surname" value="PURPLE"></interp>
                     <interp inst="t18120701-67-defend604" type="given" value="THOMAS"></interp>
