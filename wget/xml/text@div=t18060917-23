               <p>
            
                  <persName id="t18060917-23-person205">
                  SAMUEL 
                  GURTERIDGE
               <interp inst="t18060917-23-person205" type="surname" value="GURTERIDGE"></interp>
                     <interp inst="t18060917-23-person205" type="given" value="SAMUEL"></interp>
                     <interp inst="t18060917-23-person205" type="gender" value="male"></interp>
