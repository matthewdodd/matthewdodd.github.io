                     <interp inst="t18030216-32-defend195" type="age" value="24"></interp>
                  </persName>
             and 
               <persName id="t18030216-32-defend196" type="defendantName">
                  JAMES 
                  HOKEWAY
               <interp inst="t18030216-32-defend196" type="surname" value="HOKEWAY"></interp>
