                     <hi rend="largeCaps">DENIS COCKLIN</hi> (29)</persName>
                  <rs id="t18760918-430-offence-1" type="offenceDescription">
                     <interp inst="t18760918-430-offence-1" type="offenceCategory" value="theft"></interp>
                     <interp inst="t18760918-430-offence-1" type="offenceSubcategory" value="simpleLarceny"></interp>, Stealing 1<hi rend="italic">l</hi>. of <persName id="t18760918-name-307" type="victimName">
                        <interp inst="t18760918-name-307" type="gender" value="male"></interp>
                        <interp inst="t18760918-name-307" type="surname" value="NEALE"></interp>
