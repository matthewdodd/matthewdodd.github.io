                        <interp inst="t17430223-43-defend443" type="gender" value="male"></interp>
                     </persName>
                
             , of <placeName id="t17430223-43-defloc220">St James Westminster</placeName>
                     <interp inst="t17430223-43-defloc220" type="placeName" value="St James Westminster"></interp>
                     <interp inst="t17430223-43-defloc220" type="type" value="defendantHome"></interp>
                     <join result="persNamePlace" targOrder="Y" targets="t17430223-43-defend444 t17430223-43-defloc220"></join>
                     <join result="persNamePlace" targOrder="Y" targets="t17430223-43-defend443 t17430223-43-defloc220"></join>, was indicted with 
                  Elizabeth
                   his 
                  <rs id="t17430223-43-deflabel221" type="occupation">Wife</rs>
                     <join result="persNameOccupation" targOrder="Y" targets="t17430223-43-defend444 t17430223-43-deflabel221"></join>
