               <p>357. 
               
                  <persName id="t18120408-47-defend524" type="defendantName">
                     JAMES 
                     DAWSON
                  <interp inst="t18120408-47-defend524" type="surname" value="DAWSON"></interp>
                     <interp inst="t18120408-47-defend524" type="given" value="JAMES"></interp>
