                  </persName>
               
             and 
               
                  <persName id="t18240916-14-defend189" type="defendantName">
                     JAMES 
                     GRIFFITHS
                  <interp inst="t18240916-14-defend189" type="surname" value="GRIFFITHS"></interp>
                     <interp inst="t18240916-14-defend189" type="given" value="JAMES"></interp>
