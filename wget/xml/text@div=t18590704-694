               <join result="criminalCharge" id="t18590704-694-charge-1" targOrder="Y" targets="def1-694-18590704 t18590704-694-offence-1 t18590704-694-verdict-1"></join>
               <join result="criminalCharge" id="t18590704-694-charge-2" targOrder="Y" targets="def2-694-18590704 t18590704-694-offence-1 t18590704-694-verdict-2"></join>
