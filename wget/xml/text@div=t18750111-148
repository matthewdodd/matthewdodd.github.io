                  </rs> 
                  <hi rend="largeCaps">KNEVITT</hi> 
                  <hi rend="italic">and</hi> 
                  <hi rend="largeCaps">BROWN</hi>—<hi rend="italic">
                     <rs id="t18750111-148-punishment-39" type="punishmentDescription">
