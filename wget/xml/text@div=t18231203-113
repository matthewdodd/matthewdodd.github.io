             and 
               
                  <persName id="t18231203-113-defend1069" type="defendantName">
                     GEORGE 
                     CURTIS
                  <interp inst="t18231203-113-defend1069" type="surname" value="CURTIS"></interp>
                     <interp inst="t18231203-113-defend1069" type="given" value="GEORGE"></interp>
                     <interp inst="t18231203-113-defend1069" type="gender" value="male"></interp>
