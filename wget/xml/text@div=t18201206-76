                  <persName id="t18201206-76-person767">
                  MARY 
                  MORTIMER
               <interp inst="t18201206-76-person767" type="surname" value="MORTIMER"></interp>
                     <interp inst="t18201206-76-person767" type="given" value="MARY"></interp>
