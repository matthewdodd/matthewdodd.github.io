                        <join result="defendantPunishment" targOrder="Y" targets="def1-659-18580614 t18580614-659-punishment-50"></join>Six Years Penal Servitude.</rs>
                  </hi> 
                  <hi rend="largeCaps">DE PUZEY</hi> 
                  <hi rend="italic">and</hi> 
                  <hi rend="largeCaps">PARTRIDGE</hi>—<hi rend="smallCaps">
                     <rs id="t18580614-659-verdict-4" type="verdictDescription">
                        <interp inst="t18580614-659-verdict-4" type="verdictCategory" value="guilty"></interp>
                        <interp inst="t18580614-659-verdict-4" type="verdictSubcategory" value="no_subcategory"></interp>GUILTY</rs>
                  </hi>.—<hi rend="italic">
