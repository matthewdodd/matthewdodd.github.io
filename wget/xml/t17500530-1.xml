                     <interp inst="t17500530-1-off2" type="offenceSubcategory" value="highwayRobbery"></interp>
               assaulting 
                     
                        <persName id="t17500530-1-victim32" type="victimName">
                           William 
                           Rock 
                        <interp inst="t17500530-1-victim32" type="surname" value="Rock"></interp>
                        <interp inst="t17500530-1-victim32" type="given" value="William"></interp>
