                  <persName id="t17720603-36-victim377" type="victimName">
                     George
                      Adamson
                  <interp inst="t17720603-36-victim377" type="surname" value="Adamson"></interp>
                     <interp inst="t17720603-36-victim377" type="given" value="George"></interp>
