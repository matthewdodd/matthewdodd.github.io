            , and 
               
                  <persName id="t18231203-88-defend901" type="defendantName">
                     THOMAS 
                     OFFWOOD
                  <interp inst="t18231203-88-defend901" type="surname" value="OFFWOOD"></interp>
                     <interp inst="t18231203-88-defend901" type="given" value="THOMAS"></interp>
                     <interp inst="t18231203-88-defend901" type="gender" value="male"></interp>
                  </persName>
               
            , were indicted for <rs id="t18231203-88-off471" type="offenceDescription">
                     <interp inst="t18231203-88-off471" type="offenceCategory" value="theft"></interp>
                     <interp inst="t18231203-88-off471" type="offenceSubcategory" value="grandLarceny"></interp>
               stealing, on the <rs id="t18231203-88-cd472" type="crimeDate">4th of December</rs>
                     <join result="offenceCrimeDate" targOrder="Y" targets="t18231203-88-off471 t18231203-88-cd472"></join>, four teaspoons, value 8 s.; a wooden till, value 2 s.; four half-crowns,<xptr type="pageFacsimile" doc="182312030043"></xptr>thirty sixpences, twelve penny pieces, and twenty-four halfpence
            </rs>, the property of 
               
                  <persName id="t18231203-88-victim903" type="victimName">
                     John 
                     Norris
                  <interp inst="t18231203-88-victim903" type="surname" value="Norris"></interp>
