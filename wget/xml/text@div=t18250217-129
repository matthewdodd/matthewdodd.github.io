                     <interp inst="t18250217-129-off750" type="offenceCategory" value="theft"></interp>
                     <interp inst="t18250217-129-off750" type="offenceSubcategory" value="grandLarceny"></interp>
               stealing, on the <rs id="t18250217-129-cd751" type="crimeDate">10th of February</rs>
                     <join result="offenceCrimeDate" targOrder="Y" targets="t18250217-129-off750 t18250217-129-cd751"></join>, a square, value 2 s. 6 d.
            </rs>, the goods of 
               <persName id="t18250217-129-victim733" type="victimName">
                  Francis 
                  Bryant
               <interp inst="t18250217-129-victim733" type="surname" value="Bryant"></interp>
                     <interp inst="t18250217-129-victim733" type="given" value="Francis"></interp>
                     <interp inst="t18250217-129-victim733" type="gender" value="male"></interp>
                     <join result="offenceVictim" targOrder="Y" targets="t18250217-129-off750 t18250217-129-victim733"></join>
