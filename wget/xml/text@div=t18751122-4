                        <interp inst="t18751122-4-verdict-1" type="verdictSubcategory" value="no_subcategory"></interp>GUILTY</rs>
                  </hi>—<hi rend="italic">
                     <rs id="t18751122-4-punishment-3" type="punishmentDescription">
                        <interp inst="t18751122-4-punishment-3" type="punishmentCategory" value="imprison"></interp>
