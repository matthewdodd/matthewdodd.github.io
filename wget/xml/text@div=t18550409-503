                  <hi rend="smallCaps">
                     <rs id="t18550409-503-verdict-1" type="verdictDescription">
                        <interp inst="t18550409-503-verdict-1" type="verdictCategory" value="guilty"></interp>
                        <interp inst="t18550409-503-verdict-1" type="verdictSubcategory" value="pleadedGuilty"></interp>PLEADED GUILTY</rs>
