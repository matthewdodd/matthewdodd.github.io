                     <interp inst="t18750405-262-verdict-2" type="verdictSubcategory" value="manslaughter"></interp>
                     <hi rend="smallCaps">GUILTY</hi> 
                     <hi rend="italic">of manslaughter.</hi>
                  </rs> 
