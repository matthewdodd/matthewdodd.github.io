               <interp inst="t18280911-231-defend1312" type="surname" value="WHITLEY"></interp>
                     <interp inst="t18280911-231-defend1312" type="given" value="JOHN WILLIAM"></interp>
                     <interp inst="t18280911-231-defend1312" type="gender" value="male"></interp>
                     <interp inst="t18280911-231-defend1312" type="age" value="12"></interp>
                  </persName>
             were indicted for <rs id="t18280911-231-off1183" type="offenceDescription">
                     <interp inst="t18280911-231-off1183" type="offenceCategory" value="theft"></interp>
                     <interp inst="t18280911-231-off1183" type="offenceSubcategory" value="simpleLarceny"></interp>
               stealing, on the <rs id="t18280911-231-cd1184" type="crimeDate">30th of August</rs>
                     <join result="offenceCrimeDate" targOrder="Y" targets="t18280911-231-off1183 t18280911-231-cd1184"></join>, 7 lbs. of cheese, value 6s.
            </rs>, the goods of 
               <persName id="t18280911-231-victim1313" type="victimName">
                  Richard 
                  Morriss
               <interp inst="t18280911-231-victim1313" type="surname" value="Morriss"></interp>
                     <interp inst="t18280911-231-victim1313" type="given" value="Richard"></interp>
