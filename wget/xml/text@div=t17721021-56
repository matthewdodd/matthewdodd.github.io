                  <xptr type="pageFacsimile" doc="177210210035"></xptr>
            
               
                  <persName id="t17721021-56-victim531" type="victimName">
                     John
                      Stearne
                  <interp inst="t17721021-56-victim531" type="surname" value="Stearne"></interp>
