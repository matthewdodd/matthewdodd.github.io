                     <interp inst="t18310407-118-off653" type="offenceCategory" value="theft"></interp>
                     <interp inst="t18310407-118-off653" type="offenceSubcategory" value="simpleLarceny"></interp>
               stealing, on the <rs id="t18310407-118-cd654" type="crimeDate">22nd of March</rs>
                     <join result="offenceCrimeDate" targOrder="Y" targets="t18310407-118-off653 t18310407-118-cd654"></join>, 1 chair, value 15s.
            </rs>, the goods of 
               
                  <persName id="t18310407-118-victim1025" type="victimName">
                     Levi 
                     Oughton
                  <interp inst="t18310407-118-victim1025" type="surname" value="Oughton"></interp>
                     <interp inst="t18310407-118-victim1025" type="given" value="Levi"></interp>
                     <interp inst="t18310407-118-victim1025" type="gender" value="male"></interp>
                     <join result="offenceVictim" targOrder="Y" targets="t18310407-118-off653 t18310407-118-victim1025"></join>
