                        <join result="offenceCrimeDate" targOrder="Y" targets="t18381126-180-offence-1 t18381126-cd-157"></join>11th of September</rs>, 1 bed, value 2<hi rend="italic">l</hi>.; 1 bolster, value 3<hi rend="italic">s</hi>.; 1 blanket, value-1<hi rend="italic">s</hi>.; 2 sheets, value 3<hi rend="italic">s</hi>.; 1 quilt, value 9<hi rend="italic">d</hi>.; 1 set of fire-irons, value 3<hi rend="italic">s</hi>.; 1 printed book, value 1<hi rend="italic">s</hi>.; 1 table-cloth, value 2<hi rend="italic">s</hi>.; 1 shawl, value 1<hi rend="italic">s</hi>. 6<hi rend="italic">d</hi>.; 2 pillows, value 4<hi rend="italic">s</hi>.; and X flat-iron, value 4<hi rend="italic">d</hi>.; the goods of <persName id="t18381126-name-1052" type="victimName">
                        <interp inst="t18381126-name-1052" type="gender" value="male"></interp>
                        <interp inst="t18381126-name-1052" type="surname" value="KELSEY"></interp>
