                        <interp inst="t17850511-84-victim794" type="gender" value="male"></interp>
                     </persName>
                     
                  , 
                     
                        <persName id="t17850511-84-victim796" type="victimName">
                           George 
                           Hubert
                        <interp inst="t17850511-84-victim796" type="surname" value="Hubert"></interp>
