            , and 
               
                  <persName id="t18191027-83-defend946" type="defendantName">
                     EDWARD 
                     HARMER
                  <interp inst="t18191027-83-defend946" type="surname" value="HARMER"></interp>
                     <interp inst="t18191027-83-defend946" type="given" value="EDWARD"></interp>
