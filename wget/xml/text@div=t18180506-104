            ; and 
               
                  <persName id="t18180506-104-defend901" type="defendantName">
                     THOMAS
                     <xptr type="pageFacsimile" doc="181805060035"></xptr> 
                     AVERY
                  <interp inst="t18180506-104-defend901" type="surname" value="AVERY"></interp>
                     <interp inst="t18180506-104-defend901" type="given" value="THOMAS"></interp>
