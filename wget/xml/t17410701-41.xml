               <p>60. 
               
                  <persName id="t17410701-41-defend371" type="defendantName">
                     Joseph 
                     Whitaker 
                  <interp inst="t17410701-41-defend371" type="surname" value="Whitaker"></interp>
                     <interp inst="t17410701-41-defend371" type="given" value="Joseph"></interp>
                     <interp inst="t17410701-41-defend371" type="gender" value="male"></interp>
                  </persName>
               
