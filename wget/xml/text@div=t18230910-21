                     <interp inst="t18230910-21-defend311" type="age" value="50"></interp>
                  </persName>
               
             were indicted for the <rs id="t18230910-21-off113" type="offenceDescription">
                     <interp inst="t18230910-21-off113" type="offenceCategory" value="kill"></interp>
                     <interp inst="t18230910-21-off113" type="offenceSubcategory" value="murder"></interp>
               wilful murder of 
                     
                        <persName id="t18230910-21-victim313" type="victimName">
                           Samuel 
                           Dance
                        <interp inst="t18230910-21-victim313" type="surname" value="Dance"></interp>
                        <interp inst="t18230910-21-victim313" type="given" value="Samuel"></interp>
                        <interp inst="t18230910-21-victim313" type="gender" value="male"></interp>
