                     <join result="defendantPunishment" targOrder="Y" targets="t17830430-50-defend667 t17830430-50-punish279"></join>
               privately whipped
            </rs> and <rs id="t17830430-50-punish280" type="punishmentDescription">
                     <interp inst="t17830430-50-punish280" type="punishmentCategory" value="imprison"></interp>
