                  <persName id="t17830430-50-person670">
                  CHARLES
                   RIDE
               <interp inst="t17830430-50-person670" type="surname" value="RIDE"></interp>
                     <interp inst="t17830430-50-person670" type="given" value="CHARLES"></interp>
