                  </persName>
            , 
               <persName id="t18050529-27-victim248" type="victimName">
                  John-Truman 
                  Villebois
               <interp inst="t18050529-27-victim248" type="surname" value="Villebois"></interp>
                     <interp inst="t18050529-27-victim248" type="given" value="John-Truman"></interp>
                     <interp inst="t18050529-27-victim248" type="gender" value="male"></interp>
                  </persName>
            , and 
               <persName id="t18050529-27-victim249" type="victimName">
                  Henry 
                  Villebois
               <interp inst="t18050529-27-victim249" type="surname" value="Villebois"></interp>
