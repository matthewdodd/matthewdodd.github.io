             and 
               
                  <persName id="t18191201-122-defend1270" type="defendantName">
                     ESTHER 
                     SWATMAN
                  <interp inst="t18191201-122-defend1270" type="surname" value="SWATMAN"></interp>
                     <interp inst="t18191201-122-defend1270" type="given" value="ESTHER"></interp>
