                  <persName id="t17890422-101-person892">
                  ELIZABETH
                   KING
               <interp inst="t17890422-101-person892" type="surname" value="KING"></interp>
                     <interp inst="t17890422-101-person892" type="given" value="ELIZABETH"></interp>
