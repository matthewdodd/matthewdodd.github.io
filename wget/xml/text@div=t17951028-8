               <p>478. 
               
                  <persName id="t17951028-8-defend138" type="defendantName">
                     WILLIAM LUCKEN 
                     OYENS
                  <interp inst="t17951028-8-defend138" type="surname" value="OYENS"></interp>
                     <interp inst="t17951028-8-defend138" type="given" value="WILLIAM LUCKEN"></interp>
                     <interp inst="t17951028-8-defend138" type="gender" value="male"></interp>
