               <p>
                  <hi rend="largeCaps">JONES</hi> pleaded <hi rend="smallCaps">
                     <rs id="t18480228-880-verdict-3" type="verdictDescription">
                        <interp inst="t18480228-880-verdict-3" type="verdictCategory" value="guilty"></interp>
