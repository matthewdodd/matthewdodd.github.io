               <p>133. 
               
                  <persName id="t17920215-27-defend358" type="defendantName">
                     THOMAS 
                     WEST
                  <interp inst="t17920215-27-defend358" type="surname" value="WEST"></interp>
                     <interp inst="t17920215-27-defend358" type="given" value="THOMAS"></interp>
