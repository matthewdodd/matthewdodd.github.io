                  <hi rend="largeCaps">BAKER</hi>—<hi rend="smallCaps">
                     <rs id="t18650918-878-verdict-1" type="verdictDescription">
                        <interp inst="t18650918-878-verdict-1" type="verdictCategory" value="guilty"></interp>
