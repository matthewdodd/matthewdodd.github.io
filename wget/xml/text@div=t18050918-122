             and 
               
                  <persName id="t18050918-122-defend1128" type="defendantName">
                     FRANCES 
                     ROSE
                  <interp inst="t18050918-122-defend1128" type="surname" value="ROSE"></interp>
                     <interp inst="t18050918-122-defend1128" type="given" value="FRANCES"></interp>
