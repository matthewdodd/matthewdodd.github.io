               <p>423. 
               
                  <persName id="t17950916-52-defend595" type="defendantName">
                     AGNES 
                     VALLANCE
                  <interp inst="t17950916-52-defend595" type="surname" value="VALLANCE"></interp>
                     <interp inst="t17950916-52-defend595" type="given" value="AGNES"></interp>
