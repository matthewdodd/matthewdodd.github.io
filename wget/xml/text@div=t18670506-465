                  <hi rend="largeCaps">SHEPHERD</hi> 
                  <hi rend="smallCaps">
                     <rs id="t18670506-465-verdict-1" type="verdictDescription">
                        <interp inst="t18670506-465-verdict-1" type="verdictCategory" value="guilty"></interp>
                        <interp inst="t18670506-465-verdict-1" type="verdictSubcategory" value="pleadedGuilty"></interp>PLEADED GUILTY</rs>
                  </hi>.—<hi rend="italic">
                     <rs id="t18670506-465-punishment-12" type="punishmentDescription">
                        <interp inst="t18670506-465-punishment-12" type="punishmentCategory" value="imprison"></interp>
