                  <hi rend="largeCaps">HAYNES</hi> 
                  <rs id="t18480821-1809-verdict-1" type="verdictDescription">
                     <interp inst="t18480821-1809-verdict-1" type="verdictCategory" value="guilty"></interp>
                     <interp inst="t18480821-1809-verdict-1" type="verdictSubcategory" value="pleadedGuilty"></interp>pleaded Guilty</rs>. Aged 15.—<hi rend="italic">
                     <rs id="t18480821-1809-punishment-41" type="punishmentDescription">
                        <interp inst="t18480821-1809-punishment-41" type="punishmentCategory" value="imprison"></interp>
