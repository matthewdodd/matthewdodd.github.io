<div1 type="trialAccount" id="t17600709-4">
               <interp inst="t17600709-4" type="collection" value="BAILEY"></interp>
               <interp inst="t17600709-4" type="year" value="1760"></interp>
               <interp inst="t17600709-4" type="uri" value="sessionsPapers/17600709"></interp>
