                     <interp inst="t17230116-39-victim212" type="surname" value="Mitchel"></interp>
                        <interp inst="t17230116-39-victim212" type="given" value="Susannah"></interp>
                        <interp inst="t17230116-39-victim212" type="gender" value="female"></interp>
                        <join result="offenceVictim" targOrder="Y" targets="t17230116-39-off208 t17230116-39-victim212"></join>
