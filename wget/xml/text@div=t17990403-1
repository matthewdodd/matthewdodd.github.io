               <p>
            
                  <persName id="t17990403-1-person71">
                  ISAAC
                   STRANGE
               <interp inst="t17990403-1-person71" type="surname" value="STRANGE"></interp>
                     <interp inst="t17990403-1-person71" type="given" value="ISAAC"></interp>
