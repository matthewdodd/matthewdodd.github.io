                        <interp inst="t18740608-394-verdict-1" type="verdictSubcategory" value="no_subcategory"></interp>GUILTY</rs>
                  </hi>—<hi rend="italic">
                     <rs id="t18740608-394-punishment-11" type="punishmentDescription">
                        <interp inst="t18740608-394-punishment-11" type="punishmentCategory" value="imprison"></interp>
