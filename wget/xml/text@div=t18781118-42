                     <interp inst="def2-42-18781118" type="surname" value="BURT"></interp>
                     <interp inst="def2-42-18781118" type="given" value="FINDLEY WENTWORTH"></interp>
                     <hi rend="largeCaps">FINDLEY WENTWORTH BURT</hi> (22)</persName>, and <persName id="def3-42-18781118" type="defendantName">
                     <interp inst="def3-42-18781118" type="gender" value="male"></interp>
