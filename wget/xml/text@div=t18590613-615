                        <interp inst="t18590613-615-verdict-1" type="verdictSubcategory" value="no_subcategory"></interp>GUILTY</rs>
                  </hi>.—<hi rend="italic">
                     <rs id="t18590613-615-punishment-65" type="punishmentDescription">
                        <interp inst="t18590613-615-punishment-65" type="punishmentCategory" value="imprison"></interp>
