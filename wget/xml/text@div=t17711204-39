               <join result="criminalCharge" id="t17711204-39-off201-c438" targOrder="Y" targets="t17711204-39-defend557 t17711204-39-off201 t17711204-39-verdict213"></join>
               <join result="criminalCharge" id="t17711204-39-off201-c439" targOrder="Y" targets="t17711204-39-defend558 t17711204-39-off201 t17711204-39-verdict215"></join>
         
               <p>63, 64, 66, 67, 65, 68, (M.) 
               
                  <persName id="t17711204-39-defend549" type="defendantName">
                     LEVI
                      WEIL
                  <interp inst="t17711204-39-defend549" type="surname" value="WEIL"></interp>
