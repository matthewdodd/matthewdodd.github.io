                  <persName id="t17711204-39-defend555" type="defendantName">
                     JACOB
                      LAZARUS, otherwise <rs id="t17711204-39-alias-3" type="alias">
                        <join result="nameAlias" targOrder="Y" targets="t17711204-39-defend555 t17711204-39-alias-3"></join>HYAM DRESDEN</rs>, otherwise <rs id="t17711204-39-alias-4" type="alias">
                        <join result="nameAlias" targOrder="Y" targets="t17711204-39-defend555 t17711204-39-alias-4"></join>HYAM LAZARUS</rs>
                     <interp inst="t17711204-39-defend555" type="surname" value="LAZARUS"></interp>
