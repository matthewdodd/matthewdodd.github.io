            , 
               
                  <persName id="t17711204-39-defend557" type="defendantName">
                     SOLOMON
                      PORTER, otherwise <rs id="t17711204-39-alias-5" type="alias">
                        <join result="nameAlias" targOrder="Y" targets="t17711204-39-defend557 t17711204-39-alias-5"></join>MOSES</rs>
                     <interp inst="t17711204-39-defend557" type="surname" value="PORTER"></interp>
