                  </persName> and <persName id="def2-1019-18530919" type="defendantName">
                     <interp inst="def2-1019-18530919" type="gender" value="male"></interp>
                     <interp inst="def2-1019-18530919" type="surname" value="SHIPPERS"></interp>
