               <p>
            
               
                  <persName id="t16871207-43-defend193" type="defendantName">
                     Robert 
                     Sampson
                  <interp inst="t16871207-43-defend193" type="surname" value="Sampson"></interp>
                     <interp inst="t16871207-43-defend193" type="given" value="Robert"></interp>
                     <interp inst="t16871207-43-defend193" type="gender" value="male"></interp>
                  </persName>
               
            , of <placeName id="t16871207-43-defloc199">St. Leonard Shoreditch</placeName>
                  <interp inst="t16871207-43-defloc199" type="placeName" value="St. Leonard Shoreditch"></interp>
                  <interp inst="t16871207-43-defloc199" type="type" value="defendantHome"></interp>
                  <join result="persNamePlace" targOrder="Y" targets="t16871207-43-defend193 t16871207-43-defloc199"></join>, Indicted for <rs id="t16871207-43-off200" type="offenceDescription">
                     <interp inst="t16871207-43-off200" type="offenceCategory" value="theft"></interp>
                     <interp inst="t16871207-43-off200" type="offenceSubcategory" value="burglary"></interp>
               breaking the House of 
                     
                        <persName id="t16871207-43-victim195" type="victimName">
                           Robert 
                           Fisher
                        <interp inst="t16871207-43-victim195" type="surname" value="Fisher"></interp>
                        <interp inst="t16871207-43-victim195" type="given" value="Robert"></interp>
