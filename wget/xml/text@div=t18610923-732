                        <interp inst="t18610923-732-verdict-1" type="verdictSubcategory" value="no_subcategory"></interp>GUILTY</rs>
                  </hi>.—<hi rend="italic">
                     <rs id="t18610923-732-punishment-3" type="punishmentDescription">
                        <interp inst="t18610923-732-punishment-3" type="punishmentCategory" value="imprison"></interp>
