                  <persName id="t18210110-86-victim862" type="victimName">
                     JOHN COPE 
                     FOLKARD
                  <interp inst="t18210110-86-victim862" type="surname" value="FOLKARD"></interp>
                     <interp inst="t18210110-86-victim862" type="given" value="JOHN COPE"></interp>
