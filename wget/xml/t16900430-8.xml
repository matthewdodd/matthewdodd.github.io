                     <interp inst="t16900430-8-person76" type="surname" value="Cane"></interp>
                        <interp inst="t16900430-8-person76" type="given" value="Richard"></interp>
                        <interp inst="t16900430-8-person76" type="gender" value="male"></interp>
                     </persName>
                  , and 
                     <persName id="t16900430-8-person77">
                        James 
                        Kyrbey
                     <interp inst="t16900430-8-person77" type="surname" value="Kyrbey"></interp>
                        <interp inst="t16900430-8-person77" type="given" value="James"></interp>
                        <interp inst="t16900430-8-person77" type="gender" value="male"></interp>
                     </persName>
