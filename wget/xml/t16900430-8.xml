                  <join result="persNamePlace" targOrder="Y" targets="t16900430-8-defend57 t16900430-8-defloc37"></join>
                  <join result="persNamePlace" targOrder="Y" targets="t16900430-8-defend59 t16900430-8-defloc37"></join>
