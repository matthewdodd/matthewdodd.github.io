                  <hi rend="largeCaps">PEARCE</hi> 
                  <hi rend="smallCaps">
                     <rs id="t18770205-223-verdict-1" type="verdictDescription">
                        <interp inst="t18770205-223-verdict-1" type="verdictCategory" value="guilty"></interp>
