                        <interp inst="def2-629-18550611" type="surname" value="SHIELDS"></interp>
                        <interp inst="def2-629-18550611" type="given" value="JOHN"></interp> JOHN SHIELDS</persName>
                  </hi>, and <persName id="def3-629-18550611" type="defendantName">
                     <interp inst="def3-629-18550611" type="gender" value="male"></interp>
                     <interp inst="def3-629-18550611" type="surname" value="MCNAMARA"></interp>
                     <interp inst="def3-629-18550611" type="given" value="JOHN"></interp>
                     <hi rend="largeCaps">JOHN MCNAMARA</hi>
                  </persName>
                  <rs id="t18550611-629-offence-1" type="offenceDescription">
                     <interp inst="t18550611-629-offence-1" type="offenceCategory" value="theft"></interp>
                     <interp inst="t18550611-629-offence-1" type="offenceSubcategory" value="pocketpicking"></interp>, stealing 1 purse and 1 handkerchief, value 3<hi rend="italic">s</hi>., and 7<hi rend="italic">s</hi>. 9<hi rend="italic">d</hi>. in money; the property of <persName id="t18550611-name-235" type="victimName">
                        <interp inst="t18550611-name-235" type="gender" value="female"></interp>
                        <interp inst="t18550611-name-235" type="surname" value="CORDEROY"></interp>
