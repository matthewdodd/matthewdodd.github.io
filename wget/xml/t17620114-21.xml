<div1 type="trialAccount" id="t17620114-21">
               <interp inst="t17620114-21" type="collection" value="BAILEY"></interp>
               <interp inst="t17620114-21" type="year" value="1762"></interp>
               <interp inst="t17620114-21" type="uri" value="sessionsPapers/17620114"></interp>
               <interp inst="t17620114-21" type="date" value="17620114"></interp>
               <join result="criminalCharge" id="t17620114-21-off102-c151" targOrder="Y" targets="t17620114-21-defend187 t17620114-21-off102 t17620114-21-verdict105"></join>
         
               <p>71. (M.) 
               
                  <persName id="t17620114-21-defend187" type="defendantName">
                     Richard 
                     Rushit 
                  <interp inst="t17620114-21-defend187" type="surname" value="Rushit"></interp>
                     <interp inst="t17620114-21-defend187" type="given" value="Richard"></interp>
                     <interp inst="t17620114-21-defend187" type="gender" value="male"></interp>
                  </persName>
               
            , <rs id="t17620114-21-deflabel101" type="occupation">carpenter</rs>
                  <join result="persNameOccupation" targOrder="Y" targets="t17620114-21-defend187 t17620114-21-deflabel101"></join>, was indicted for that <rs id="t17620114-21-off102" type="offenceDescription">
                     <interp inst="t17620114-21-off102" type="offenceCategory" value="sexual"></interp>
                     <interp inst="t17620114-21-off102" type="offenceSubcategory" value="bigamy"></interp>
               he married 
                     
                        <persName id="t17620114-21-victim189" type="victimName">
                           Mary 
                           Dean 
                        <interp inst="t17620114-21-victim189" type="surname" value="Dean"></interp>
                        <interp inst="t17620114-21-victim189" type="given" value="Mary"></interp>
                        <interp inst="t17620114-21-victim189" type="gender" value="female"></interp>
                     </persName>
                      
                   on the 14th of February, in the 13th year of his late Majesty, and afterwards, to wit, on the <rs id="t17620114-21-cd103" type="crimeDate">3d of February, in the 31st year of the said reign</rs>
                     <join result="offenceCrimeDate" targOrder="Y" targets="t17620114-21-off102 t17620114-21-cd103"></join>, he married 
                     
                        <persName id="t17620114-21-victim191" type="victimName">
                           Susanna 
                           Philips 
                        <interp inst="t17620114-21-victim191" type="surname" value="Philips"></interp>
                        <interp inst="t17620114-21-victim191" type="given" value="Susanna"></interp>
                        <interp inst="t17620114-21-victim191" type="gender" value="female"></interp>
                     </persName>
                      
                  , <rs id="t17620114-21-viclabel104" type="occupation">Spinster</rs>
                     <join result="persNameOccupation" targOrder="Y" targets="t17620114-21-victim191 t17620114-21-viclabel104"></join>, his former wife being then living and in full life
            </rs>; to which he <rs id="t17620114-21-verdict105" type="verdictDescription">
                     <interp inst="t17620114-21-verdict105" type="verdictCategory" value="guilty"></interp>
                     <interp inst="t17620114-21-verdict105" type="verdictSubcategory" value="pleadedGuilty"></interp>
               pleaded Guilty
            </rs>.</p>
               <p>
                  <rs id="t17620114-21-punish106" type="punishmentDescription">
                     <interp inst="t17620114-21-punish106" type="punishmentCategory" value="miscPunish"></interp>
                     <interp inst="t17620114-21-punish106" type="punishmentSubcategory" value="branding"></interp>
                     <join result="defendantPunishment" targOrder="Y" targets="t17620114-21-defend187 t17620114-21-punish106"></join>
               
                     <note>[Branding. See summary.]</note>
               
                  </rs>
               </p>
