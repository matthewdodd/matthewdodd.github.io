                     <interp inst="t18140706-92-victim762" type="gender" value="male"></interp>
                  </persName>
               
            , 
               
                  <persName id="t18140706-92-victim764" type="victimName">
                     George 
                     Reeves
                  <interp inst="t18140706-92-victim764" type="surname" value="Reeves"></interp>
                     <interp inst="t18140706-92-victim764" type="given" value="George"></interp>
