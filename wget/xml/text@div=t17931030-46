                  <persName id="t17931030-46-person485">
                  MARY
                   GRIFFITHS
               <interp inst="t17931030-46-person485" type="surname" value="GRIFFITHS"></interp>
                     <interp inst="t17931030-46-person485" type="given" value="MARY"></interp>
