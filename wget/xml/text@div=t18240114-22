                        <interp inst="t18240114-22-victim272" type="given" value="John"></interp>
                        <interp inst="t18240114-22-victim272" type="gender" value="male"></interp>
                     </persName>
                     
                  , and 
                     
                        <persName id="t18240114-22-victim274" type="victimName">
                           William Orton 
                           Aikin
