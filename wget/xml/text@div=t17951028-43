             and 
               
                  <persName id="t17951028-43-defend438" type="defendantName">
                     ANN 
                     KENE
                  <interp inst="t17951028-43-defend438" type="surname" value="KENE"></interp>
                     <interp inst="t17951028-43-defend438" type="given" value="ANN"></interp>
                     <interp inst="t17951028-43-defend438" type="gender" value="female"></interp>
                  </persName>
               
             were indicted for <rs id="t17951028-43-off241" type="offenceDescription">
                     <interp inst="t17951028-43-off241" type="offenceCategory" value="theft"></interp>
                     <interp inst="t17951028-43-off241" type="offenceSubcategory" value="grandLarceny"></interp>
               feloniously stealing, on the <rs id="t17951028-43-cd242" type="crimeDate">17th of October</rs>
                     <join result="offenceCrimeDate" targOrder="Y" targets="t17951028-43-off241 t17951028-43-cd242"></join>, a silver watch, value 35s.
            </rs> the goods of 
               
                  <persName id="t17951028-43-victim440" type="victimName">
                     William 
                     Kirkpatrick
                  <interp inst="t17951028-43-victim440" type="surname" value="Kirkpatrick"></interp>
