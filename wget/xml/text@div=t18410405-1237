               <p>1237. <persName id="def1-1237-18410405" type="defendantName">
                     <interp inst="def1-1237-18410405" type="gender" value="female"></interp>
                     <interp inst="def1-1237-18410405" type="age" value="24"></interp>
                     <interp inst="def1-1237-18410405" type="surname" value="COX"></interp>
                     <interp inst="def1-1237-18410405" type="given" value="MARY ANN"></interp>
