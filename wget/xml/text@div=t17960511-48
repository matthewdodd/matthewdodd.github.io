            , and 
               
                  <persName id="t17960511-48-defend634" type="defendantName">
                     ELIZABETH 
                     WALTON
                  <interp inst="t17960511-48-defend634" type="surname" value="WALTON"></interp>
                     <interp inst="t17960511-48-defend634" type="given" value="ELIZABETH"></interp>
