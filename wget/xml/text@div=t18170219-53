                  <persName id="t18170219-53-person447">
                  ABIGAIL 
                  WEST
               <interp inst="t18170219-53-person447" type="surname" value="WEST"></interp>
                     <interp inst="t18170219-53-person447" type="given" value="ABIGAIL"></interp>
