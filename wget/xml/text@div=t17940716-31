                  <persName id="t17940716-31-person345">
                  ANN
                   MACCLUGH
               <interp inst="t17940716-31-person345" type="surname" value="MACCLUGH"></interp>
                     <interp inst="t17940716-31-person345" type="given" value="ANN"></interp>
