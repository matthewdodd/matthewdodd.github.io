                  <persName id="t18240603-80-person717">
                  SAMUEL 
                  SHEFFIELD
               <interp inst="t18240603-80-person717" type="surname" value="SHEFFIELD"></interp>
                     <interp inst="t18240603-80-person717" type="given" value="SAMUEL"></interp>
                     <interp inst="t18240603-80-person717" type="gender" value="male"></interp>
