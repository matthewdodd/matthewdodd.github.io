               <p>1241. 
               <persName id="t18250915-19-defend202" type="defendantName">
                  CORNELIUS 
                  GORE
               <interp inst="t18250915-19-defend202" type="surname" value="GORE"></interp>
                     <interp inst="t18250915-19-defend202" type="given" value="CORNELIUS"></interp>
