                  <persName id="t17920329-47-person503">
                  JANE
                   GARRICK
               <interp inst="t17920329-47-person503" type="surname" value="GARRICK"></interp>
                     <interp inst="t17920329-47-person503" type="given" value="JANE"></interp>
