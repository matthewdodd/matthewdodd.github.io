                  <persName id="t18230409-132-person1177">
                  SAMUEL 
                  LUSH
               <interp inst="t18230409-132-person1177" type="surname" value="LUSH"></interp>
                     <interp inst="t18230409-132-person1177" type="given" value="SAMUEL"></interp>
