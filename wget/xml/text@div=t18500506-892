                        <interp inst="t18500506-892-verdict-1" type="verdictSubcategory" value="no_subcategory"></interp>GUILTY</rs>
                  </hi>.—<hi rend="italic">
                     <rs id="t18500506-892-punishment-14" type="punishmentDescription">
