                        <interp inst="t18670923-889-verdict-1" type="verdictSubcategory" value="no_subcategory"></interp>GUILTY</rs>
                  </hi>.—<hi rend="italic">
                     <rs id="t18670923-889-punishment-34" type="punishmentDescription">
