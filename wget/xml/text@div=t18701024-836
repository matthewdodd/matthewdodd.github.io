                     <interp inst="def1-836-18701024" type="surname" value="KING"></interp>
                     <interp inst="def1-836-18701024" type="given" value="WILLIAM"></interp>
                     <hi rend="largeCaps">WILLIAM KING</hi> (21)</persName>, and <persName id="def2-836-18701024" type="defendantName">
                     <interp inst="def2-836-18701024" type="gender" value="male"></interp>
                     <interp inst="def2-836-18701024" type="age" value="21"></interp>
