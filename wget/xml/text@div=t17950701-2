               <join result="criminalCharge" id="t17950701-2-off7-c55" targOrder="Y" targets="t17950701-2-defend64 t17950701-2-off7 t17950701-2-verdict12"></join>
               <join result="criminalCharge" id="t17950701-2-off7-c56" targOrder="Y" targets="t17950701-2-defend66 t17950701-2-off7 t17950701-2-verdict12"></join>
               <join result="criminalCharge" id="t17950701-2-off7-c57" targOrder="Y" targets="t17950701-2-defend68 t17950701-2-off7 t17950701-2-verdict12"></join>
