            , 
               
                  <persName id="t17950701-2-defend64" type="defendantName">
                     SIMON 
                     JACOBS
                  <interp inst="t17950701-2-defend64" type="surname" value="JACOBS"></interp>
                     <interp inst="t17950701-2-defend64" type="given" value="SIMON"></interp>
