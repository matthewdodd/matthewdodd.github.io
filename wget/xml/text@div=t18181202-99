             and 
               
                  <persName id="t18181202-99-defend962" type="defendantName">
                     HENRY 
                     BERRY
                  <interp inst="t18181202-99-defend962" type="surname" value="BERRY"></interp>
                     <interp inst="t18181202-99-defend962" type="given" value="HENRY"></interp>
                     <interp inst="t18181202-99-defend962" type="gender" value="male"></interp>
                  </persName>
               
             were indicted for <rs id="t18181202-99-off550" type="offenceDescription">
                     <interp inst="t18181202-99-off550" type="offenceCategory" value="theft"></interp>
                     <interp inst="t18181202-99-off550" type="offenceSubcategory" value="grandLarceny"></interp>
               stealing, on the <rs id="t18181202-99-cd551" type="crimeDate">11th of November</rs>
                     <join result="offenceCrimeDate" targOrder="Y" targets="t18181202-99-off550 t18181202-99-cd551"></join>, 12 lbs of beef, value 5 s.
            </rs>, the property of 
               
                  <persName id="t18181202-99-victim964" type="victimName">
                     Gabriel 
                     Woodham
                  <interp inst="t18181202-99-victim964" type="surname" value="Woodham"></interp>
