               <p>
            
                  <persName id="t18190915-192-person1663">
                  ELEANOR 
                  HARRIS
               <interp inst="t18190915-192-person1663" type="surname" value="HARRIS"></interp>
                     <interp inst="t18190915-192-person1663" type="given" value="ELEANOR"></interp>
                     <interp inst="t18190915-192-person1663" type="gender" value="female"></interp>
