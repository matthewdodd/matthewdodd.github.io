                  <persName id="t18110918-73-person548">
                  SAMUEL 
                  NICKLIN
               <interp inst="t18110918-73-person548" type="surname" value="NICKLIN"></interp>
                     <interp inst="t18110918-73-person548" type="given" value="SAMUEL"></interp>
