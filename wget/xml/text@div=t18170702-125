               <p>1054. 
               
                  <persName id="t18170702-125-defend989" type="defendantName">
                     SARAH 
                     TRASSAM
                  <interp inst="t18170702-125-defend989" type="surname" value="TRASSAM"></interp>
                     <interp inst="t18170702-125-defend989" type="given" value="SARAH"></interp>
