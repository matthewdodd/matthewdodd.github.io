                     <hi rend="largeCaps">ROBERT BARTON</hi> (29)</persName> 
                  <hi rend="italic">and</hi> 
                  <persName id="def2-468-18750712" type="defendantName">
                     <interp inst="def2-468-18750712" type="gender" value="male"></interp>
                     <interp inst="def2-468-18750712" type="age" value="19"></interp>
