               <p>102. 
               <persName id="t18271206-102-defend627" type="defendantName">
                  JOHN 
                  SELWIN
               <interp inst="t18271206-102-defend627" type="surname" value="SELWIN"></interp>
                     <interp inst="t18271206-102-defend627" type="given" value="JOHN"></interp>
