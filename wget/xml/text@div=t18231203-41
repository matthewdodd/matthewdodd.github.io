                     <interp inst="t18231203-41-defend424" type="gender" value="male"></interp>
                     <interp inst="t18231203-41-defend424" type="age" value="51"></interp>
                  </persName>
               
             was indicted for <rs id="t18231203-41-off221" type="offenceDescription">
                     <interp inst="t18231203-41-off221" type="offenceCategory" value="theft"></interp>
                     <interp inst="t18231203-41-off221" type="offenceSubcategory" value="grandLarceny"></interp>
               stealing, on the <rs id="t18231203-41-cd222" type="crimeDate">25th of October</rs>
                     <join result="offenceCrimeDate" targOrder="Y" targets="t18231203-41-off221 t18231203-41-cd222"></join>, 10 lbs. of horse-hair, value 7 s., and a bag, value 4 s., the goods of 
                     
                        <persName id="t18231203-41-victim426" type="victimName">
                           Thomas 
                           Rhodes
                        <interp inst="t18231203-41-victim426" type="surname" value="Rhodes"></interp>
