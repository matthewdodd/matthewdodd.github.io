               <p>
            
                  <persName id="t18190915-216-victim1842" type="victimName">
                  
                     <persName id="t18190915-216-person1843" type="victimName">
                        RICHARD 
                        SELLERS 
                        YARNELL
                     <interp inst="t18190915-216-person1843" type="surname" value="SELLERS YARNELL"></interp>
