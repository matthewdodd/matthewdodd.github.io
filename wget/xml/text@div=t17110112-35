               <persName id="t17110112-35-person191">
                  Jane 
                  Smith
               <interp inst="t17110112-35-person191" type="surname" value="Smith"></interp>
                     <interp inst="t17110112-35-person191" type="given" value="Jane"></interp>
                     <interp inst="t17110112-35-person191" type="gender" value="female"></interp>
