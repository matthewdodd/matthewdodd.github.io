               <p>
            
                  <persName id="t18331017-11-person236">
                  John 
                  Richards
               <interp inst="t18331017-11-person236" type="surname" value="Richards"></interp>
                     <interp inst="t18331017-11-person236" type="given" value="John"></interp>
                     <interp inst="t18331017-11-person236" type="gender" value="male"></interp>
                  </persName>
            , shoemaker, Pelham-street, Brick-lane; and 
               <persName id="t18331017-11-person237">
                  John 
                  Whisker
               <interp inst="t18331017-11-person237" type="surname" value="Whisker"></interp>
