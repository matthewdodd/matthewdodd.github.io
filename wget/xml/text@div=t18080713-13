            . I <rs id="t18080713-13-viclabel82" type="occupation">keep a clothes shop</rs>
                  <join result="persNameOccupation" targOrder="Y" targets="t18080713-13-victim183 t18080713-13-viclabel82"></join> in <placeName id="t18080713-13-crimeloc83">Houndsditch</placeName>
