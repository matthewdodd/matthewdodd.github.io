                     <interp inst="t17831029-2-person114" type="gender" value="male"></interp>
                  </persName>
            , 
               <persName id="t17831029-2-person115">
                  FRANCES
                   WARREN
               <interp inst="t17831029-2-person115" type="surname" value="WARREN"></interp>
                     <interp inst="t17831029-2-person115" type="given" value="FRANCES"></interp>
