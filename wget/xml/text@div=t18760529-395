                     <hi rend="largeCaps">WILLIAM BARLOW</hi> (23)</persName>, <hi rend="italic">and</hi> 
                  <persName id="def3-395-18760529" type="defendantName">
                     <interp inst="def3-395-18760529" type="gender" value="male"></interp>
