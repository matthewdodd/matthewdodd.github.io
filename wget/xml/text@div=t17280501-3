                     <interp inst="t17280501-3-off11" type="offenceSubcategory" value="murder"></interp>
               Murder of 
                     
                        <persName id="t17280501-3-victim40" type="victimName">
                           Edward 
                           Bryan 
                        <interp inst="t17280501-3-victim40" type="surname" value="Bryan"></interp>
                        <interp inst="t17280501-3-victim40" type="given" value="Edward"></interp>
                        <interp inst="t17280501-3-victim40" type="gender" value="male"></interp>
                        <join result="offenceVictim" targOrder="Y" targets="t17280501-3-off11 t17280501-3-victim40"></join>
