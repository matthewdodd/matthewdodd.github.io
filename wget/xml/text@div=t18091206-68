               <p>67. 
               
                  <persName id="t18091206-68-defend597" type="defendantName">
                     ELIZABETH 
                     SMITH
                  <interp inst="t18091206-68-defend597" type="surname" value="SMITH"></interp>
                     <interp inst="t18091206-68-defend597" type="given" value="ELIZABETH"></interp>
