               <p>134. 
               
                  <persName id="t17940115-65-defend921" type="defendantName">
                     JAMES 
                     GAGE
                  <interp inst="t17940115-65-defend921" type="surname" value="GAGE"></interp>
                     <interp inst="t17940115-65-defend921" type="given" value="JAMES"></interp>
                     <interp inst="t17940115-65-defend921" type="gender" value="male"></interp>
