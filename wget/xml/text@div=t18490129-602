                  <hi rend="largeCaps">WRIGHT</hi> pleaded <hi rend="smallCaps">
                     <rs id="t18490129-602-verdict-1" type="verdictDescription">
                        <interp inst="t18490129-602-verdict-1" type="verdictCategory" value="guilty"></interp>
                        <interp inst="t18490129-602-verdict-1" type="verdictSubcategory" value="pleadedGuilty"></interp>GUILTY</rs>
                  </hi>.</p>
               <p>
                  <hi rend="largeCaps">CARTER</hi> pleaded <hi rend="smallCaps">
                     <rs id="t18490129-602-verdict-1" type="verdictDescription">
                        <interp inst="t18490129-602-verdict-1" type="verdictCategory" value="guilty"></interp>
