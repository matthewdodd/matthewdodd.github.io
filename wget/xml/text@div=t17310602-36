             of the Parish of <placeName id="t17310602-36-defloc174">Belfond</placeName>
                  <interp inst="t17310602-36-defloc174" type="placeName" value="Belfond"></interp>
                  <interp inst="t17310602-36-defloc174" type="type" value="defendantHome"></interp>
                  <join result="persNamePlace" targOrder="Y" targets="t17310602-36-defend243 t17310602-36-defloc174"></join>
                  <join result="persNamePlace" targOrder="Y" targets="t17310602-36-defend245 t17310602-36-defloc174"></join>, were indicted for <rs id="t17310602-36-off175" type="offenceDescription">
                     <interp inst="t17310602-36-off175" type="offenceCategory" value="violentTheft"></interp>
                     <interp inst="t17310602-36-off175" type="offenceSubcategory" value="highwayRobbery"></interp>
               assaulting 
                     
                        <persName id="t17310602-36-victim247" type="victimName">
                           Priscilla 
                           Scott 
                        <interp inst="t17310602-36-victim247" type="surname" value="Scott"></interp>
                        <interp inst="t17310602-36-victim247" type="given" value="Priscilla"></interp>
                        <interp inst="t17310602-36-victim247" type="gender" value="female"></interp>
                        <join result="offenceVictim" targOrder="Y" targets="t17310602-36-off175 t17310602-36-victim247"></join>
                     </persName>
                      
                   , <rs id="t17310602-36-viclabel176" type="occupation">Wife</rs>
                     <join result="persNameOccupation" targOrder="Y" targets="t17310602-36-victim247 t17310602-36-viclabel176"></join> of 
                     <persName id="t17310602-36-person248">
                        Edward 
                        Scott 
                     <interp inst="t17310602-36-person248" type="surname" value="Scott"></interp>
