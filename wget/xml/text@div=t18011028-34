                     <interp inst="t18011028-34-defend329" type="given" value="HENRY"></interp>
                     <interp inst="t18011028-34-defend329" type="gender" value="male"></interp>
                  </persName>
               
             was indicted for <rs id="t18011028-34-off182" type="offenceDescription">
                     <interp inst="t18011028-34-off182" type="offenceCategory" value="theft"></interp>
                     <interp inst="t18011028-34-off182" type="offenceSubcategory" value="grandLarceny"></interp>
               feloniously stealing, on the <rs id="t18011028-34-cd183" type="crimeDate">23d of September</rs>
                     <join result="offenceCrimeDate" targOrder="Y" targets="t18011028-34-off182 t18011028-34-cd183"></join>, four sacks, value 4s.
            </rs> the property of 
               
                  <persName id="t18011028-34-victim331" type="victimName">
                     George 
                     Aylin
                  <interp inst="t18011028-34-victim331" type="surname" value="Aylin"></interp>
