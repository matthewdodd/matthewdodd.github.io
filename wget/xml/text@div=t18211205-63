                     <join result="offenceCrimeDate" targOrder="Y" targets="t18211205-63-off389 t18211205-63-cd377"></join>, at <placeName id="t18211205-63-crimeloc378">St. George, Hanover-square</placeName>
                     <interp inst="t18211205-63-crimeloc378" type="placeName" value="St. George, Hanover-square"></interp>
                     <interp inst="t18211205-63-crimeloc378" type="type" value="crimeLocation"></interp>
