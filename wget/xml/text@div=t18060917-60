               <p>467. 
               
                  <persName id="t18060917-60-defend557" type="defendantName">
                     THOMAS 
                     MITCHELL
                  <interp inst="t18060917-60-defend557" type="surname" value="MITCHELL"></interp>
                     <interp inst="t18060917-60-defend557" type="given" value="THOMAS"></interp>
