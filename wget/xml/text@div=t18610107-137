                        <interp inst="t18610107-137-verdict-1" type="verdictSubcategory" value="no_subcategory"></interp>GUILTY</rs>
                  </hi>.—<hi rend="italic">
                     <rs id="t18610107-137-punishment-21" type="punishmentDescription">
                        <interp inst="t18610107-137-punishment-21" type="punishmentCategory" value="imprison"></interp>
