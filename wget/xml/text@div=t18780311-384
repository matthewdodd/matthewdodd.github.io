                        <interp inst="t18780311-384-verdict-1" type="verdictSubcategory" value="no_subcategory"></interp>GUILTY</rs>
                  </hi>.—<hi rend="italic">
                     <rs id="t18780311-384-punishment-54" type="punishmentDescription">
                        <interp inst="t18780311-384-punishment-54" type="punishmentCategory" value="imprison"></interp>
