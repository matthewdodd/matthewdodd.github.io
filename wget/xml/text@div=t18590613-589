                        <interp inst="t18590613-589-verdict-1" type="verdictSubcategory" value="no_subcategory"></interp>GUILTY</rs>
                  </hi>.—<hi rend="italic">
                     <rs id="t18590613-589-punishment-41" type="punishmentDescription">
                        <interp inst="t18590613-589-punishment-41" type="punishmentCategory" value="imprison"></interp>
