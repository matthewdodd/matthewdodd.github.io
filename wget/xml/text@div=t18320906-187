                     <interp inst="t18320906-187-off1053" type="offenceCategory" value="theft"></interp>
                     <interp inst="t18320906-187-off1053" type="offenceSubcategory" value="pocketpicking"></interp>
               stealing, on the <rs id="t18320906-187-cd1054" type="crimeDate">4th of September</rs>
                     <join result="offenceCrimeDate" targOrder="Y" targets="t18320906-187-off1053 t18320906-187-cd1054"></join>, 1 handkerchief, value 3s., the goods of 
                     
                        <persName id="t18320906-187-victim1577" type="victimName">
                           William 
                           Surridge
                        <interp inst="t18320906-187-victim1577" type="surname" value="Surridge"></interp>
