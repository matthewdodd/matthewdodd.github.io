                        <interp inst="t18641024-979-verdict-1" type="verdictSubcategory" value="no_subcategory"></interp>GUILTY</rs>
                  </hi>.—<hi rend="italic">
                     <rs id="t18641024-979-punishment-72" type="punishmentDescription">
                        <interp inst="t18641024-979-punishment-72" type="punishmentCategory" value="imprison"></interp>
