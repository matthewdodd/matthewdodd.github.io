               <p>1010. 
               <persName id="t18280410-219-defend1244" type="defendantName">
                  STEPHEN 
                  CABLE
               <interp inst="t18280410-219-defend1244" type="surname" value="CABLE"></interp>
                     <interp inst="t18280410-219-defend1244" type="given" value="STEPHEN"></interp>
