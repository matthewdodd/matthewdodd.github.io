            , and 
               
                  <persName id="t17780115-48-defend462" type="defendantName">
                     JAMES
                      GABEL
                  <interp inst="t17780115-48-defend462" type="surname" value="GABEL"></interp>
                     <interp inst="t17780115-48-defend462" type="given" value="JAMES"></interp>
                     <interp inst="t17780115-48-defend462" type="gender" value="male"></interp>
