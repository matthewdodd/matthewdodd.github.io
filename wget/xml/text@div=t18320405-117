            , and 
               
                  <persName id="t18320405-117-defend1094" type="defendantName">
                     FREDERICK 
                     COOPER
                  <interp inst="t18320405-117-defend1094" type="surname" value="COOPER"></interp>
                     <interp inst="t18320405-117-defend1094" type="given" value="FREDERICK"></interp>
