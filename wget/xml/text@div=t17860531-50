                  <persName id="t17860531-50-person626">
                  MARY
                   LARA
               <interp inst="t17860531-50-person626" type="surname" value="LARA"></interp>
                     <interp inst="t17860531-50-person626" type="given" value="MARY"></interp>
                     <interp inst="t17860531-50-person626" type="gender" value="female"></interp>
