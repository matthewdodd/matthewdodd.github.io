                  </persName>
               
            , and 
               
                  <persName id="t18090920-147-defend1433" type="defendantName">
                     JOHN 
                     BURTON
                  <interp inst="t18090920-147-defend1433" type="surname" value="BURTON"></interp>
                     <interp inst="t18090920-147-defend1433" type="given" value="JOHN"></interp>
                     <interp inst="t18090920-147-defend1433" type="gender" value="male"></interp>
