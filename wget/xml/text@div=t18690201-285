                        <interp inst="t18690201-285-verdict-1" type="verdictSubcategory" value="no_subcategory"></interp>GUILTY</rs>
                  </hi>.—<hi rend="italic">
                     <rs id="t18690201-285-punishment-36" type="punishmentDescription">
                        <interp inst="t18690201-285-punishment-36" type="punishmentCategory" value="imprison"></interp>
