                     <interp inst="t18470614-1341-offence-1" type="offenceCategory" value="theft"></interp>
                     <interp inst="t18470614-1341-offence-1" type="offenceSubcategory" value="stealingFromMaster"></interp> for stealing 1 needle-case, value 6<hi rend="italic">d</hi>., the goods of <persName id="t18470614-name-151" type="victimName">
                        <interp inst="t18470614-name-151" type="gender" value="female"></interp>
                        <interp inst="t18470614-name-151" type="surname" value="GLEGG"></interp>
                        <interp inst="t18470614-name-151" type="given" value="MARY ELIZABETH"></interp>
                        <join result="offenceVictim" targOrder="Y" targets="t18470614-1341-offence-1 t18470614-name-151"></join>Mary Elizabeth Glegg</persName>; 12 printed books, value 12<hi rend="italic">s</hi>.; and 2 yards of damask, 2<hi rend="italic">s</hi>.; the goods of <persName id="t18470614-name-152" type="victimName">
                        <interp inst="t18470614-name-152" type="gender" value="male"></interp>
                        <interp inst="t18470614-name-152" type="surname" value="GLEGG"></interp>
