                        <interp inst="t18500304-name-124" type="surname" value="BURGESS"></interp>
                        <interp inst="t18500304-name-124" type="given" value="JOSEPH"></interp>
                        <join result="offenceVictim" targOrder="Y" targets="t18500304-514-offence-1 t18500304-name-124"></join>Joseph Burgess</persName>.</rs>
