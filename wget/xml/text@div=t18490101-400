                     <interp inst="def1-400-18490101" type="given" value="WILLIAM"></interp>
                     <hi rend="largeCaps">WILLIAM BRYAN</hi>
                  </persName> and <persName id="def2-400-18490101" type="defendantName">
                     <interp inst="def2-400-18490101" type="gender" value="male"></interp>
                     <interp inst="def2-400-18490101" type="age" value="20"></interp>
                     <interp inst="def2-400-18490101" type="surname" value="RING"></interp>
                     <interp inst="def2-400-18490101" type="given" value="JOHN"></interp>
                     <hi rend="largeCaps">JOHN RING</hi>
                  </persName>
                  <rs id="t18490101-400-offence-1" type="offenceDescription">
                     <interp inst="t18490101-400-offence-1" type="offenceCategory" value="theft"></interp>
                     <interp inst="t18490101-400-offence-1" type="offenceSubcategory" value="simpleLarceny"></interp>, stealing 2 1/2 lbs. weight of lead, value 2 1/2 <hi rend="italic">d</hi>.; the goods of <persName id="t18490101-name-249" type="victimName">
                        <interp inst="t18490101-name-249" type="gender" value="male"></interp>
                        <interp inst="t18490101-name-249" type="surname" value="JOHNS"></interp>
                        <interp inst="t18490101-name-249" type="given" value="RICHARD"></interp>
                        <join result="offenceVictim" targOrder="Y" targets="t18490101-400-offence-1 t18490101-name-249"></join>Richard Johns</persName>; 35 lbs. weight of lead, value 2<hi rend="italic">s</hi>. 11<hi rend="italic">d</hi>.; the goods of <persName id="t18490101-name-250" type="victimName">
                        <interp inst="t18490101-name-250" type="gender" value="male"></interp>
                        <interp inst="t18490101-name-250" type="surname" value="ARMFIELD"></interp>
                        <interp inst="t18490101-name-250" type="given" value="GEORGE"></interp>
                        <join result="offenceVictim" targOrder="Y" targets="t18490101-400-offence-1 t18490101-name-250"></join>George Armfield</persName>; and 24 lbs. weight of lead, value 2<hi rend="italic">s</hi>.; the goods of <persName id="t18490101-name-251" type="victimName">
                        <interp inst="t18490101-name-251" type="gender" value="male"></interp>
