                        <interp inst="t18641212-111-verdict-1" type="verdictSubcategory" value="no_subcategory"></interp>GUILTY</rs>
                  </hi>.—<hi rend="italic">
                     <rs id="t18641212-111-punishment-27" type="punishmentDescription">
