<div1 type="trialAccount" id="t16921207-19">
               <interp inst="t16921207-19" type="collection" value="BAILEY"></interp>
               <interp inst="t16921207-19" type="year" value="1692"></interp>
               <interp inst="t16921207-19" type="uri" value="sessionsPapers/16921207"></interp>
