               <p>168. 
               
                  <persName id="t18331128-163-defend1642" type="defendantName">
                     ELIZABETH 
                     DIXON
                  <interp inst="t18331128-163-defend1642" type="surname" value="DIXON"></interp>
                     <interp inst="t18331128-163-defend1642" type="given" value="ELIZABETH"></interp>
                     <interp inst="t18331128-163-defend1642" type="gender" value="female"></interp>
                  </persName>
               
             was indicted for <rs id="t18331128-163-off908" type="offenceDescription">
                     <interp inst="t18331128-163-off908" type="offenceCategory" value="violentTheft"></interp>
                     <interp inst="t18331128-163-off908" type="offenceSubcategory" value="robbery"></interp>
               assaulting 
                     
                        <persName id="t18331128-163-victim1644" type="victimName">
                           John 
                           Crocker
                        <interp inst="t18331128-163-victim1644" type="surname" value="Crocker"></interp>
                        <interp inst="t18331128-163-victim1644" type="given" value="John"></interp>
