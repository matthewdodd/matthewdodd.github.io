            , and 
               
                  <persName id="t17321011-29-defend249" type="defendantName">
                     Amy 
                     Mason , alias <rs id="t17321011-29-alias-4" type="alias">
                        <join result="nameAlias" targOrder="Y" targets="t17321011-29-defend249 t17321011-29-alias-4"></join>Griffin</rs>
