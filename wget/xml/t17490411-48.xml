                     <join result="offenceVictim" targOrder="Y" targets="t17490411-48-off212 t17490411-48-victim315"></join>
                  </persName>
                
            , <rs id="t17490411-48-cd213" type="crimeDate">May 3</rs>
                  <join result="offenceCrimeDate" targOrder="Y" targets="t17490411-48-off212 t17490411-48-cd213"></join>.</p>
               <p>
                  <rs id="t17490411-48-verdict214" type="verdictDescription">
                     <interp inst="t17490411-48-verdict214" type="verdictCategory" value="guilty"></interp>
                     <interp inst="t17490411-48-verdict214" type="verdictSubcategory" value="theftunder1s"></interp>
               Guilty 10 d
            </rs>.</p>
               <p>
