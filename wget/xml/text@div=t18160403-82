                  </persName>
             and 
               <persName id="t18160403-82-defend685" type="defendantName">
                  CHARLOTTE 
                  WHITE-MAN
               <interp inst="t18160403-82-defend685" type="surname" value="WHITE-MAN"></interp>
                     <interp inst="t18160403-82-defend685" type="given" value="CHARLOTTE"></interp>
