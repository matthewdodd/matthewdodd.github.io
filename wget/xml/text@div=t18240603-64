             were indicted for <rs id="t18240603-64-off335" type="offenceDescription">
                     <interp inst="t18240603-64-off335" type="offenceCategory" value="violentTheft"></interp>
                     <interp inst="t18240603-64-off335" type="offenceSubcategory" value="highwayRobbery"></interp>
               feloniously assaulting 
                     <persName id="t18240603-64-victim572" type="victimName">
                        Elizabeth, the <rs id="t18240603-64-viclabel336" type="occupation">wife</rs>
                        <join result="persNameOccupation" targOrder="Y" targets="t18240603-64-victim572 t18240603-64-viclabel336"></join> of 
                              
                                 <persName id="t18240603-64-victim574" type="victimName">
                                    William 
                                    Arms
                                 <interp inst="t18240603-64-victim574" type="surname" value="Arms"></interp>
                           <interp inst="t18240603-64-victim574" type="given" value="William"></interp>
                           <interp inst="t18240603-64-victim574" type="gender" value="male"></interp>
                        </persName>
                              
                           
                        <interp inst="t18240603-64-victim572" type="surname" value="Arms"></interp>
                        <interp inst="t18240603-64-victim572" type="given" value="Elizabeth"></interp>
                        <interp inst="t18240603-64-victim572" type="gender" value="female"></interp>
