                  <persName id="t18221023-28-person365">
                  JOHN 
                  BEALE
               <interp inst="t18221023-28-person365" type="surname" value="BEALE"></interp>
                     <interp inst="t18221023-28-person365" type="given" value="JOHN"></interp>
                     <interp inst="t18221023-28-person365" type="gender" value="male"></interp>
