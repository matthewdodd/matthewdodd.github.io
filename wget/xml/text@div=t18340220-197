                  <persName id="t18340220-197-victim1842" type="victimName">
                     WILLIAM 
                     BRADBURY
                  <interp inst="t18340220-197-victim1842" type="surname" value="BRADBURY"></interp>
                     <interp inst="t18340220-197-victim1842" type="given" value="WILLIAM"></interp>
