               <p>
                  <xptr type="pageFacsimile" doc="177602210047"></xptr>
            
                  <persName id="t17760221-61-person598">
                  SAMUEL
                   RHODES
               <interp inst="t17760221-61-person598" type="surname" value="RHODES"></interp>
                     <interp inst="t17760221-61-person598" type="given" value="SAMUEL"></interp>
