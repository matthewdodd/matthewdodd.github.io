               <p>
                  <hi rend="largeCaps">SMITH</hi> 
                  <hi rend="smallCaps">
                     <rs id="t18610128-182-verdict-1" type="verdictDescription">
                        <interp inst="t18610128-182-verdict-1" type="verdictCategory" value="guilty"></interp>
                        <interp inst="t18610128-182-verdict-1" type="verdictSubcategory" value="pleadedGuilty"></interp>PLEADED GUILTY</rs>
                  </hi>.—<hi rend="italic">
                     <rs id="t18610128-182-punishment-22" type="punishmentDescription">
                        <interp inst="t18610128-182-punishment-22" type="punishmentCategory" value="imprison"></interp>
