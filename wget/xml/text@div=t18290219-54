                     <interp inst="t18290219-54-off288" type="offenceCategory" value="theft"></interp>
                     <interp inst="t18290219-54-off288" type="offenceSubcategory" value="simpleLarceny"></interp>
               stealing on the <rs id="t18290219-54-cd289" type="crimeDate">31st of January</rs>
                     <join result="offenceCrimeDate" targOrder="Y" targets="t18290219-54-off288 t18290219-54-cd289"></join>, 4 lbs. weight of bacon, value 2s.
            </rs> the goods of 
               
                  <persName id="t18290219-54-victim541" type="victimName">
                     Sarah 
                     Jones
                  <interp inst="t18290219-54-victim541" type="surname" value="Jones"></interp>
