                  <rs id="t18490101-391-offence-1" type="offenceDescription">
                     <interp inst="t18490101-391-offence-1" type="offenceCategory" value="theft"></interp>
                     <interp inst="t18490101-391-offence-1" type="offenceSubcategory" value="stealingFromMaster"></interp>, stealing 1 bottle, value 2<hi rend="italic">d</hi>., and 1 1/2 pints of gin, value 2<hi rend="italic">s</hi>. 6<hi rend="italic">d</hi>.; the goods of <persName id="t18490101-name-215" type="victimName">
                        <interp inst="t18490101-name-215" type="gender" value="male"></interp>
                        <interp inst="t18490101-name-215" type="surname" value="WIEKETT COHAM"></interp>
