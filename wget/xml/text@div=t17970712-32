                  <join result="persNameOccupation" targOrder="Y" targets="t17970712-32-victim355 t17970712-32-viclabel182"></join>
                  <join result="persNameOccupation" targOrder="Y" targets="t17970712-32-victim357 t17970712-32-viclabel182"></join>
