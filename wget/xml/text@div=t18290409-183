                     <interp inst="t18290409-183-defend1787" type="gender" value="male"></interp>
                     <interp inst="t18290409-183-defend1787" type="age" value="15"></interp>
                  </persName>
               
             was indicted for <rs id="t18290409-183-off1034" type="offenceDescription">
                     <interp inst="t18290409-183-off1034" type="offenceCategory" value="theft"></interp>
                     <interp inst="t18290409-183-off1034" type="offenceSubcategory" value="simpleLarceny"></interp>
               stealing, on the <rs id="t18290409-183-cd1035" type="crimeDate">9th of February</rs>
                     <join result="offenceCrimeDate" targOrder="Y" targets="t18290409-183-off1034 t18290409-183-cd1035"></join>, 2 quarts of beer, value 10d., and 1 cann, value 2s.
            </rs>, the goods of 
               
                  <persName id="t18290409-183-victim1789" type="victimName">
                     James 
                     Webb
                  <interp inst="t18290409-183-victim1789" type="surname" value="Webb"></interp>
