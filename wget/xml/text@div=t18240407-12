               <p>
            
                  <persName id="t18240407-12-person183">
                  GEORGE 
                  AUGUSTUS BURGESS
               <interp inst="t18240407-12-person183" type="surname" value="AUGUSTUS BURGESS"></interp>
                     <interp inst="t18240407-12-person183" type="given" value="GEORGE"></interp>
                     <interp inst="t18240407-12-person183" type="gender" value="male"></interp>
