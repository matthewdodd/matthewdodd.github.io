                     <hi rend="largeCaps">EDWIN JAMES MITCHELL</hi>
                  </persName>
                  <rs id="t18480103-505-offence-1" type="offenceDescription">
                     <interp inst="t18480103-505-offence-1" type="offenceCategory" value="theft"></interp>
                     <interp inst="t18480103-505-offence-1" type="offenceSubcategory" value="stealingFromMaster"></interp>, stealing 15 yards of satinet, value 2<hi rend="italic">l</hi>.; the goods of <persName id="t18480103-name-571" type="victimName">
                        <interp inst="t18480103-name-571" type="gender" value="male"></interp>
                        <interp inst="t18480103-name-571" type="surname" value="RUMBALL"></interp>
