            , of <placeName id="t17290226-35-defloc150">St. George's Hanover Square</placeName>
                  <interp inst="t17290226-35-defloc150" type="placeName" value="St. George's Hanover Square"></interp>
                  <interp inst="t17290226-35-defloc150" type="type" value="defendantHome"></interp>
                  <join result="persNamePlace" targOrder="Y" targets="t17290226-35-defend202 t17290226-35-defloc150"></join>
                  <join result="persNamePlace" targOrder="Y" targets="t17290226-35-defend204 t17290226-35-defloc150"></join>, were indicted for <rs id="t17290226-35-off151" type="offenceDescription">
                     <interp inst="t17290226-35-off151" type="offenceCategory" value="violentTheft"></interp>
                     <interp inst="t17290226-35-off151" type="offenceSubcategory" value="highwayRobbery"></interp>
               assaulting 
                     
                        <persName id="t17290226-35-victim206" type="victimName">
                           John 
                           Stout 
                        <interp inst="t17290226-35-victim206" type="surname" value="Stout"></interp>
