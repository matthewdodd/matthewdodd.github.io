               <interp inst="t18330905-124-person1292" type="surname" value="Lee"></interp>
                     <interp inst="t18330905-124-person1292" type="given" value="John"></interp>
                     <interp inst="t18330905-124-person1292" type="gender" value="male"></interp>
                  </persName>
            , omnibus driver; 
               <persName id="t18330905-124-person1293">
