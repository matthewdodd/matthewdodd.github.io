                  </persName>
            , Hunter's Arms, Compton-street; 
               <persName id="t18330905-124-person1297">
                  James 
                  Bound
               <interp inst="t18330905-124-person1297" type="surname" value="Bound"></interp>
                     <interp inst="t18330905-124-person1297" type="given" value="James"></interp>
