                  <persName id="def4-2358-18481023" type="defendantName">
                     <interp inst="def4-2358-18481023" type="gender" value="male"></interp>
                     <interp inst="def4-2358-18481023" type="surname" value="BUTLER"></interp>
                     <interp inst="def4-2358-18481023" type="given" value="ISAAC"></interp>
