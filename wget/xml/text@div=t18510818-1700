                        <join result="offenceVictim" targOrder="Y" targets="t18510818-1700-offence-1 t18510818-name-663"></join>Agatha Trann</persName>; and 2 spoons value 10<hi rend="italic">s</hi>.; the goods of <persName id="t18510818-name-664" type="victimName">
                        <interp inst="t18510818-name-664" type="gender" value="male"></interp>
                        <interp inst="t18510818-name-664" type="surname" value="RONGE"></interp>
