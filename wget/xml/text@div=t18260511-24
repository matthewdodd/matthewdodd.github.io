<div1 type="trialAccount" id="t18260511-24">
               <interp inst="t18260511-24" type="collection" value="BAILEY"></interp>
               <interp inst="t18260511-24" type="year" value="1826"></interp>
               <interp inst="t18260511-24" type="uri" value="sessionsPapers/18260511"></interp>
