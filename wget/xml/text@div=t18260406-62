               <p>638. 
               <persName id="t18260406-62-defend419" type="defendantName">
                  JAMES 
                  ASHBY, alias <rs id="t18260406-62-alias-3" type="alias">
                        <join result="nameAlias" targOrder="Y" targets="t18260406-62-defend419 t18260406-62-alias-3"></join>FISHER</rs>
                     <interp inst="t18260406-62-defend419" type="surname" value="ASHBY"></interp>
                     <interp inst="t18260406-62-defend419" type="given" value="JAMES"></interp>
