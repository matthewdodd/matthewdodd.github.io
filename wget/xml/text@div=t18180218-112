             and 
               
                  <persName id="t18180218-112-defend695" type="defendantName">
                     CHARLES 
                     NOSEWORTHY
                  <interp inst="t18180218-112-defend695" type="surname" value="NOSEWORTHY"></interp>
                     <interp inst="t18180218-112-defend695" type="given" value="CHARLES"></interp>
