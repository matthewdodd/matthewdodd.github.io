                  </persName>
               
            , and 
               <persName id="t17710410-34-defend401" type="defendantName">
                  Mary, the <rs id="t17710410-34-deflabel184" type="occupation">wife</rs>
                     <join result="persNameOccupation" targOrder="Y" targets="t17710410-34-defend401 t17710410-34-deflabel184"></join> of 
                     <persName id="t17710410-34-person402">
                        Joseph
                         Cove
                     <interp inst="t17710410-34-person402" type="surname" value="Cove"></interp>
                        <interp inst="t17710410-34-person402" type="given" value="Joseph"></interp>
