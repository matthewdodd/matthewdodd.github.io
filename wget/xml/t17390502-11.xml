                     <interp inst="t17390502-11-off52" type="offenceSubcategory" value="highwayRobbery"></interp>
               assaulting 
                     
                        <persName id="t17390502-11-victim101" type="victimName">
                           William 
                           Linton 
                        <interp inst="t17390502-11-victim101" type="surname" value="Linton"></interp>
                        <interp inst="t17390502-11-victim101" type="given" value="William"></interp>
                        <interp inst="t17390502-11-victim101" type="gender" value="male"></interp>
                     </persName>
                      
