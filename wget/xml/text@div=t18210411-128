                  </persName>
               
            , and 
               
                  <persName id="t18210411-128-defend1363" type="defendantName">
                     SAMUEL 
                     BARRETT
                  <interp inst="t18210411-128-defend1363" type="surname" value="BARRETT"></interp>
                     <interp inst="t18210411-128-defend1363" type="given" value="SAMUEL"></interp>
