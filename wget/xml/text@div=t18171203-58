                     <interp inst="t18171203-58-off295" type="offenceCategory" value="violentTheft"></interp>
                     <interp inst="t18171203-58-off295" type="offenceSubcategory" value="highwayRobbery"></interp>
               feloniously assaulting 
                     <persName id="t18171203-58-victim414" type="victimName">
                        Eliza 
                        Garrett
                     <interp inst="t18171203-58-victim414" type="surname" value="Garrett"></interp>
