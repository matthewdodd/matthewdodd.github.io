                     <join result="defendantPunishment" targOrder="Y" targets="t17830226-26-defend337 t17830226-26-punish150"></join>
               fined 1 s.
            </rs> and <rs id="t17830226-26-punish151" type="punishmentDescription">
                     <interp inst="t17830226-26-punish151" type="punishmentCategory" value="imprison"></interp>
