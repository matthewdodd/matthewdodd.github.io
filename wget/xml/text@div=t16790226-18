                        <rs id="t16790226-18-viclabel78" type="occupation">Scotch-Merchant</rs>
                        <join result="persNameOccupation" targOrder="Y" targets="t16790226-18-victim38 t16790226-18-viclabel78"></join>
                        <interp inst="t16790226-18-victim38" type="gender" value="male"></interp>
