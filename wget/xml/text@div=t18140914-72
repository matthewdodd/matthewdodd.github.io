               <p>
            
                  <persName id="t18140914-72-person744">
                  GEORGE 
                  SHAW
               <interp inst="t18140914-72-person744" type="surname" value="SHAW"></interp>
                     <interp inst="t18140914-72-person744" type="given" value="GEORGE"></interp>
