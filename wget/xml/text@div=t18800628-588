                        <interp inst="t18800628-588-verdict-1" type="verdictSubcategory" value="no_subcategory"></interp>GUILTY</rs>
                  </hi>.—<hi rend="italic">
                     <rs id="t18800628-588-punishment-8" type="punishmentDescription">
                        <interp inst="t18800628-588-punishment-8" type="punishmentCategory" value="imprison"></interp>
