               <join result="criminalCharge" id="t18540403-480-charge-2" targOrder="Y" targets="def2-480-18540403 t18540403-480-offence-1 t18540403-480-verdict-2"></join>
               <join result="criminalCharge" id="t18540403-480-charge-3" targOrder="Y" targets="def3-480-18540403 t18540403-480-offence-1 t18540403-480-verdict-3"></join>
