                        <interp inst="t18580104-188-verdict-1" type="verdictSubcategory" value="no_subcategory"></interp>GUILTY</rs>
                  </hi>.—<hi rend="italic">
                     <rs id="t18580104-188-punishment-31" type="punishmentDescription">
                        <interp inst="t18580104-188-punishment-31" type="punishmentCategory" value="imprison"></interp>
