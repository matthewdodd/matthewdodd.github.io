                     <join result="defendantPunishment" targOrder="Y" targets="t17960217-22-defend258 t17960217-22-punish134"></join>
               Publickly whipped
            </rs> and <rs id="t17960217-22-punish135" type="punishmentDescription">
                     <interp inst="t17960217-22-punish135" type="punishmentCategory" value="imprison"></interp>
