                     <interp inst="t18681123-17-verdict-1" type="verdictSubcategory" value="no_subcategory"></interp>
                     <hi rend="smallCaps">GUILTY</hi>
                  </rs>—<hi rend="italic">
                     <rs id="t18681123-17-punishment-17" type="punishmentDescription">
