                     <hi rend="smallCaps">GUILTY</hi>
                  </rs>.—<hi rend="largeCaps">DAWSON</hi> 
                  <hi rend="italic">and</hi> 
                  <hi rend="largeCaps">ERRINGTON</hi>—<rs id="t18730818-533-punishment-39" type="punishmentDescription">
