                  <join result="persNameOccupation" targOrder="Y" targets="t18210214-32-victim436 t18210214-32-viclabel211"></join> and 
               
                  <persName id="t18210214-32-defend438" type="defendantName">
                     EDWARD 
                     DRINKWATER
                  <interp inst="t18210214-32-defend438" type="surname" value="DRINKWATER"></interp>
