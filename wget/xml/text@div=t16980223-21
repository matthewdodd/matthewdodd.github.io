                     <join result="offenceCrimeDate" targOrder="Y" targets="t16980223-21-off112 t16980223-21-cd113"></join>, murder 
                     <persName id="t16980223-21-victim84" type="victimName">
                        William 
                        Fisher
                     <interp inst="t16980223-21-victim84" type="surname" value="Fisher"></interp>
