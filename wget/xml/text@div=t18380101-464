                     <hi rend="largeCaps">EDWARD READING</hi>
                  </persName> and <persName id="def2-464-18380101" type="defendantName">
                     <interp inst="def2-464-18380101" type="gender" value="male"></interp>
                     <interp inst="def2-464-18380101" type="age" value="21"></interp>
