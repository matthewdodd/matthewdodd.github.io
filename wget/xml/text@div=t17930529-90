               <p>
            
                  <persName id="t17930529-90-person1067">
                  ELIZABETH
                   CARPENTER
               <interp inst="t17930529-90-person1067" type="surname" value="CARPENTER"></interp>
                     <interp inst="t17930529-90-person1067" type="given" value="ELIZABETH"></interp>
                     <interp inst="t17930529-90-person1067" type="gender" value="female"></interp>
