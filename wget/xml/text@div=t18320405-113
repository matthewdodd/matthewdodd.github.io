                  <persName id="t18320405-113-person1058">
                  ELIZABETH 
                  FORSTER
               <interp inst="t18320405-113-person1058" type="surname" value="FORSTER"></interp>
                     <interp inst="t18320405-113-person1058" type="given" value="ELIZABETH"></interp>
                     <interp inst="t18320405-113-person1058" type="gender" value="female"></interp>
