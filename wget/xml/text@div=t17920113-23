            , 
               
                  <persName id="t17920113-23-victim315" type="victimName">
                     Thomas 
                     Pemberton
                  <interp inst="t17920113-23-victim315" type="surname" value="Pemberton"></interp>
                     <interp inst="t17920113-23-victim315" type="given" value="Thomas"></interp>
                     <interp inst="t17920113-23-victim315" type="gender" value="male"></interp>
                  </persName>
               
            , and 
               
                  <persName id="t17920113-23-victim317" type="victimName">
                     Thomas 
                     Johnson
