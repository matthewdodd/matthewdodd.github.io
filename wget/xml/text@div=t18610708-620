                        <interp inst="t18610708-620-verdict-1" type="verdictSubcategory" value="no_subcategory"></interp>GUILTY</rs>
                  </hi>.—<hi rend="italic">
                     <rs id="t18610708-620-punishment-59" type="punishmentDescription">
                        <interp inst="t18610708-620-punishment-59" type="punishmentCategory" value="imprison"></interp>
