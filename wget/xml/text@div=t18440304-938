                     <interp inst="t18440304-938-offence-1" type="offenceSubcategory" value="simpleLarceny"></interp> for stealing 4 waistcoats, value 3<hi rend="italic">s</hi>.; 2 razors, 1<hi rend="italic">s</hi>. 6<hi rend="italic">d</hi>.; 2 pairs of trowsers, 2<hi rend="italic">s</hi>.; 2 shirts, 2<hi rend="italic">s</hi>. 6<hi rend="italic">d</hi>.; 2 pairs of stockings, 4<hi rend="italic">d</hi>.; 2 night-caps, 1<hi rend="italic">s</hi>.; 1 jacket, 1<hi rend="italic">s</hi>.; 1 pair of drawers, 6<hi rend="italic">d</hi>.,; and 1/2lb. weight of soap, 2<hi rend="italic">d</hi>.; the goods of <persName id="t18440304-name-640" type="victimName">
                        <interp inst="t18440304-name-640" type="gender" value="male"></interp>
                        <interp inst="t18440304-name-640" type="surname" value="MILLER"></interp>
                        <interp inst="t18440304-name-640" type="given" value="CHARLES BENJAMIN"></interp>
                        <join result="offenceVictim" targOrder="Y" targets="t18440304-938-offence-1 t18440304-name-640"></join>Charles Benjamin Miller</persName>.</rs>
