            , and 
               
                  <persName id="t18220911-321-defend2835" type="defendantName">
                     SAMUEL 
                     MIDLAND
                  <interp inst="t18220911-321-defend2835" type="surname" value="MIDLAND"></interp>
                     <interp inst="t18220911-321-defend2835" type="given" value="SAMUEL"></interp>
