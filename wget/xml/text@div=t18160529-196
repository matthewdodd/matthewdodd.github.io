               <p>672. 
               
                  <persName id="t18160529-196-defend1582" type="defendantName">
                     RICHARD 
                     KING
                  <interp inst="t18160529-196-defend1582" type="surname" value="KING"></interp>
                     <interp inst="t18160529-196-defend1582" type="given" value="RICHARD"></interp>
