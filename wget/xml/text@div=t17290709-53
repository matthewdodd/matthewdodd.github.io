                     <interp inst="t17290709-53-verdict263" type="verdictCategory" value="guilty"></interp>
                     <interp inst="t17290709-53-verdict263" type="verdictSubcategory" value="manslaughter"></interp>
               guilty of Manslaughter
            </rs>, but <rs id="t17290709-53-verdict264" type="verdictDescription">
                     <interp inst="t17290709-53-verdict264" type="verdictCategory" value="notGuilty"></interp>
               acquitted
            </rs> 
            
