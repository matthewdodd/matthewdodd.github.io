                     <interp inst="t18100606-105-person831" type="gender" value="male"></interp>
                  </persName>
            , he is a <rs id="t18100606-105-viclabel586" type="occupation">grocer</rs>
                  <join result="persNameOccupation" targOrder="Y" targets="t18100606-105-victim830 t18100606-105-viclabel586"></join>, <placeName id="t18100606-105-crimeloc587">No. 3, Raven-row, Spital-fields</placeName>
                  <interp inst="t18100606-105-crimeloc587" type="placeName" value="No. 3, Raven-row, Spital-fields"></interp>
