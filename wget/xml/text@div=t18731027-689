                        <interp inst="t18731027-689-verdict-1" type="verdictSubcategory" value="no_subcategory"></interp>GUILTY</rs>
                  </hi>—<hi rend="italic">
                     <rs id="t18731027-689-punishment-64" type="punishmentDescription">
