                     <hi rend="largeCaps">WILLIAM JENNINGS</hi> (26)</persName>
                  <hi rend="italic">and</hi> 
                  <persName id="def2-273-18760626" type="defendantName">
                     <interp inst="def2-273-18760626" type="gender" value="female"></interp>
                     <interp inst="def2-273-18760626" type="age" value="21"></interp>
                     <interp inst="def2-273-18760626" type="surname" value="PEARCE"></interp>
                     <interp inst="def2-273-18760626" type="given" value="CHARLOTTE"></interp>
