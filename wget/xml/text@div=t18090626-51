            , and 
               
                  <persName id="t18090626-51-victim622" type="victimName">
                     Charles 
                     Blizard
                  <interp inst="t18090626-51-victim622" type="surname" value="Blizard"></interp>
                     <interp inst="t18090626-51-victim622" type="given" value="Charles"></interp>
                     <interp inst="t18090626-51-victim622" type="gender" value="male"></interp>
