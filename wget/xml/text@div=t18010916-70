                  <xptr type="pageFacsimile" doc="180109160048"></xptr>696. 
               <persName id="t18010916-70-defend521" type="defendantName">
                  
                     WILLIAM 
                     INGRAM
                  , alias <rs id="t18010916-70-alias-5" type="alias">
                        <join result="nameAlias" targOrder="Y" targets="t18010916-70-defend521 t18010916-70-alias-5"></join>EAGLE</rs>
                     <interp inst="t18010916-70-defend521" type="surname" value="INGRAM"></interp>
