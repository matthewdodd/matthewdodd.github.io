                     <interp inst="t18350921-2096-offence-1" type="offenceCategory" value="theft"></interp>
                     <interp inst="t18350921-2096-offence-1" type="offenceSubcategory" value="simpleLarceny"></interp> for stealing, on the <rs id="t18350921-cd-149" type="crimeDate">
                        <join result="offenceCrimeDate" targOrder="Y" targets="t18350921-2096-offence-1 t18350921-cd-149"></join>24th of August</rs>, 1 shawl, value 9<hi rend="italic">s</hi>.; 1 handerchief, value 1<hi rend="italic">s</hi>.; and 1 gown, value 4<hi rend="italic">s</hi>.; the goods of <persName id="t18350921-name-1047" type="victimName">
                        <interp inst="t18350921-name-1047" type="gender" value="female"></interp>
