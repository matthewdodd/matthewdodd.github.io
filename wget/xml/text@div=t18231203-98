                  <persName id="t18231203-98-person957">
                  FLORENCE 
                  SULLIVAN
               <interp inst="t18231203-98-person957" type="surname" value="SULLIVAN"></interp>
                     <interp inst="t18231203-98-person957" type="given" value="FLORENCE"></interp>
                     <interp inst="t18231203-98-person957" type="gender" value="female"></interp>
