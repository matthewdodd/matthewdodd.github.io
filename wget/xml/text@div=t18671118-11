                        <interp inst="t18671118-11-verdict-2" type="verdictSubcategory" value="no_subcategory"></interp>GUILTY</rs>
                  </hi>.**—<hi rend="italic">
                     <rs id="t18671118-11-punishment-12" type="punishmentDescription">
