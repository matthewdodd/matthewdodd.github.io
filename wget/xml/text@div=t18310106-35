                  ; 1 iron, value 10d., and 1 comb, value 2d., the goods of 
                     
                        <persName id="t18310106-35-victim387" type="victimName">
                           Daniel 
                           Scott
                        <interp inst="t18310106-35-victim387" type="surname" value="Scott"></interp>
