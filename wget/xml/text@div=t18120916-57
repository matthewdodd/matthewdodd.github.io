                  <persName id="t18120916-57-person493">
                  NATHANIEL 
                  JONES
               <interp inst="t18120916-57-person493" type="surname" value="JONES"></interp>
                     <interp inst="t18120916-57-person493" type="given" value="NATHANIEL"></interp>
