<div1 type="trialAccount" id="t16861208-18">
               <interp inst="t16861208-18" type="collection" value="BAILEY"></interp>
               <interp inst="t16861208-18" type="year" value="1686"></interp>
               <interp inst="t16861208-18" type="uri" value="sessionsPapers/16861208"></interp>
