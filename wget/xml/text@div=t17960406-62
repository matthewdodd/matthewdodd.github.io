               <p>288. 
               <persName id="t17960406-62-defend607" type="defendantName">
                  
                     SARAH 
                     SCOTT
                  , otherwise <rs id="t17960406-62-alias-4" type="alias">
                        <join result="nameAlias" targOrder="Y" targets="t17960406-62-defend607 t17960406-62-alias-4"></join>CHAND-LER</rs>
