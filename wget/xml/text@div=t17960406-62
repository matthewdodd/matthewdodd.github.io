                     </persName>
                  , and 
                     <persName id="t17960406-62-person613">
                        Gerard
                         Bailliant
                     <interp inst="t17960406-62-person613" type="surname" value="Bailliant"></interp>
                        <interp inst="t17960406-62-person613" type="given" value="Gerard"></interp>
