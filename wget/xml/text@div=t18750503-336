                     <interp inst="t18750503-name-209" type="given" value="CHARLES"></interp>Charles Nuth</persName>, <persName id="t18750503-name-210">
                     <interp inst="t18750503-name-210" type="gender" value="male"></interp>
                     <interp inst="t18750503-name-210" type="surname" value="PEARCE"></interp>
                     <interp inst="t18750503-name-210" type="given" value="JOHN"></interp>John Pearce</persName>, and "<persName id="t18750503-name-211">
                     <interp inst="t18750503-name-211" type="gender" value="male"></interp>
                     <interp inst="t18750503-name-211" type="surname" value="HUMPHRIES"></interp>
