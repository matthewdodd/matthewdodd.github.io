                        <interp inst="t18420228-name-617" type="occupation" value="carpenter"></interp>
                        <join result="offenceVictim" targOrder="Y" targets="t18420228-968-offence-1 t18420228-name-617"></join>John Collins</persName>: and 1 saw, value 2<hi rend="italic">s</hi>. 6<hi rend="italic">d</hi>.; and 2 planes, value 4<hi rend="italic">s</hi>.; the goods of <persName id="t18420228-name-618" type="victimName">
                        <interp inst="t18420228-name-618" type="gender" value="male"></interp>
                        <interp inst="t18420228-name-618" type="surname" value="STEPHENS"></interp>
