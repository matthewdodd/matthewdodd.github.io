               <p>1219. 
               
                  <persName id="t18161030-121-defend940" type="defendantName">
                     ELIZABETH 
                     BUNTING
                  <interp inst="t18161030-121-defend940" type="surname" value="BUNTING"></interp>
                     <interp inst="t18161030-121-defend940" type="given" value="ELIZABETH"></interp>
