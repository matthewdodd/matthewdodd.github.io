               <persName id="t18280110-205-defend1235" type="defendantName">
                  JOHN 
                  BEDFORD
               <interp inst="t18280110-205-defend1235" type="surname" value="BEDFORD"></interp>
                     <interp inst="t18280110-205-defend1235" type="given" value="JOHN"></interp>
