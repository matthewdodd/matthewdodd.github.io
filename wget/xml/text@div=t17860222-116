            , <rs id="t17860222-116-deflabel586" type="occupation">wife</rs>
                  <join result="persNameOccupation" targOrder="Y" targets="t17860222-116-defend1129 t17860222-116-deflabel586"></join> of 
               <persName id="t17860222-116-person1130">
                  Thomas
                   Meads
               <interp inst="t17860222-116-person1130" type="surname" value="Meads"></interp>
                     <interp inst="t17860222-116-person1130" type="given" value="Thomas"></interp>
                     <interp inst="t17860222-116-person1130" type="gender" value="male"></interp>
                  </persName>
            , was indicted for <rs id="t17860222-116-off587" type="offenceDescription">
                     <interp inst="t17860222-116-off587" type="offenceCategory" value="violentTheft"></interp>
                     <interp inst="t17860222-116-off587" type="offenceSubcategory" value="robbery"></interp>
               feloniously assaulting 
                     
                        <persName id="t17860222-116-victim1132" type="victimName">
                           William 
                           Moore
                        <interp inst="t17860222-116-victim1132" type="surname" value="Moore"></interp>
                        <interp inst="t17860222-116-victim1132" type="given" value="William"></interp>
