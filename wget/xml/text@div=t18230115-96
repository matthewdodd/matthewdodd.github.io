               <p>
            
                  <persName id="t18230115-96-person893">
                  JOHN 
                  BENNETT
               <interp inst="t18230115-96-person893" type="surname" value="BENNETT"></interp>
                     <interp inst="t18230115-96-person893" type="given" value="JOHN"></interp>
                     <interp inst="t18230115-96-person893" type="gender" value="male"></interp>
