               <p>
            
                  <persName id="t18320405-125-person1159">
                  GEORGE 
                  HODGKINSON
               <interp inst="t18320405-125-person1159" type="surname" value="HODGKINSON"></interp>
                     <interp inst="t18320405-125-person1159" type="given" value="GEORGE"></interp>
                     <interp inst="t18320405-125-person1159" type="gender" value="male"></interp>
