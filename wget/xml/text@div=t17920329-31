                     <interp inst="t17920329-31-defend341" type="gender" value="male"></interp>
                  </persName>
               
             was indicted for <rs id="t17920329-31-off172" type="offenceDescription">
                     <interp inst="t17920329-31-off172" type="offenceCategory" value="violentTheft"></interp>
                     <interp inst="t17920329-31-off172" type="offenceSubcategory" value="highwayRobbery"></interp>
               feloniously assaulting 
                     
                        <persName id="t17920329-31-victim343" type="victimName">
                           Edward 
                           Howarth
                        <interp inst="t17920329-31-victim343" type="surname" value="Howarth"></interp>
                        <interp inst="t17920329-31-victim343" type="given" value="Edward"></interp>
                        <interp inst="t17920329-31-victim343" type="gender" value="male"></interp>
                        <join result="offenceVictim" targOrder="Y" targets="t17920329-31-off172 t17920329-31-victim343"></join>
