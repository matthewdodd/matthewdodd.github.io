                        <join result="offenceVictim" targOrder="Y" targets="t18480515-1354-offence-1 t18480515-name-942"></join>John Loddy</persName>: and </rs>
                  <persName id="def2-1354-18480515" type="defendantName">
                     <interp inst="def2-1354-18480515" type="gender" value="male"></interp>
                     <interp inst="def2-1354-18480515" type="surname" value="SHIBLEY"></interp>
