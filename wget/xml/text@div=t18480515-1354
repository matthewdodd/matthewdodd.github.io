                     <interp inst="def1-1354-18480515" type="given" value="WILLIAM"></interp>
                     <hi rend="largeCaps">WILLIAM ROGERS</hi>
                  </persName>
                  <rs id="t18480515-1354-offence-1" type="offenceDescription">
                     <interp inst="t18480515-1354-offence-1" type="offenceCategory" value="theft"></interp>
                     <interp inst="t18480515-1354-offence-1" type="offenceSubcategory" value="simpleLarceny"></interp>, stealing 2 coats, value 2<hi rend="italic">l</hi>. 8<hi rend="italic">s</hi>.; and 1 scarf, 2<hi rend="italic">s</hi>.; the goods of <persName id="t18480515-name-942" type="victimName">
                        <interp inst="t18480515-name-942" type="gender" value="male"></interp>
                        <interp inst="t18480515-name-942" type="surname" value="LODDY"></interp>
