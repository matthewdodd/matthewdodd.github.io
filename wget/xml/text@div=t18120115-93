                  <persName id="t18120115-93-person944">
                  FRANCES 
                  HAGLEY
               <interp inst="t18120115-93-person944" type="surname" value="HAGLEY"></interp>
                     <interp inst="t18120115-93-person944" type="given" value="FRANCES"></interp>
