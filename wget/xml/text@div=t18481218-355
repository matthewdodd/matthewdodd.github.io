                     <interp inst="def1-355-18481218" type="given" value="CHARLES"></interp>
                     <hi rend="largeCaps">CHARLES MORGAN</hi>
                  </persName> and <persName id="def2-355-18481218" type="defendantName">
                     <interp inst="def2-355-18481218" type="gender" value="male"></interp>
                     <interp inst="def2-355-18481218" type="age" value="58"></interp>
