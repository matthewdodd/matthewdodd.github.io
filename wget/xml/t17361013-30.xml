                     Curtis 
                  <interp inst="t17361013-30-defend250" type="surname" value="Curtis"></interp>
                     <interp inst="t17361013-30-defend250" type="given" value="Richard"></interp>
                     <interp inst="t17361013-30-defend250" type="gender" value="male"></interp>
                  </persName>
                
             and 
               
                  <persName id="t17361013-30-defend252" type="defendantName">
                     Edward 
                     Joice 
                  <interp inst="t17361013-30-defend252" type="surname" value="Joice"></interp>
                     <interp inst="t17361013-30-defend252" type="given" value="Edward"></interp>
                     <interp inst="t17361013-30-defend252" type="gender" value="male"></interp>
                  </persName>
                
