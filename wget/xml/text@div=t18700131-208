                     <rs id="t18700131-208-verdict-1" type="verdictDescription">
                        <interp inst="t18700131-208-verdict-1" type="verdictCategory" value="guilty"></interp>
                        <interp inst="t18700131-208-verdict-1" type="verdictSubcategory" value="no_subcategory"></interp>GUILTY</rs>
                  </hi>*—<hi rend="italic">
                     <rs id="t18700131-208-punishment-19" type="punishmentDescription">
