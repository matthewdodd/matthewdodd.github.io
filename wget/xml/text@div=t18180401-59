               <p>588. 
               <persName id="t18180401-59-defend459" type="defendantName">
                  JOHN 
                  CONNER
               <interp inst="t18180401-59-defend459" type="surname" value="CONNER"></interp>
                     <interp inst="t18180401-59-defend459" type="given" value="JOHN"></interp>
