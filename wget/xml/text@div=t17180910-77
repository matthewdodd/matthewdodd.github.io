                  <join result="persNamePlace" targOrder="Y" targets="t17180910-77-defend375 t17180910-77-defloc383"></join>, was <rs id="t17180910-77-off384" type="offenceDescription">
                     <interp inst="t17180910-77-off384" type="offenceCategory" value="kill"></interp>
                     <interp inst="t17180910-77-off384" type="offenceSubcategory" value="murder"></interp>
               indicted for the Murther of his <rs id="t17180910-77-viclabel385" type="occupation">Wife</rs>
                     <join result="persNameOccupation" targOrder="Y" targets="t17180910-77-victim377 t17180910-77-viclabel385"></join> 
                  
                     
                        <persName id="t17180910-77-victim377" type="victimName">
                           Catherine 
                           Townsend 
                        <interp inst="t17180910-77-victim377" type="surname" value="Townsend"></interp>
                        <interp inst="t17180910-77-victim377" type="given" value="Catherine"></interp>
