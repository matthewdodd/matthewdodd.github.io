                     <interp inst="t18540130-287-offence-1" type="offenceCategory" value="theft"></interp>
                     <interp inst="t18540130-287-offence-1" type="offenceSubcategory" value="pocketpicking"></interp>, stealing 1 handkerchief, value 6<hi rend="italic">d</hi>.; the goods of <persName id="t18540130-name-271" type="victimName">
                        <interp inst="t18540130-name-271" type="gender" value="male"></interp>
                        <interp inst="t18540130-name-271" type="surname" value="HUCKS"></interp>
