            , Indicted for <rs id="t16840116-15-off69" type="offenceDescription">
                     <interp inst="t16840116-15-off69" type="offenceCategory" value="kill"></interp>
                     <interp inst="t16840116-15-off69" type="offenceSubcategory" value="other"></interp>
               Killing 
                     <persName id="t16840116-15-victim55" type="victimName">
                        Thomas 
                        Sydens
                     <interp inst="t16840116-15-victim55" type="surname" value="Sydens"></interp>
                        <interp inst="t16840116-15-victim55" type="given" value="Thomas"></interp>
                        <interp inst="t16840116-15-victim55" type="gender" value="male"></interp>
                        <join result="offenceVictim" targOrder="Y" targets="t16840116-15-off69 t16840116-15-victim55"></join>
