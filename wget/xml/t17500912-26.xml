            , was indicted for <rs id="t17500912-26-off125" type="offenceDescription">
                     <interp inst="t17500912-26-off125" type="offenceCategory" value="sexual"></interp>
                     <interp inst="t17500912-26-off125" type="offenceSubcategory" value="bigamy"></interp>
               marrying 
                     
                        <persName id="t17500912-26-victim202" type="victimName">
                           George 
                           Hutton 
                        <interp inst="t17500912-26-victim202" type="surname" value="Hutton"></interp>
