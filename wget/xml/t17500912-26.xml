               <p>531. 
               <persName id="t17500912-26-defend199" type="defendantName">
                  Mary
                  <rs id="t17500912-26-deflabel124" type="occupation">wife of 
                        <persName id="t17500912-26-person200">
                           Richard 
                           Clement 
                        <interp inst="t17500912-26-person200" type="surname" value="Clement"></interp>
                           <interp inst="t17500912-26-person200" type="given" value="Richard"></interp>
                           <interp inst="t17500912-26-person200" type="gender" value="male"></interp>
                        </persName>
                     
                     </rs>
                     <join result="persNameOccupation" targOrder="Y" targets="t17500912-26-defend199 t17500912-26-deflabel124"></join>
                     <interp inst="t17500912-26-defend199" type="surname" value="Clement"></interp>
                     <interp inst="t17500912-26-defend199" type="given" value="Mary"></interp>
                     <interp inst="t17500912-26-defend199" type="gender" value="female"></interp>
