             and 
               
                  <persName id="t18100718-29-defend306" type="defendantName">
                     LEWIS 
                     ROBINSON
                  <interp inst="t18100718-29-defend306" type="surname" value="ROBINSON"></interp>
                     <interp inst="t18100718-29-defend306" type="given" value="LEWIS"></interp>
                     <interp inst="t18100718-29-defend306" type="gender" value="male"></interp>
                     <interp inst="t18100718-29-defend306" type="age" value="66"></interp>
                  </persName>
               
             was indicted for <rs id="t18100718-29-off170" type="offenceDescription">
                     <interp inst="t18100718-29-off170" type="offenceCategory" value="theft"></interp>
                     <interp inst="t18100718-29-off170" type="offenceSubcategory" value="theftFromPlace"></interp>
               feloniously stealing on the <rs id="t18100718-29-cd171" type="crimeDate">4th of July</rs>
                     <join result="offenceCrimeDate" targOrder="Y" targets="t18100718-29-off170 t18100718-29-cd171"></join>, a sack, value 3 s. and one hundred and nine pound weight of flour, value 2 l. 10 s. the property of 
                     
                        <persName id="t18100718-29-victim308" type="victimName">
                           John 
                           Hill
                        <interp inst="t18100718-29-victim308" type="surname" value="Hill"></interp>
