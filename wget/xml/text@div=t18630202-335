                        <interp inst="t18630202-335-verdict-1" type="verdictSubcategory" value="no_subcategory"></interp>GUILTY</rs>
                  </hi>.—<hi rend="italic">
                     <rs id="t18630202-335-punishment-56" type="punishmentDescription">
                        <interp inst="t18630202-335-punishment-56" type="punishmentCategory" value="imprison"></interp>
