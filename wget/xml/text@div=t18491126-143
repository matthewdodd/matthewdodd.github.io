                  <rs id="t18491126-143-offence-1" type="offenceDescription">
                     <interp inst="t18491126-143-offence-1" type="offenceCategory" value="theft"></interp>
                     <interp inst="t18491126-143-offence-1" type="offenceSubcategory" value="theftFromPlace"></interp>, stealing 1 guard-chain, value 6<hi rend="italic">l</hi>. 6<hi rend="italic">s</hi>.; the goods of <persName id="t18491126-name-843" type="victimName">
                        <interp inst="t18491126-name-843" type="gender" value="male"></interp>
                        <interp inst="t18491126-name-843" type="surname" value="POSTLETHWAITE"></interp>
