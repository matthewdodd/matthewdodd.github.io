                  <hi rend="largeCaps">NELSON</hi> 
                  <hi rend="smallCaps">
                     <rs id="t18780527-569-verdict-1" type="verdictDescription">
                        <interp inst="t18780527-569-verdict-1" type="verdictCategory" value="guilty"></interp>
