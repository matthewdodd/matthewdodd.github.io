            , 
               <persName id="t17760911-42-person426">
                  Samuel
                   Whiting
               <interp inst="t17760911-42-person426" type="surname" value="Whiting"></interp>
                     <interp inst="t17760911-42-person426" type="given" value="Samuel"></interp>
