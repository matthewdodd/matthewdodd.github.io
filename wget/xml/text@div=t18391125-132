                        <join result="offenceVictim" targOrder="Y" targets="t18391125-132-offence-1 t18391125-name-685"></join>John Brooks Pain</persName>, his master; and </rs>
                  <persName id="def2-132-18391125" type="defendantName">
                     <interp inst="def2-132-18391125" type="gender" value="female"></interp>
                     <interp inst="def2-132-18391125" type="surname" value="COUPLAND"></interp>
