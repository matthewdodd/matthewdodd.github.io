               <p>
                  <hi rend="largeCaps">DALY</hi> 
                  <hi rend="smallCaps">
                     <rs id="t18770917-711-verdict-1" type="verdictDescription">
                        <interp inst="t18770917-711-verdict-1" type="verdictCategory" value="guilty"></interp>
                        <interp inst="t18770917-711-verdict-1" type="verdictSubcategory" value="pleadedGuilty"></interp>PLEADED GUILTY</rs>
