               <p>
            
                  <persName id="t18090920-49-person501">
                  SAMUEL 
                  DUTTON
               <interp inst="t18090920-49-person501" type="surname" value="DUTTON"></interp>
                     <interp inst="t18090920-49-person501" type="given" value="SAMUEL"></interp>
                     <interp inst="t18090920-49-person501" type="gender" value="male"></interp>
