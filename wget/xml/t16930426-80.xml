<div1 type="trialAccount" id="t16930426-80">
               <interp inst="t16930426-80" type="collection" value="BAILEY"></interp>
               <interp inst="t16930426-80" type="year" value="1693"></interp>
               <interp inst="t16930426-80" type="uri" value="sessionsPapers/16930426"></interp>
