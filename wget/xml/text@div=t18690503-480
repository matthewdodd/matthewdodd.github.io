               <p>
                  <hi rend="largeCaps">BARRY</hi> 
                  <hi rend="smallCaps">
                     <rs id="t18690503-480-verdict-1" type="verdictDescription">
                        <interp inst="t18690503-480-verdict-1" type="verdictCategory" value="guilty"></interp>
                        <interp inst="t18690503-480-verdict-1" type="verdictSubcategory" value="pleadedGuilty"></interp>PLEADED GUILTY</rs>
                  </hi>.—<hi rend="italic">
                     <rs id="t18690503-480-punishment-33" type="punishmentDescription">
                        <interp inst="t18690503-480-punishment-33" type="punishmentCategory" value="imprison"></interp>
