               <p>
            
                  <persName id="t18060917-72-person690">
                  GEORGE 
                  SMITH
               <interp inst="t18060917-72-person690" type="surname" value="SMITH"></interp>
                     <interp inst="t18060917-72-person690" type="given" value="GEORGE"></interp>
                     <interp inst="t18060917-72-person690" type="gender" value="male"></interp>
