                        <interp inst="t18790915-872-verdict-1" type="verdictSubcategory" value="no_subcategory"></interp>GUILTY</rs>
                  </hi>—<hi rend="italic">
                     <rs id="t18790915-872-punishment-99" type="punishmentDescription">
