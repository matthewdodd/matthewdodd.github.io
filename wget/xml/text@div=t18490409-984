               <p>
                  <hi rend="largeCaps">DEALY</hi> pleaded <hi rend="smallCaps">
                     <rs id="t18490409-984-verdict-1" type="verdictDescription">
                        <interp inst="t18490409-984-verdict-1" type="verdictCategory" value="guilty"></interp>
