<div1 type="trialAccount" id="t16900226-36">
               <interp inst="t16900226-36" type="collection" value="BAILEY"></interp>
               <interp inst="t16900226-36" type="year" value="1690"></interp>
               <interp inst="t16900226-36" type="uri" value="sessionsPapers/16900226"></interp>
