               <p>
                  <xptr type="pageFacsimile" doc="178001120035"></xptr>67. 
               <persName id="t17800112-21-defend296" type="defendantName">
                  JOHN 
                  KETTLEBY
               <interp inst="t17800112-21-defend296" type="surname" value="KETTLEBY"></interp>
