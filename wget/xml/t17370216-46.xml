                        <persName id="t17370216-46-victim297" type="victimName">
                           John 
                           Hayward 
                        <interp inst="t17370216-46-victim297" type="surname" value="Hayward"></interp>
                        <interp inst="t17370216-46-victim297" type="given" value="John"></interp>
                        <interp inst="t17370216-46-victim297" type="gender" value="male"></interp>
                     </persName>
                      
