                  <hi rend="largeCaps">KELLY</hi> 
                  <hi rend="smallCaps">
                     <rs id="t18751122-34-verdict-1" type="verdictDescription">
                        <interp inst="t18751122-34-verdict-1" type="verdictCategory" value="guilty"></interp>
