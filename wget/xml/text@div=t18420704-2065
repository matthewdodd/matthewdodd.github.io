                     <hi rend="largeCaps">SARAH ELIZABETH HORNBY</hi>, <hi rend="italic">alias<rs id="t18420704-alias-1" type="alias">
                           <join result="nameAlias" targOrder="Y" targets="def1-2065-18420704 t18420704-alias-1"></join> Plummer,</rs>
