                     <interp inst="def4-1388-18420404" type="gender" value="male"></interp>
                     <interp inst="def4-1388-18420404" type="surname" value="WINDLEY"></interp>
                     <interp inst="def4-1388-18420404" type="given" value="WILLIAM"></interp>
