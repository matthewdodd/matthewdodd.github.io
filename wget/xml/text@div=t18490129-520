                     <interp inst="t18490129-520-offence-1" type="offenceCategory" value="theft"></interp>
                     <interp inst="t18490129-520-offence-1" type="offenceSubcategory" value="simpleLarceny"></interp>, stealing 1 spoon, value 3<hi rend="italic">s</hi>.; the goods of <persName id="t18490129-name-474" type="victimName">
                        <interp inst="t18490129-name-474" type="gender" value="male"></interp>
                        <interp inst="t18490129-name-474" type="surname" value="MASON"></interp>
