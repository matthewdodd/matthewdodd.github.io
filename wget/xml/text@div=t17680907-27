                  <persName id="t17680907-27-person308">
                  Mary 
                  Spratley 
               <interp inst="t17680907-27-person308" type="surname" value="Spratley"></interp>
                     <interp inst="t17680907-27-person308" type="given" value="Mary"></interp>
