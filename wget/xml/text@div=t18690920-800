                     <hi rend="largeCaps">ANN SIMPSON</hi> (47)</persName>, and <persName id="def4-800-18690920" type="defendantName">
                     <interp inst="def4-800-18690920" type="gender" value="female"></interp>
                     <interp inst="def4-800-18690920" type="age" value="14"></interp>
