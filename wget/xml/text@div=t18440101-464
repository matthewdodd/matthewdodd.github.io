                     <interp inst="t18440101-464-offence-1" type="offenceSubcategory" value="simpleLarceny"></interp> for stealing 1 work-box, value 12<hi rend="italic">s</hi>.; one knife, 7<hi rend="italic">s</hi>.; 1 rule, 1<hi rend="italic">s</hi>.; 1 needle-case, 6<hi rend="italic">d</hi>.; 1 pencil-case, 2<hi rend="italic">s</hi>.; 1 cross, 3<hi rend="italic">s</hi>.; 1 necklace, 1<hi rend="italic">s</hi>. 6<hi rend="italic">d</hi>.; 2 brooches, 5<hi rend="italic">s</hi>.; two ear-rings, 4<hi rend="italic">s</hi>.; 1 breast-pin, 3<hi rend="italic">s</hi>.; 1 ring, 1<hi rend="italic">s</hi>.; 1 toothpick, 6<hi rend="italic">d</hi>.; and 1 bodkin, 1<hi rend="italic">s</hi>.; the goods of <persName id="t18440101-name-521" type="victimName">
                        <interp inst="t18440101-name-521" type="gender" value="female"></interp>
                        <interp inst="t18440101-name-521" type="surname" value="PALMER"></interp>
