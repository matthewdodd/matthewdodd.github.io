                  <persName id="t17760109-41-person397">
                  ROBERT
                   READ
               <interp inst="t17760109-41-person397" type="surname" value="READ"></interp>
                     <interp inst="t17760109-41-person397" type="given" value="ROBERT"></interp>
                     <interp inst="t17760109-41-person397" type="gender" value="male"></interp>
