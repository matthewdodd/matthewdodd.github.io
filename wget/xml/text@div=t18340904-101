                  </persName>
            ; and 
               <persName id="t18340904-101-defend621" type="defendantName">
                  CATHERINE 
                  ROGERS
               <interp inst="t18340904-101-defend621" type="surname" value="ROGERS"></interp>
                     <interp inst="t18340904-101-defend621" type="given" value="CATHERINE"></interp>
                     <interp inst="t18340904-101-defend621" type="gender" value="female"></interp>
