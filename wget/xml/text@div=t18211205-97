                     <join result="defendantPunishment" targOrder="Y" targets="t18211205-97-defend1135 t18211205-97-punish613"></join>
               Confined One Month
            </rs>, and <rs id="t18211205-97-punish614" type="punishmentDescription">
                     <interp inst="t18211205-97-punish614" type="punishmentCategory" value="corporal"></interp>
                     <interp inst="t18211205-97-punish614" type="punishmentSubcategory" value="whipping"></interp>
