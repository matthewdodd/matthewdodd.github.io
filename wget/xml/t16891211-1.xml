<div1 type="trialAccount" id="t16891211-1">
               <interp inst="t16891211-1" type="collection" value="BAILEY"></interp>
               <interp inst="t16891211-1" type="year" value="1689"></interp>
               <interp inst="t16891211-1" type="uri" value="sessionsPapers/16891211"></interp>
