                  <persName id="t17980110-51-person549" type="jurorName">
                     Thomas
                      Yorden
                  <interp inst="t17980110-51-person549" type="surname" value="Yorden"></interp>
                     <interp inst="t17980110-51-person549" type="given" value="Thomas"></interp>
                     <interp inst="t17980110-51-person549" type="gender" value="male"></interp>
