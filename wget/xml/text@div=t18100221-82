               <persName id="t18100221-82-person883">
                  William 
                  Smith
               <interp inst="t18100221-82-person883" type="surname" value="Smith"></interp>
                     <interp inst="t18100221-82-person883" type="given" value="William"></interp>
                     <interp inst="t18100221-82-person883" type="gender" value="male"></interp>
