                     <interp inst="t17370706-33-defend259" type="given" value="Mary"></interp>
                     <interp inst="t17370706-33-defend259" type="gender" value="female"></interp>
                  </persName>
               
            , was indicted for <rs id="t17370706-33-off162" type="offenceDescription">
                     <interp inst="t17370706-33-off162" type="offenceCategory" value="sexual"></interp>
                     <interp inst="t17370706-33-off162" type="offenceSubcategory" value="bigamy"></interp>
               marrying 
                     
                        <persName id="t17370706-33-victim261" type="victimName">
