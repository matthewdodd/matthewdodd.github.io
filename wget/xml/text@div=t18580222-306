                        <interp inst="t18580222-306-verdict-1" type="verdictSubcategory" value="no_subcategory"></interp>GUILTY</rs>
                  </hi>.—<hi rend="italic">
                     <rs id="t18580222-306-punishment-24" type="punishmentDescription">
                        <interp inst="t18580222-306-punishment-24" type="punishmentCategory" value="imprison"></interp>
