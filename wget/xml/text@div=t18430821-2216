                  <hi rend="smallCaps">
                     <persName id="t18430821-name-89" type="witnessName">
                        <interp inst="t18430821-name-89" type="gender" value="male"></interp>
                        <interp inst="t18430821-name-89" type="surname" value="ROBERTS"></interp>
                        <interp inst="t18430821-name-89" type="given" value="HUGH"></interp>HUGH ROBERTS</persName>
