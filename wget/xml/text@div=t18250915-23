               <p>1245. 
               <persName id="t18250915-23-defend221" type="defendantName">
                  WILLIAM 
                  MANNING
               <interp inst="t18250915-23-defend221" type="surname" value="MANNING"></interp>
                     <interp inst="t18250915-23-defend221" type="given" value="WILLIAM"></interp>
