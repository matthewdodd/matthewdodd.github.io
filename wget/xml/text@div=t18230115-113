             and 
               <persName id="t18230115-113-defend1020" type="defendantName">
                  HIRAM 
                  M'ADAMS
               <interp inst="t18230115-113-defend1020" type="surname" value="M'ADAMS"></interp>
                     <interp inst="t18230115-113-defend1020" type="given" value="HIRAM"></interp>
                     <interp inst="t18230115-113-defend1020" type="gender" value="male"></interp>
                  </persName>
             were indicted for <rs id="t18230115-113-off626" type="offenceDescription">
                     <interp inst="t18230115-113-off626" type="offenceCategory" value="theft"></interp>
                     <interp inst="t18230115-113-off626" type="offenceSubcategory" value="theftFromPlace"></interp>
               stealing, on the <rs id="t18230115-113-cd627" type="crimeDate">7th of December</rs>
                     <join result="offenceCrimeDate" targOrder="Y" targets="t18230115-113-off626 t18230115-113-cd627"></join>
                     <join result="offenceCrimeDate" targOrder="Y" targets="t18230115-113-off628 t18230115-113-cd627"></join>, 8 lbs. of lead, value 6 s., and a fixture, (i. e.); a brass cock, value 3 s., the goods of 
                     
                        <persName id="t18230115-113-victim1022" type="victimName">
                           Benjamin 
                           Chapman
                        <interp inst="t18230115-113-victim1022" type="surname" value="Chapman"></interp>
