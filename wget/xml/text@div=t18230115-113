                  <persName id="t18230115-113-defend1024" type="defendantName">
                     JOHN 
                     TUCK
                  <interp inst="t18230115-113-defend1024" type="surname" value="TUCK"></interp>
                     <interp inst="t18230115-113-defend1024" type="given" value="JOHN"></interp>
                     <interp inst="t18230115-113-defend1024" type="gender" value="male"></interp>
