<div1 type="trialAccount" id="t16910708-17">
               <interp inst="t16910708-17" type="collection" value="BAILEY"></interp>
               <interp inst="t16910708-17" type="year" value="1691"></interp>
               <interp inst="t16910708-17" type="uri" value="sessionsPapers/16910708"></interp>
