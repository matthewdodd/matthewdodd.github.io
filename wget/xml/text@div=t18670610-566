                     <join result="defendantPunishment" targOrder="Y" targets="def1-566-18670610 t18670610-566-punishment-37"></join>
                     <hi rend="italic">Ten Years' Penal Servitude.</hi>
                  </rs> 
                  <hi rend="largeCaps">WILLIAMS</hi>—<hi rend="smallCaps">
                     <rs id="t18670610-566-verdict-2" type="verdictDescription">
