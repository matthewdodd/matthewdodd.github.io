                     <interp inst="t18300114-140-victim967" type="gender" value="male"></interp>
                  </persName>
            . her master; and 
               
                  <persName id="t18300114-140-defend969" type="defendantName">
                     RALPH 
                     RAWLINS
                  <interp inst="t18300114-140-defend969" type="surname" value="RAWLINS"></interp>
