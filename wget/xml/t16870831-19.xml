               <p>
            
                  <persName id="t16870831-19-defend109" type="defendantName">
                  Elizabeth 
                  Green, alias <rs id="t16870831-19-alias-3" type="alias">
                        <join result="nameAlias" targOrder="Y" targets="t16870831-19-defend109 t16870831-19-alias-3"></join>Mount Joy</rs>
