               <p>
            
               
                  <persName id="t16870701-45-defend194" type="defendantName">
                     Edw 
                     Wyde
                  <interp inst="t16870701-45-defend194" type="surname" value="Wyde"></interp>
                     <interp inst="t16870701-45-defend194" type="given" value="Edw"></interp>
                     <interp inst="t16870701-45-defend194" type="gender" value="male"></interp>
                  </persName>
               
             of <placeName id="t16870701-45-defloc201">St. Andrews Holbourn</placeName>
