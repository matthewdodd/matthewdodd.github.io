                        <interp inst="t18781118-8-verdict-1" type="verdictSubcategory" value="no_subcategory"></interp>GUILTY</rs>
                  </hi>.**—<hi rend="italic">
                     <rs id="t18781118-8-punishment-6" type="punishmentDescription">
