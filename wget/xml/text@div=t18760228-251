                        <interp inst="t18760228-251-verdict-2" type="verdictSubcategory" value="no_subcategory"></interp>GUILTY</rs>
                  </hi>—<hi rend="italic">
                     <rs id="t18760228-251-punishment-49" type="punishmentDescription">
                        <interp inst="t18760228-251-punishment-49" type="punishmentCategory" value="imprison"></interp>
