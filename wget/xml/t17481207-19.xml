<div1 type="trialAccount" id="t17481207-19">
               <interp inst="t17481207-19" type="collection" value="BAILEY"></interp>
               <interp inst="t17481207-19" type="year" value="1748"></interp>
               <interp inst="t17481207-19" type="uri" value="sessionsPapers/17481207"></interp>
