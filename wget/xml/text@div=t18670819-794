               <p>
                  <hi rend="smallCaps">
                     <persName id="t18670819-name-456" type="witnessName">
                        <interp inst="t18670819-name-456" type="gender" value="indeterminate"></interp>
                        <interp inst="t18670819-name-456" type="surname" value="SLEIGH"></interp>
                        <interp inst="t18670819-name-456" type="given" value="WARNER"></interp>MR. WARNER SLEIGH</persName>
