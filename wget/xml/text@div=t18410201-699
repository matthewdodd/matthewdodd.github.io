                        <join result="offenceCrimeDate" targOrder="Y" targets="t18410201-699-offence-1 t18410201-cd-113"></join>2nd of January</rs>, 1 carpet, value 17<hi rend="italic">s</hi>.; 1 counterpane, value 5<hi rend="italic">s</hi>.; 3 blankets, value 1<hi rend="italic">l</hi>.; 1 sheet, value 3<hi rend="italic">s</hi>.; 2 candlesticks, value 5<hi rend="italic">s</hi>.; 2 pillows, value 6<hi rend="italic">s</hi>.; 2 pillow-cases, value 1<hi rend="italic">s</hi>.; 1 brush, value 10<hi rend="italic">d</hi>.; 1 table-cloth, value 1<hi rend="italic">s</hi>.; and 1 cloak, value 1<hi rend="italic">l</hi>. 6<hi rend="italic">s</hi>.; the goods of <persName id="t18410201-name-684" type="victimName">
                        <interp inst="t18410201-name-684" type="gender" value="female"></interp>
