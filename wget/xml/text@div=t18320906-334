                  <persName id="t18320906-334-person2773">
                  GEORGE 
                  ROBERT GALLOWAY
               <interp inst="t18320906-334-person2773" type="surname" value="ROBERT GALLOWAY"></interp>
                     <interp inst="t18320906-334-person2773" type="given" value="GEORGE"></interp>
