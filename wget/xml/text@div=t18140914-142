                  <persName id="t18140914-142-person1269">
                  WILLIAM 
                  DECKINS
               <interp inst="t18140914-142-person1269" type="surname" value="DECKINS"></interp>
                     <interp inst="t18140914-142-person1269" type="given" value="WILLIAM"></interp>
                     <interp inst="t18140914-142-person1269" type="gender" value="male"></interp>
