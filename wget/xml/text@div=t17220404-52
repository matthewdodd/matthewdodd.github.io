               <persName id="t17220404-52-person321">
                  Samuel 
                  Hicks 
               <interp inst="t17220404-52-person321" type="surname" value="Hicks"></interp>
                     <interp inst="t17220404-52-person321" type="given" value="Samuel"></interp>
