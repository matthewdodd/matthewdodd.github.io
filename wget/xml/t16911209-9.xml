               <p>
            
                  <persName id="t16911209-9-defend41" type="defendantName">
                  Jane 
                  Bridgeman
               <interp inst="t16911209-9-defend41" type="surname" value="Bridgeman"></interp>
                     <interp inst="t16911209-9-defend41" type="given" value="Jane"></interp>
                     <interp inst="t16911209-9-defend41" type="gender" value="female"></interp>
                  </persName>
             was Indicted for that she together with 
               <persName id="t16911209-9-person42">
                  Mary 
                  Rubey
               <interp inst="t16911209-9-person42" type="surname" value="Rubey"></interp>
                     <interp inst="t16911209-9-person42" type="given" value="Mary"></interp>
                     <interp inst="t16911209-9-person42" type="gender" value="female"></interp>
                  </persName>
            , 
               <persName id="t16911209-9-person43">
                  Mary 
                  Davies
               <interp inst="t16911209-9-person43" type="surname" value="Davies"></interp>
                     <interp inst="t16911209-9-person43" type="given" value="Mary"></interp>
                     <interp inst="t16911209-9-person43" type="gender" value="female"></interp>
                  </persName>
             and 
               <persName id="t16911209-9-person44">
                  Elizabeth 
                  Renolds
               <interp inst="t16911209-9-person44" type="surname" value="Renolds"></interp>
                     <interp inst="t16911209-9-person44" type="given" value="Elizabeth"></interp>
                     <interp inst="t16911209-9-person44" type="gender" value="female"></interp>
                  </persName>
             did <rs id="t16911209-9-off33" type="offenceDescription">
                     <interp inst="t16911209-9-off33" type="offenceCategory" value="violentTheft"></interp>
                     <interp inst="t16911209-9-off33" type="offenceSubcategory" value="highwayRobbery"></interp>
