                        <interp inst="t18620407-434-verdict-1" type="verdictSubcategory" value="no_subcategory"></interp>GUILTY</rs>
                  </hi>.—<hi rend="italic">
                     <rs id="t18620407-434-punishment-52" type="punishmentDescription">
                        <interp inst="t18620407-434-punishment-52" type="punishmentCategory" value="imprison"></interp>
