               <p>
                  <xptr type="pageFacsimile" doc="175510220014"></xptr>385. (L.) 
               
                  <persName id="t17551022-21-defend172" type="defendantName">
                     Edward 
                     Lambeth 
                  <interp inst="t17551022-21-defend172" type="surname" value="Lambeth"></interp>
                     <interp inst="t17551022-21-defend172" type="given" value="Edward"></interp>
                     <interp inst="t17551022-21-defend172" type="gender" value="male"></interp>
                  </persName>
               
            , <rs id="t17551022-21-deflabel109" type="occupation">bargeman</rs>
                  <join result="persNameOccupation" targOrder="Y" targets="t17551022-21-defend172 t17551022-21-deflabel109"></join>, <rs id="t17551022-21-off110" type="offenceDescription">
