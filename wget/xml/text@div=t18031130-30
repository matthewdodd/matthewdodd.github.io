                     <interp inst="t18031130-30-victim279" type="gender" value="male"></interp>
                  </persName>
               
            , and 
               
                  <persName id="t18031130-30-victim281" type="victimName">
                     Ephraim 
                     Burford
                  <interp inst="t18031130-30-victim281" type="surname" value="Burford"></interp>
