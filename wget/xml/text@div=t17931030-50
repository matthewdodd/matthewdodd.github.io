               <p>
            
               
                  <persName id="t17931030-50-defend520" type="defendantName">
                     EDWARD 
                     MOORE
                  <interp inst="t17931030-50-defend520" type="surname" value="MOORE"></interp>
                     <interp inst="t17931030-50-defend520" type="given" value="EDWARD"></interp>
