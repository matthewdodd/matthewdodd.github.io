                  </rs>, and 
               
                  <persName id="t18240715-111-defend1007" type="defendantName">
                     PHILIP 
                     BUCHANAN
                  <interp inst="t18240715-111-defend1007" type="surname" value="BUCHANAN"></interp>
                     <interp inst="t18240715-111-defend1007" type="given" value="PHILIP"></interp>
