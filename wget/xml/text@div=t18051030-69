                           <interp inst="t18051030-69-person807" type="gender" value="male"></interp>
                        </persName>
                        
                        <interp inst="t18051030-69-victim806" type="surname" value="Evans"></interp>
                        <interp inst="t18051030-69-victim806" type="given" value="Elizabeth"></interp>
