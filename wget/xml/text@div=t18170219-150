                  <persName id="t18170219-150-person1126">
                  ALICE 
                  TYZACK
               <interp inst="t18170219-150-person1126" type="surname" value="TYZACK"></interp>
                     <interp inst="t18170219-150-person1126" type="given" value="ALICE"></interp>
                     <interp inst="t18170219-150-person1126" type="gender" value="female"></interp>
