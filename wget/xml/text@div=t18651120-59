                        <interp inst="t18651120-59-verdict-1" type="verdictSubcategory" value="no_subcategory"></interp>GUILTY</rs>
                  </hi>.—<hi rend="italic">
                     <rs id="t18651120-59-punishment-50" type="punishmentDescription">
                        <interp inst="t18651120-59-punishment-50" type="punishmentCategory" value="imprison"></interp>
