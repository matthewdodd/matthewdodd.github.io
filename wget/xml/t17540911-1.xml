                     <interp inst="t17540911-1-person35" type="gender" value="male"></interp>
                  </persName>
            , 
               <persName id="t17540911-1-person36">
                  John 
                  Portrow 
               <interp inst="t17540911-1-person36" type="surname" value="Portrow"></interp>
                     <interp inst="t17540911-1-person36" type="given" value="John"></interp>
                     <interp inst="t17540911-1-person36" type="gender" value="male"></interp>
