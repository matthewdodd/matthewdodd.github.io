                  Jonathan 
                  Lovan 
               <interp inst="t17540911-1-person34" type="surname" value="Lovan"></interp>
                     <interp inst="t17540911-1-person34" type="given" value="Jonathan"></interp>
                     <interp inst="t17540911-1-person34" type="gender" value="male"></interp>
                  </persName>
            , 
               <persName id="t17540911-1-person35">
                  John 
                  Lefeaver 
               <interp inst="t17540911-1-person35" type="surname" value="Lefeaver"></interp>
