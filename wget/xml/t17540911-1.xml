               <interp inst="t17540911-1-person39" type="surname" value="Castle"></interp>
                     <interp inst="t17540911-1-person39" type="given" value="Ann"></interp>
                     <interp inst="t17540911-1-person39" type="gender" value="female"></interp>
                  </persName>
            , Badley Navaugh, and 
               <persName id="t17540911-1-person40">
                  Francis 
                  Cathel 
               <interp inst="t17540911-1-person40" type="surname" value="Cathel"></interp>
                     <interp inst="t17540911-1-person40" type="given" value="Francis"></interp>
                     <interp inst="t17540911-1-person40" type="gender" value="male"></interp>
