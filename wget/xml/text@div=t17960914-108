                     <join result="offenceCrimeDate" targOrder="Y" targets="t17960914-108-off605 t17960914-108-cd606"></join>, to one 
                     
                        <persName id="t17960914-108-victim1043" type="victimName">
                           West 
                           Adams
                        <interp inst="t17960914-108-victim1043" type="surname" value="Adams"></interp>
