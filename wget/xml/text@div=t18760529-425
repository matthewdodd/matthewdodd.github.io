                     <interp inst="def1-425-18760529" type="given" value="DANIEL"></interp>
                     <hi rend="largeCaps">DANIEL LYNCH</hi> (50)</persName>, <hi rend="italic">and</hi> 
                  <persName id="def2-425-18760529" type="defendantName">
                     <interp inst="def2-425-18760529" type="gender" value="male"></interp>
