                     <interp inst="def2-425-18760529" type="given" value="MICHAEL"></interp>
                     <hi rend="largeCaps">MICHAEL LYNCH</hi> (26)</persName>, <hi rend="italic">and</hi> 
                  <persName id="def3-425-18760529" type="defendantName">
                     <interp inst="def3-425-18760529" type="gender" value="male"></interp>
