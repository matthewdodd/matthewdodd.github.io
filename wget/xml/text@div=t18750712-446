                        <interp inst="t18750712-446-verdict-1" type="verdictSubcategory" value="no_subcategory"></interp>GUILTY</rs>
                  </hi>—<hi rend="italic">
                     <rs id="t18750712-446-punishment-1" type="punishmentDescription">
                        <interp inst="t18750712-446-punishment-1" type="punishmentCategory" value="imprison"></interp>
