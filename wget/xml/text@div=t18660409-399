                        <interp inst="t18660409-399-verdict-1" type="verdictSubcategory" value="no_subcategory"></interp>GUILTY</rs>
                  </hi>. <hi rend="italic">
                     <rs id="t18660409-399-punishment-66" type="punishmentDescription">
                        <interp inst="t18660409-399-punishment-66" type="punishmentCategory" value="imprison"></interp>
