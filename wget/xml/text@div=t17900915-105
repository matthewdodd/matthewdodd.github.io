               <p>
            
                  <persName id="t17900915-105-person1040">
                  ELIZABETH
                   GOODLUCK
               <interp inst="t17900915-105-person1040" type="surname" value="GOODLUCK"></interp>
                     <interp inst="t17900915-105-person1040" type="given" value="ELIZABETH"></interp>
