               <p>
            
                  <persName id="t16930426-71-defend205" type="defendantName">
                  William 
                  Martin
               <interp inst="t16930426-71-defend205" type="surname" value="Martin"></interp>
                     <interp inst="t16930426-71-defend205" type="given" value="William"></interp>
                     <interp inst="t16930426-71-defend205" type="gender" value="male"></interp>
                  </persName>
            , Charged for <rs id="t16930426-71-off316" type="offenceDescription">
                     <interp inst="t16930426-71-off316" type="offenceCategory" value="theft"></interp>
                     <interp inst="t16930426-71-off316" type="offenceSubcategory" value="grandLarceny"></interp>
               Robbing the Honourable the 
                     <persName id="t16930426-71-victim206" type="victimName">
                        <rs id="t16930426-71-viclabel317" type="occupation">Baroness</rs>
                        <join result="persNameOccupation" targOrder="Y" targets="t16930426-71-victim206 t16930426-71-viclabel317"></join> of Dartmouth<interp inst="t16930426-71-victim206" type="gender" value="female"></interp>
                        <join result="offenceVictim" targOrder="Y" targets="t16930426-71-off316 t16930426-71-victim206"></join>
                     </persName>
                   on the <rs id="t16930426-71-cd318" type="crimeDate">22d of January</rs>
                     <join result="offenceCrimeDate" targOrder="Y" targets="t16930426-71-off316 t16930426-71-cd318"></join> last of a great quantity of Plate of very great value
            </rs>; and 
               <persName id="t16930426-71-defend207" type="defendantName">
                  
                     <persName id="t16930426-71-defend208" type="defendantName">
                        John 
                        Abbot
                     <interp inst="t16930426-71-defend208" type="surname" value="Abbot"></interp>
                        <interp inst="t16930426-71-defend208" type="given" value="John"></interp>
                        <interp inst="t16930426-71-defend208" type="gender" value="male"></interp>
                     </persName>
