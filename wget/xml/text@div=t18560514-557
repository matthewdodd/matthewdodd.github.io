                     <interp inst="t18560514-557-offence-1" type="offenceCategory" value="theft"></interp>
                     <interp inst="t18560514-557-offence-1" type="offenceSubcategory" value="pocketpicking"></interp>, stealing 1 watch, value 6<hi rend="italic">l</hi>.; the goods of <persName id="t18560514-name-612" type="victimName">
                        <interp inst="t18560514-name-612" type="gender" value="male"></interp>
                        <interp inst="t18560514-name-612" type="surname" value="FEWINS"></interp>
