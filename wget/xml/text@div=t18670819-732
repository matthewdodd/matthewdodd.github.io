                     <hi rend="largeCaps">JOHN McCARTHY</hi> (24)</persName> and <persName id="def2-732-18670819" type="defendantName">
                     <interp inst="def2-732-18670819" type="gender" value="male"></interp>
                     <interp inst="def2-732-18670819" type="age" value="27"></interp>
