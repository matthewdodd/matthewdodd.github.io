               <p>
                  <xptr type="pageFacsimile" doc="180504240003"></xptr>220. 
               
                  <persName id="t18050424-1-defend52" type="defendantName">
                     JAMES 
                     NEAGLE
                  <interp inst="t18050424-1-defend52" type="surname" value="NEAGLE"></interp>
                     <interp inst="t18050424-1-defend52" type="given" value="JAMES"></interp>
