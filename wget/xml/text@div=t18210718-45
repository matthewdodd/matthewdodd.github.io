                     <interp inst="t18210718-45-off261" type="offenceCategory" value="theft"></interp>
                     <interp inst="t18210718-45-off261" type="offenceSubcategory" value="theftFromPlace"></interp>
               stealing, on the <rs id="t18210718-45-cd262" type="crimeDate">8th of July</rs>
                     <join result="offenceCrimeDate" targOrder="Y" targets="t18210718-45-off261 t18210718-45-cd262"></join>, at <placeName id="t18210718-45-crimeloc263">Ealing</placeName>
                     <interp inst="t18210718-45-crimeloc263" type="placeName" value="Ealing"></interp>
                     <interp inst="t18210718-45-crimeloc263" type="type" value="crimeLocation"></interp>
                     <join result="offencePlace" targOrder="Y" targets="t18210718-45-off261 t18210718-45-crimeloc263"></join>, in the dwelling-house of 
                     <persName id="t18210718-45-person438">
                        Jonathan 
                        Stevens
                     <interp inst="t18210718-45-person438" type="surname" value="Stevens"></interp>
                        <interp inst="t18210718-45-person438" type="given" value="Jonathan"></interp>
                        <interp inst="t18210718-45-person438" type="gender" value="male"></interp>
