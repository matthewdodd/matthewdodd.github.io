               <p>
                  <xptr type="pageFacsimile" doc="180806010064"></xptr>450. 
               
                  <persName id="t18080601-86-defend720" type="defendantName">
                     ANN 
                     COVEY
                  <interp inst="t18080601-86-defend720" type="surname" value="COVEY"></interp>
                     <interp inst="t18080601-86-defend720" type="given" value="ANN"></interp>
