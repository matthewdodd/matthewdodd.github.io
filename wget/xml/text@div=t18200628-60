                  </persName>
               
            , 
               
                  <persName id="t18200628-60-victim598" type="victimName">
                     William 
                     Loddiges
                  <interp inst="t18200628-60-victim598" type="surname" value="Loddiges"></interp>
                     <interp inst="t18200628-60-victim598" type="given" value="William"></interp>
                     <interp inst="t18200628-60-victim598" type="gender" value="male"></interp>
                  </persName>
               
            , and 
               
                  <persName id="t18200628-60-victim600" type="victimName">
