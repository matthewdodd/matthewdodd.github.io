               <p>1366. 
               <persName id="t18231022-65-defend620" type="defendantName">
                  GEORGE 
                  M'KNIGHT
               <interp inst="t18231022-65-defend620" type="surname" value="M'KNIGHT"></interp>
                     <interp inst="t18231022-65-defend620" type="given" value="GEORGE"></interp>
