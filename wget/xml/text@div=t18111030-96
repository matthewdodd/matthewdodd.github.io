                  <persName id="t18111030-96-person826">
                  JOHN 
                  BURTON
               <interp inst="t18111030-96-person826" type="surname" value="BURTON"></interp>
                     <interp inst="t18111030-96-person826" type="given" value="JOHN"></interp>
                     <interp inst="t18111030-96-person826" type="gender" value="male"></interp>
