                     <hi rend="largeCaps">JOHN LAURENCE</hi>
                  </persName> and <persName id="def2-580-18470201" type="defendantName">
                     <interp inst="def2-580-18470201" type="gender" value="male"></interp>
                     <interp inst="def2-580-18470201" type="age" value="17"></interp>
