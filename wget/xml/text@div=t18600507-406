                        <interp inst="t18600507-406-verdict-1" type="verdictSubcategory" value="no_subcategory"></interp>GUILTY</rs>
                  </hi> 
                  <hi rend="italic">
                     <rs id="t18600507-406-punishment-10" type="punishmentDescription">
                        <interp inst="t18600507-406-punishment-10" type="punishmentCategory" value="imprison"></interp>
