                        <interp inst="t18581025-974-verdict-1" type="verdictSubcategory" value="no_subcategory"></interp>GUILTY</rs>
                  </hi>.—<hi rend="italic">
                     <rs id="t18581025-974-punishment-39" type="punishmentDescription">
                        <interp inst="t18581025-974-punishment-39" type="punishmentCategory" value="imprison"></interp>
