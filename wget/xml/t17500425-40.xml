<div1 type="trialAccount" id="t17500425-40">
               <interp inst="t17500425-40" type="collection" value="BAILEY"></interp>
               <interp inst="t17500425-40" type="year" value="1750"></interp>
               <interp inst="t17500425-40" type="uri" value="sessionsPapers/17500425"></interp>
