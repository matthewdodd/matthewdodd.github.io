                  <join result="persNameOccupation" targOrder="Y" targets="t18320705-210-victim1763 t18320705-210-viclabel1101"></join>, and <rs id="t18320705-210-viclabel1102" type="occupation">keep a shop</rs>
                  <join result="persNameOccupation" targOrder="Y" targets="t18320705-210-victim1763 t18320705-210-viclabel1102"></join> in <placeName id="t18320705-210-crimeloc1103">Joseph-street, St. Pancras</placeName>
