            , of <placeName id="t17220907-35-defloc181">St. Margaret Westminster</placeName>
                  <interp inst="t17220907-35-defloc181" type="placeName" value="St. Margaret Westminster"></interp>
                  <interp inst="t17220907-35-defloc181" type="type" value="defendantHome"></interp>
                  <join result="persNamePlace" targOrder="Y" targets="t17220907-35-defend192 t17220907-35-defloc181"></join>
