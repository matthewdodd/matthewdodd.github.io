                     <interp inst="t17220907-35-off182" type="offenceCategory" value="kill"></interp>
                     <interp inst="t17220907-35-off182" type="offenceSubcategory" value="murder"></interp>
               feloniously Murder 
                     
                        <persName id="t17220907-35-victim196" type="victimName">
                           Peter 
                           Martin 
                        <interp inst="t17220907-35-victim196" type="surname" value="Martin"></interp>
