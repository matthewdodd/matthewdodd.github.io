             and 
               
                  <persName id="t18320405-291-defend2433" type="defendantName">
                     ISAAC 
                     LEVY
                  <interp inst="t18320405-291-defend2433" type="surname" value="LEVY"></interp>
                     <interp inst="t18320405-291-defend2433" type="given" value="ISAAC"></interp>
                     <interp inst="t18320405-291-defend2433" type="gender" value="male"></interp>
                     <interp inst="t18320405-291-defend2433" type="age" value="26"></interp>
