<div1 type="trialAccount" id="t16871207-30">
               <interp inst="t16871207-30" type="collection" value="BAILEY"></interp>
               <interp inst="t16871207-30" type="year" value="1687"></interp>
               <interp inst="t16871207-30" type="uri" value="sessionsPapers/16871207"></interp>
