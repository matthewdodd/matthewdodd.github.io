                     <interp inst="t18011028-14-victim156" type="gender" value="male"></interp>
                  </persName>
            , and 
               <persName id="t18011028-14-victim157" type="victimName">
                  Benjamin 
                  Gray
               <interp inst="t18011028-14-victim157" type="surname" value="Gray"></interp>
