                     <interp inst="t18351026-2135-offence-1" type="offenceCategory" value="theft"></interp>
                     <interp inst="t18351026-2135-offence-1" type="offenceSubcategory" value="stealingFromMaster"></interp>for stealing, on the <rs id="t18351026-cd-16" type="crimeDate">
                        <join result="offenceCrimeDate" targOrder="Y" targets="t18351026-2135-offence-1 t18351026-cd-16"></join>1st of Octo<lb></lb>ber</rs>, 2 blankets, value 9<hi rend="italic">s</hi>.; 2 quilts, value 16<hi rend="italic">s</hi>.; 2 sheets, value 4<hi rend="italic">s</hi>.; 2 table-cloths, value 3<hi rend="italic">s</hi>.; 3 gowns, value 7<hi rend="italic">s</hi>.; 1 shawl, value 7<hi rend="italic">s</hi>.; 1 waist<lb></lb>coat, value 4<hi rend="italic">s</hi>.; and 1 frock, value 1<hi rend="italic">s</hi>. 6<hi rend="italic">d</hi>.; the goods of <persName id="t18351026-name-163" type="victimName">
                        <interp inst="t18351026-name-163" type="gender" value="male"></interp>
                        <interp inst="t18351026-name-163" type="surname" value="DENHAM"></interp>
