                     <interp inst="def1-876-18620818" type="given" value="ELIZABETH"></interp>
                     <hi rend="largeCaps">ELIZABETH CONNOR</hi> (13)</persName>, <persName id="def2-876-18620818" type="defendantName">
                     <interp inst="def2-876-18620818" type="gender" value="male"></interp>
                     <interp inst="def2-876-18620818" type="age" value="50"></interp>
                     <interp inst="def2-876-18620818" type="surname" value="CONNOR"></interp>
                     <interp inst="def2-876-18620818" type="given" value="GEORGE"></interp>
                     <hi rend="largeCaps">GEORGE CONNOR</hi> (50)</persName>, <persName id="def3-876-18620818" type="defendantName">
                     <interp inst="def3-876-18620818" type="gender" value="male"></interp>
                     <interp inst="def3-876-18620818" type="age" value="25"></interp>
                     <interp inst="def3-876-18620818" type="surname" value="CONNOR"></interp>
                     <interp inst="def3-876-18620818" type="given" value="JOHN"></interp>
                     <hi rend="largeCaps">JOHN CONNOR</hi> (25)</persName>, and <persName id="def4-876-18620818" type="defendantName">
                     <interp inst="def4-876-18620818" type="gender" value="female"></interp>
                     <interp inst="def4-876-18620818" type="age" value="51"></interp>
