               <join result="criminalCharge" id="t16910422-32-off156-c118" targOrder="Y" targets="t16910422-32-defend118 t16910422-32-off156 t16910422-32-verdict159"></join>
               <join result="criminalCharge" id="t16910422-32-off156-c119" targOrder="Y" targets="t16910422-32-defend119 t16910422-32-off156 t16910422-32-verdict159"></join>
