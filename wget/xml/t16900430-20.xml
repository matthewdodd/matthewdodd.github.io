<div1 type="trialAccount" id="t16900430-20">
               <interp inst="t16900430-20" type="collection" value="BAILEY"></interp>
               <interp inst="t16900430-20" type="year" value="1690"></interp>
               <interp inst="t16900430-20" type="uri" value="sessionsPapers/16900430"></interp>
