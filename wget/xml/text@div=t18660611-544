               <p>544. <persName id="def1-544-18660611" type="defendantName">
                     <interp inst="def1-544-18660611" type="gender" value="male"></interp>
                     <interp inst="def1-544-18660611" type="age" value="19"></interp>
                     <interp inst="def1-544-18660611" type="surname" value="SMYRK"></interp>
                     <interp inst="def1-544-18660611" type="given" value="JOHN"></interp>
                     <hi rend="largeCaps">JOHN SMYRK</hi> (19)</persName>, and <persName id="def2-544-18660611" type="defendantName">
                     <interp inst="def2-544-18660611" type="gender" value="male"></interp>
                     <interp inst="def2-544-18660611" type="age" value="18"></interp>
