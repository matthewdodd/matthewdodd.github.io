                        <interp inst="t18461123-117-verdict-1" type="verdictSubcategory" value="no_subcategory"></interp>GUILTY</rs>
                  </hi>.—<hi rend="italic">
                     <rs id="t18461123-117-punishment-103" type="punishmentDescription">
