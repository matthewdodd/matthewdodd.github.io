             and 
               
                  <persName id="t18201206-86-defend854" type="defendantName">
                     FRANK 
                     PURDON
                  <interp inst="t18201206-86-defend854" type="surname" value="PURDON"></interp>
                     <interp inst="t18201206-86-defend854" type="given" value="FRANK"></interp>
                     <interp inst="t18201206-86-defend854" type="gender" value="male"></interp>
                  </persName>
               
             were indicted for <rs id="t18201206-86-off534" type="offenceDescription">
                     <interp inst="t18201206-86-off534" type="offenceCategory" value="theft"></interp>
                     <interp inst="t18201206-86-off534" type="offenceSubcategory" value="pocketpicking"></interp>
               stealing, from the person of 
                     
                        <persName id="t18201206-86-victim856" type="victimName">
                           Elias 
                           Levy
                        <interp inst="t18201206-86-victim856" type="surname" value="Levy"></interp>
