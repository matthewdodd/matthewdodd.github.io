               <p>387. 
               <persName id="t18160403-101-defend823" type="defendantName">
                  MARY 
                  SMITH, alias <rs id="t18160403-101-alias-8" type="alias">
                        <join result="nameAlias" targOrder="Y" targets="t18160403-101-defend823 t18160403-101-alias-8"></join>QUIN</rs>
                     <interp inst="t18160403-101-defend823" type="surname" value="SMITH"></interp>
                     <interp inst="t18160403-101-defend823" type="given" value="MARY"></interp>
