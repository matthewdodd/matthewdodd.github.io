                        <interp inst="t18780624-617-verdict-1" type="verdictSubcategory" value="no_subcategory"></interp>GUILTY</rs>
                  </hi>.—<hi rend="italic">
                     <rs id="t18780624-617-punishment-24" type="punishmentDescription">
                        <interp inst="t18780624-617-punishment-24" type="punishmentCategory" value="imprison"></interp>
