                     <interp inst="t17831029-46-defend727" type="gender" value="male"></interp>
                  </persName>
               
             was indicted for <rs id="t17831029-46-off248" type="offenceDescription">
                     <interp inst="t17831029-46-off248" type="offenceCategory" value="violentTheft"></interp>
                     <interp inst="t17831029-46-off248" type="offenceSubcategory" value="highwayRobbery"></interp>
               feloniously assaulting 
                     
                        <persName id="t17831029-46-victim729" type="victimName">
                           Lawrence 
                           Edmonds
                        <interp inst="t17831029-46-victim729" type="surname" value="Edmonds"></interp>
                        <interp inst="t17831029-46-victim729" type="given" value="Lawrence"></interp>
                        <interp inst="t17831029-46-victim729" type="gender" value="male"></interp>
                        <join result="offenceVictim" targOrder="Y" targets="t17831029-46-off248 t17831029-46-victim729"></join>
