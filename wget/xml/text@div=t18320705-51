                  <persName id="t18320705-51-person542">
                  ANN 
                  WARD
               <interp inst="t18320705-51-person542" type="surname" value="WARD"></interp>
                     <interp inst="t18320705-51-person542" type="given" value="ANN"></interp>
                     <interp inst="t18320705-51-person542" type="gender" value="female"></interp>
