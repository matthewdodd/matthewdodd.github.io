               <p>1300. 
               
                  <persName id="t18220911-157-defend1470" type="defendantName">FRANK PURDOM<interp inst="t18220911-157-defend1470" type="gender" value="male"></interp>
                     <interp inst="t18220911-157-defend1470" type="age" value="25"></interp>
                  </persName>
               
            , 
               <persName id="t18220911-157-defend1471" type="defendantName">
                  
                     WILLIAM 
                     CORBETT
                  , alias <rs id="t18220911-157-alias-2" type="alias">
