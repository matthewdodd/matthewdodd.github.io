               <p>
            
                  <persName id="t18320517-41-person585">
                  ELIZABETH 
                  CROSS
               <interp inst="t18320517-41-person585" type="surname" value="CROSS"></interp>
                     <interp inst="t18320517-41-person585" type="given" value="ELIZABETH"></interp>
