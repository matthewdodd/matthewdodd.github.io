                     <interp inst="t17620421-5-defend79" type="gender" value="male"></interp>
                  </persName>
               
            , <rs id="t17620421-5-deflabel21" type="occupation">widow</rs>
                  <join result="persNameOccupation" targOrder="Y" targets="t17620421-5-defend79 t17620421-5-deflabel21"></join>, was indicted for <rs id="t17620421-5-off22" type="offenceDescription">
