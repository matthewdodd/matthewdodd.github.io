                     <interp inst="t18381231-447-offence-1" type="offenceCategory" value="theft"></interp>
                     <interp inst="t18381231-447-offence-1" type="offenceSubcategory" value="simpleLarceny"></interp> stealing, on the <rs id="t18381231-cd-23" type="crimeDate">
                        <join result="offenceCrimeDate" targOrder="Y" targets="t18381231-447-offence-1 t18381231-cd-23"></join>21st of December</rs>, 1 pewter pot, value 18<hi rend="italic">d</hi>., the goods of <persName id="t18381231-name-194" type="victimName">
                        <interp inst="t18381231-name-194" type="gender" value="male"></interp>
                        <interp inst="t18381231-name-194" type="surname" value="RICH"></interp>
                        <interp inst="t18381231-name-194" type="given" value="SOLOMON"></interp>
                        <join result="offenceVictim" targOrder="Y" targets="t18381231-447-offence-1 t18381231-name-194"></join>Solomon Rich</persName>.</rs>
