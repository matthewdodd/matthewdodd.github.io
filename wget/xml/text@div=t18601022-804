               <join result="criminalCharge" id="t18601022-804-charge-1" targOrder="Y" targets="def1-804-18601022 t18601022-804-offence-1 t18601022-804-verdict-1"></join>
               <join result="criminalCharge" id="t18601022-804-charge-2" targOrder="Y" targets="def2-804-18601022 t18601022-804-offence-1 t18601022-804-verdict-2"></join>
