             and 
               
                  <persName id="t18191201-203-defend1931" type="defendantName">
                     ZACHARIAH 
                     CASSIMERE
                  <interp inst="t18191201-203-defend1931" type="surname" value="CASSIMERE"></interp>
                     <interp inst="t18191201-203-defend1931" type="given" value="ZACHARIAH"></interp>
