                        <interp inst="t18780527-578-verdict-1" type="verdictSubcategory" value="no_subcategory"></interp>GUILTY</rs>
                  </hi>.—<hi rend="italic">
                     <rs id="t18780527-578-punishment-30" type="punishmentDescription">
                        <interp inst="t18780527-578-punishment-30" type="punishmentCategory" value="imprison"></interp>
                        <interp inst="t18780527-578-punishment-30" type="punishmentSubcategory" value="no_subcategory"></interp>
