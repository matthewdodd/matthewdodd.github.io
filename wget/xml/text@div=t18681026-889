                        <interp inst="t18681026-889-verdict-1" type="verdictSubcategory" value="no_subcategory"></interp>GUILTY</rs>
                  </hi>.—<hi rend="italic">
                     <rs id="t18681026-889-punishment-12" type="punishmentDescription">
                        <interp inst="t18681026-889-punishment-12" type="punishmentCategory" value="imprison"></interp>
