                        <interp inst="t18590228-322-verdict-1" type="verdictSubcategory" value="no_subcategory"></interp>GUILTY</rs>
                  </hi>—<hi rend="italic">
                     <rs id="t18590228-322-punishment-27" type="punishmentDescription">
                        <interp inst="t18590228-322-punishment-27" type="punishmentCategory" value="imprison"></interp>
