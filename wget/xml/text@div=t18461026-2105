               <p>
                  <hi rend="smallCaps">
                     <persName id="t18461026-name-653" type="witnessName">
                        <interp inst="t18461026-name-653" type="gender" value="male"></interp>
                        <interp inst="t18461026-name-653" type="surname" value="GATCHELL"></interp>
                        <interp inst="t18461026-name-653" type="given" value="CHARLES"></interp>CHARLES GATCHELL</persName>
