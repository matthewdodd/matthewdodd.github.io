                     <interp inst="t17490411-36-defend234" type="given" value="Elizabeth"></interp>
                     <interp inst="t17490411-36-defend234" type="gender" value="female"></interp>
                  </persName>
               
            . <rs id="t17490411-36-deflabel160" type="occupation">widow</rs>
                  <join result="persNameOccupation" targOrder="Y" targets="t17490411-36-defend234 t17490411-36-deflabel160"></join>, was indicted <rs id="t17490411-36-off161" type="offenceDescription">
