                     <interp inst="t18260406-255-off1380" type="offenceCategory" value="theft"></interp>
                     <interp inst="t18260406-255-off1380" type="offenceSubcategory" value="grandLarceny"></interp>
               stealing, on the <rs id="t18260406-255-cd1381" type="crimeDate">1st of April</rs>
                     <join result="offenceCrimeDate" targOrder="Y" targets="t18260406-255-off1380 t18260406-255-cd1381"></join>, 20 yards of stair carpet, value 35s.
            </rs>, the goods of 
               <persName id="t18260406-255-victim1396" type="victimName">
                  George 
                  Chowles
               <interp inst="t18260406-255-victim1396" type="surname" value="Chowles"></interp>
                     <interp inst="t18260406-255-victim1396" type="given" value="George"></interp>
