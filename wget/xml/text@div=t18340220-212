                  , his master
            </rs>; and 
               
                  <persName id="t18340220-212-defend1941" type="defendantName">
                     ELIZABETH 
                     CLARK
                  <interp inst="t18340220-212-defend1941" type="surname" value="CLARK"></interp>
                     <interp inst="t18340220-212-defend1941" type="given" value="ELIZABETH"></interp>
                     <interp inst="t18340220-212-defend1941" type="gender" value="female"></interp>
