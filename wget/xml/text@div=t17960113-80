               <p>137. 
               
                  <persName id="t17960113-80-defend860" type="defendantName">
                     CHARLES 
                     NEWELL
                  <interp inst="t17960113-80-defend860" type="surname" value="NEWELL"></interp>
                     <interp inst="t17960113-80-defend860" type="given" value="CHARLES"></interp>
