               <p>
                  <hi rend="largeCaps">PERRY</hi> 
                  <hi rend="smallCaps">
                     <rs id="t18610225-221-verdict-1" type="verdictDescription">
                        <interp inst="t18610225-221-verdict-1" type="verdictCategory" value="guilty"></interp>
                        <interp inst="t18610225-221-verdict-1" type="verdictSubcategory" value="pleadedGuilty"></interp>PLEADED GUILTY</rs>
                  </hi>.*—<hi rend="italic">
                     <rs id="t18610225-221-punishment-9" type="punishmentDescription">
                        <interp inst="t18610225-221-punishment-9" type="punishmentCategory" value="imprison"></interp>
