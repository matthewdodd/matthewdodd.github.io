                     <interp inst="t18500708-1266-offence-1" type="offenceCategory" value="theft"></interp>
                     <interp inst="t18500708-1266-offence-1" type="offenceSubcategory" value="simpleLarceny"></interp>, stealing 1 brooch, value 2<hi rend="italic">s</hi>.; the goods of <persName id="t18500708-name-223" type="victimName">
                        <interp inst="t18500708-name-223" type="gender" value="female"></interp>
                        <interp inst="t18500708-name-223" type="surname" value="DOUGAL"></interp>
