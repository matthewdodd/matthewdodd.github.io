                        <interp inst="t18760110-101-verdict-1" type="verdictSubcategory" value="no_subcategory"></interp>GUILTY</rs>
                  </hi>—<hi rend="italic">
                     <rs id="t18760110-101-punishment-9" type="punishmentDescription">
                        <interp inst="t18760110-101-punishment-9" type="punishmentCategory" value="imprison"></interp>
