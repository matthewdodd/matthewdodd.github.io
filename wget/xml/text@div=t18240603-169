                  <persName id="t18240603-169-person1461">
                  SAMUEL 
                  WILLIAM COUSINS
               <interp inst="t18240603-169-person1461" type="surname" value="WILLIAM COUSINS"></interp>
                     <interp inst="t18240603-169-person1461" type="given" value="SAMUEL"></interp>
