               <p>
                  <xptr type="pageFacsimile" doc="177712030010"></xptr>15, 16. 
               
                  <persName id="t17771203-12-defend155" type="defendantName">
                     GEORGE
                      STEVENSON
                  <interp inst="t17771203-12-defend155" type="surname" value="STEVENSON"></interp>
