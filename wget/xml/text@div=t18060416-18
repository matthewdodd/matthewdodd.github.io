               <p>
            
                  <persName id="t18060416-18-person193">
                  THOMAS 
                  HALL
               <interp inst="t18060416-18-person193" type="surname" value="HALL"></interp>
                     <interp inst="t18060416-18-person193" type="given" value="THOMAS"></interp>
                     <interp inst="t18060416-18-person193" type="gender" value="male"></interp>
