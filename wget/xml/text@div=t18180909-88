               <p>
            
                  <persName id="t18180909-88-person764">
                  GEORGE 
                  GREEN
               <interp inst="t18180909-88-person764" type="surname" value="GREEN"></interp>
                     <interp inst="t18180909-88-person764" type="given" value="GEORGE"></interp>
                     <interp inst="t18180909-88-person764" type="gender" value="male"></interp>
