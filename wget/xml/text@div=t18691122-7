                        <interp inst="t18691122-7-verdict-1" type="verdictSubcategory" value="no_subcategory"></interp>GUILTY</rs>
                  </hi>—<hi rend="italic">
                     <rs id="t18691122-7-punishment-6" type="punishmentDescription">
                        <interp inst="t18691122-7-punishment-6" type="punishmentCategory" value="imprison"></interp>
