                     <hi rend="largeCaps">JOHN SHIRLEY</hi> (19)</persName>, and <persName id="def3-498-18800913" type="defendantName">
                     <interp inst="def3-498-18800913" type="gender" value="male"></interp>
                     <interp inst="def3-498-18800913" type="age" value="30"></interp>
