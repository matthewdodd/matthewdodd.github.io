                  <hi rend="largeCaps">CRAY</hi> pleaded <hi rend="smallCaps">
                     <rs id="t18510818-1714-verdict-1" type="verdictDescription">
                        <interp inst="t18510818-1714-verdict-1" type="verdictCategory" value="guilty"></interp>
