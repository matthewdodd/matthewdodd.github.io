                  <persName id="t18241028-96-person923">
                  THOMAS 
                  PETERS
               <interp inst="t18241028-96-person923" type="surname" value="PETERS"></interp>
                     <interp inst="t18241028-96-person923" type="given" value="THOMAS"></interp>
