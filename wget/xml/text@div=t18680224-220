                        <interp inst="t18680224-220-verdict-1" type="verdictSubcategory" value="no_subcategory"></interp>GUILTY</rs>
                  </hi>.—<hi rend="italic">

                     <rs id="t18680224-220-punishment-3" type="punishmentDescription">
                        <interp inst="t18680224-220-punishment-3" type="punishmentCategory" value="imprison"></interp>
