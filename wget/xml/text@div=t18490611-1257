                     <interp inst="t18490611-1257-offence-1" type="offenceCategory" value="theft"></interp>
                     <interp inst="t18490611-1257-offence-1" type="offenceSubcategory" value="pocketpicking"></interp>, stealing a watch, value 2<hi rend="italic">l</hi>.; 1 handkerchief, value 6<hi rend="italic">s</hi>., and other monies, 3<hi rend="italic">s</hi>. 2<hi rend="italic">d</hi>.; the property of <persName id="t18490611-name-284" type="victimName">
                        <interp inst="t18490611-name-284" type="gender" value="male"></interp>
                        <interp inst="t18490611-name-284" type="surname" value="ADAMS"></interp>
