                  <persName id="t18180909-15-person187">
                  MARY 
                  JOST
               <interp inst="t18180909-15-person187" type="surname" value="JOST"></interp>
                     <interp inst="t18180909-15-person187" type="given" value="MARY"></interp>
                     <interp inst="t18180909-15-person187" type="gender" value="female"></interp>
