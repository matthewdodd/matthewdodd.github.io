                     <join result="defendantPunishment" targOrder="Y" targets="t16890703-37-defend124 t16890703-37-punish170"></join>
                     <join result="defendantPunishment" targOrder="Y" targets="t16890703-37-defend125 t16890703-37-punish170"></join>
                     <join result="defendantPunishment" targOrder="Y" targets="t16890703-37-defend126 t16890703-37-punish170"></join>
               
                     <note>[Death. See summary.]</note>
               
                  </rs>
