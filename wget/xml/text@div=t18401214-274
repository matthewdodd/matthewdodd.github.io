                     <interp inst="def2-274-18401214" type="gender" value="female"></interp>
                     <interp inst="def2-274-18401214" type="surname" value="DALTON"></interp>
                     <interp inst="def2-274-18401214" type="given" value="CHARLOTTE"></interp>
                     <hi rend="largeCaps">CHARLOTTE DALTON</hi>
                  </persName> were indicted for<rs id="t18401214-274-offence-1" type="offenceDescription">
                     <interp inst="t18401214-274-offence-1" type="offenceCategory" value="theft"></interp>
                     <interp inst="t18401214-274-offence-1" type="offenceSubcategory" value="stealingFromMaster"></interp>stealing, on the <rs id="t18401214-cd-30" type="crimeDate">
                        <join result="offenceCrimeDate" targOrder="Y" targets="t18401214-274-offence-1 t18401214-cd-30"></join>31st of July</rs>, 1 table-cloth, value 4<hi rend="italic">l</hi>., and 2 spoons, value 2<hi rend="italic">l</hi>. 8<hi rend="italic">s</hi>.; the goods of <persName id="t18401214-name-206" type="victimName">
                        <interp inst="t18401214-name-206" type="gender" value="male"></interp>
                        <interp inst="t18401214-name-206" type="surname" value="BALDACCONI"></interp>
