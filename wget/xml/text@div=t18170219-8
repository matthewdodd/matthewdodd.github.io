               <p>370. 
               
                  <persName id="t18170219-8-defend138" type="defendantName">
                     MARY 
                     DICKINSON
                  <interp inst="t18170219-8-defend138" type="surname" value="DICKINSON"></interp>
                     <interp inst="t18170219-8-defend138" type="given" value="MARY"></interp>
