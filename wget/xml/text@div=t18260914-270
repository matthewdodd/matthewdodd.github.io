                     <interp inst="t18260914-270-off1533" type="offenceCategory" value="theft"></interp>
                     <interp inst="t18260914-270-off1533" type="offenceSubcategory" value="grandLarceny"></interp>
               stealing, on the <rs id="t18260914-270-cd1534" type="crimeDate">31st of July</rs>
                     <join result="offenceCrimeDate" targOrder="Y" targets="t18260914-270-off1533 t18260914-270-cd1534"></join>, 2 chairs, value 5s.
            </rs>, the goods of 
               <persName id="t18260914-270-victim1474" type="victimName">
                  William 
                  Taylor
               <interp inst="t18260914-270-victim1474" type="surname" value="Taylor"></interp>
