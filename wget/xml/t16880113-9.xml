                     <interp inst="t16880113-9-off41" type="offenceCategory" value="theft"></interp>
                     <interp inst="t16880113-9-off41" type="offenceSubcategory" value="animalTheft"></interp>
               stealing from 
                     
                        <persName id="t16880113-9-victim46" type="victimName">
