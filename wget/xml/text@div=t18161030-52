                  <persName id="t18161030-52-victim485" type="victimName">
                     CHARLES 
                     WING
                  <interp inst="t18161030-52-victim485" type="surname" value="WING"></interp>
                     <interp inst="t18161030-52-victim485" type="given" value="CHARLES"></interp>
