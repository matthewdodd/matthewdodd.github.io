<div1 type="trialAccount" id="t17350702-13">
               <interp inst="t17350702-13" type="collection" value="BAILEY"></interp>
               <interp inst="t17350702-13" type="year" value="1735"></interp>
               <interp inst="t17350702-13" type="uri" value="sessionsPapers/17350702"></interp>
