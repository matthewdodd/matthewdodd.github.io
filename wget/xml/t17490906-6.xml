<div1 type="trialAccount" id="t17490906-6">
               <interp inst="t17490906-6" type="collection" value="BAILEY"></interp>
               <interp inst="t17490906-6" type="year" value="1749"></interp>
               <interp inst="t17490906-6" type="uri" value="sessionsPapers/17490906"></interp>
