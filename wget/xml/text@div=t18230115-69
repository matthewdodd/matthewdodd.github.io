            . I <rs id="t18230115-69-viclabel394" type="occupation">keep a chandler's shop</rs>
                  <join result="persNameOccupation" targOrder="Y" targets="t18230115-69-victim707 t18230115-69-viclabel394"></join>, in <placeName id="t18230115-69-crimeloc395">Long-court, Leicester-fields</placeName>
