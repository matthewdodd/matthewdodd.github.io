             and 
               
                  <persName id="t18180617-155-defend1286" type="defendantName">
                     JOHN 
                     NEWTON
                  <interp inst="t18180617-155-defend1286" type="surname" value="NEWTON"></interp>
                     <interp inst="t18180617-155-defend1286" type="given" value="JOHN"></interp>
