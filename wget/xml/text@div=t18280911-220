                  <persName id="t18280911-220-victim1261" type="victimName">
                  CHARLES FREDERICK 
                  GREEN
               <interp inst="t18280911-220-victim1261" type="surname" value="GREEN"></interp>
                     <interp inst="t18280911-220-victim1261" type="given" value="CHARLES FREDERICK"></interp>
                     <interp inst="t18280911-220-victim1261" type="gender" value="male"></interp>
                     <join result="offenceVictim" targOrder="Y" targets="t18280911-220-off1124 t18280911-220-victim1261"></join>
