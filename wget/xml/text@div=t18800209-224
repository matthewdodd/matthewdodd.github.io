                        <interp inst="t18800209-224-verdict-1" type="verdictSubcategory" value="no_subcategory"></interp>GUILTY</rs>
                  </hi>.—<hi rend="italic">
                     <rs id="t18800209-224-punishment-35" type="punishmentDescription">
                        <interp inst="t18800209-224-punishment-35" type="punishmentCategory" value="imprison"></interp>
