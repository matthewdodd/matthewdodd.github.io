                  <rs id="t18790428-511-offence-1" type="offenceDescription">
                     <interp inst="t18790428-511-offence-1" type="offenceCategory" value="violentTheft"></interp>
                     <interp inst="t18790428-511-offence-1" type="offenceSubcategory" value="robbery"></interp>, Robbery on <persName id="t18790428-name-451" type="victimName">
                        <interp inst="t18790428-name-451" type="gender" value="male"></interp>
                        <interp inst="t18790428-name-451" type="surname" value="SULLIVAN"></interp>
                        <interp inst="t18790428-name-451" type="given" value="FLORENCE"></interp>
