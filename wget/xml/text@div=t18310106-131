                     <interp inst="t18310106-131-defend1172" type="given" value="JOHN"></interp>
                     <interp inst="t18310106-131-defend1172" type="gender" value="male"></interp>
                  </persName>
               
             was indicted for <rs id="t18310106-131-off746" type="offenceDescription">
                     <interp inst="t18310106-131-off746" type="offenceCategory" value="theft"></interp>
                     <interp inst="t18310106-131-off746" type="offenceSubcategory" value="simpleLarceny"></interp>
               stealing, on the <rs id="t18310106-131-cd747" type="crimeDate">1st of January</rs>
                     <join result="offenceCrimeDate" targOrder="Y" targets="t18310106-131-off746 t18310106-131-cd747"></join>, 1 coat, value 5s.
            </rs>, the goods of 
               
                  <persName id="t18310106-131-victim1174" type="victimName">
                     James 
                     Kennedy
                  <interp inst="t18310106-131-victim1174" type="surname" value="Kennedy"></interp>
                     <interp inst="t18310106-131-victim1174" type="given" value="James"></interp>
                     <interp inst="t18310106-131-victim1174" type="gender" value="male"></interp>
