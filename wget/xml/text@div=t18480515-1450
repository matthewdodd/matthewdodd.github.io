                  <rs id="t18480515-1450-offence-1" type="offenceDescription">
                     <interp inst="t18480515-1450-offence-1" type="offenceCategory" value="theft"></interp>
                     <interp inst="t18480515-1450-offence-1" type="offenceSubcategory" value="pocketpicking"></interp>, stealing 1 handkerchief, value 3<hi rend="italic">s</hi>.; the goods of <persName id="t18480515-name-1470" type="victimName">
                        <interp inst="t18480515-name-1470" type="gender" value="male"></interp>
                        <interp inst="t18480515-name-1470" type="surname" value="JOHNS"></interp>
