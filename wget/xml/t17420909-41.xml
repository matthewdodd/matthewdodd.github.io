                     <interp inst="t17420909-41-defend480" type="gender" value="female"></interp>
                  </persName>
               
            , * was indicted for <rs id="t17420909-41-off213" type="offenceDescription">
                     <interp inst="t17420909-41-off213" type="offenceCategory" value="theft"></interp>
                     <interp inst="t17420909-41-off213" type="offenceSubcategory" value="grandLarceny"></interp>
