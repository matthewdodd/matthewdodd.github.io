                        <interp inst="t18731215-66-verdict-1" type="verdictSubcategory" value="no_subcategory"></interp>GUILTY</rs>
                  </hi>—<hi rend="italic">
                     <rs id="t18731215-66-punishment-13" type="punishmentDescription">
