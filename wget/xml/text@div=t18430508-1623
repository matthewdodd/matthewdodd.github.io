                        <interp inst="t18430508-name-1141" type="gender" value="indeterminate"></interp>
                        <join result="offenceVictim" targOrder="Y" targets="t18430508-1623-offence-1 t18430508-name-1141"></join>Gas Light and Coke Company</persName>, and of <persName id="t18430508-name-1142" type="victimName">
