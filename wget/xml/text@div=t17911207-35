                     <join result="defendantPunishment" targOrder="Y" targets="t17911207-35-defend379 t17911207-35-punish197"></join>
               Whipped
            </rs>, and <rs id="t17911207-35-punish198" type="punishmentDescription">
                     <interp inst="t17911207-35-punish198" type="punishmentCategory" value="imprison"></interp>
