                     <interp inst="t18500204-450-offence-1" type="offenceCategory" value="theft"></interp>
                     <interp inst="t18500204-450-offence-1" type="offenceSubcategory" value="simpleLarceny"></interp>, stealing 1 copper scoop, value 8<hi rend="italic">s</hi>.; the goods of <persName id="t18500204-name-379" type="victimName">
                        <interp inst="t18500204-name-379" type="gender" value="male"></interp>
                        <interp inst="t18500204-name-379" type="surname" value="BURTON"></interp>
