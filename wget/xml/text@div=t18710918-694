                     <hi rend="smallCaps">FRANCIS SPURRETT</hi>
                  </persName> (57), and <persName id="def3-694-18710918" type="defendantName">
                     <interp inst="def3-694-18710918" type="gender" value="female"></interp>
                     <interp inst="def3-694-18710918" type="age" value="26"></interp>
