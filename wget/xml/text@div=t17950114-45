               <p>
                  <xptr type="pageFacsimile" doc="179501140081"></xptr>108. 
               
                  <persName id="t17950114-45-defend389" type="defendantName">
                     ANN 
                     SMITH
                  <interp inst="t17950114-45-defend389" type="surname" value="SMITH"></interp>
