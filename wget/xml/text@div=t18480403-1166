                     <interp inst="def2-1166-18480403" type="given" value="GEORGE"></interp>
                     <hi rend="largeCaps">
GEORGE SMITH</hi>
                  </persName>, said<persName id="def3-1166-18480403" type="defendantName">
                     <interp inst="def3-1166-18480403" type="gender" value="male"></interp>
