               <interp inst="t17700117-3-person76" type="surname" value="Corby"></interp>
                     <interp inst="t17700117-3-person76" type="given" value="William"></interp>
                     <interp inst="t17700117-3-person76" type="gender" value="male"></interp>
                  </persName>
            , and 
               <persName id="t17700117-3-person77">
                  Christopher
                   Busby
               <interp inst="t17700117-3-person77" type="surname" value="Busby"></interp>
