               <p>
                  <xptr type="pageFacsimile" doc="182710250101"></xptr>
            
                  <persName id="t18271025-246-person1276">
                  JOHN 
                  HAINES
               <interp inst="t18271025-246-person1276" type="surname" value="HAINES"></interp>
                     <interp inst="t18271025-246-person1276" type="given" value="JOHN"></interp>
