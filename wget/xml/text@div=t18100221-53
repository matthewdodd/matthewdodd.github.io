                     <join result="offenceCrimeDate" targOrder="Y" targets="t18100221-53-off309 t18100221-53-cd310"></join> was <rs id="t18100221-53-deflabel311" type="occupation">servant</rs>
                     <join result="persNameOccupation" targOrder="Y" targets="t18100221-53-defend595 t18100221-53-deflabel311"></join> to 
                     
                        <persName id="t18100221-53-victim597" type="victimName">
