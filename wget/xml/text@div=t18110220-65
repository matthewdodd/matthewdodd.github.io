                  <persName id="t18110220-65-person647">
                  JAMES 
                  HADLEY
               <interp inst="t18110220-65-person647" type="surname" value="HADLEY"></interp>
                     <interp inst="t18110220-65-person647" type="given" value="JAMES"></interp>
                     <interp inst="t18110220-65-person647" type="gender" value="male"></interp>
