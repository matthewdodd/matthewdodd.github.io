                  <join result="persNameOccupation" targOrder="Y" targets="t18280703-202-defend1121 t18280703-202-deflabel1002"></join>, and 
               <persName id="t18280703-202-defend1122" type="defendantName">
                  MARY 
                  CALLAGHAN
               <interp inst="t18280703-202-defend1122" type="surname" value="CALLAGHAN"></interp>
