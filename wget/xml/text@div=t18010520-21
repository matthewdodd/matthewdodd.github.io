                     <interp inst="t18010520-21-victim167" type="gender" value="male"></interp>
                  </persName>
            , 
               
                  <persName id="t18010520-21-victim169" type="victimName">
                     Thomas 
                     Meux
                  <interp inst="t18010520-21-victim169" type="surname" value="Meux"></interp>
                     <interp inst="t18010520-21-victim169" type="given" value="Thomas"></interp>
                     <interp inst="t18010520-21-victim169" type="gender" value="male"></interp>
                  </persName>
               
            , 
               
                  <persName id="t18010520-21-victim171" type="victimName">
                     Andrew 
                     Read
                  <interp inst="t18010520-21-victim171" type="surname" value="Read"></interp>
                     <interp inst="t18010520-21-victim171" type="given" value="Andrew"></interp>
                     <interp inst="t18010520-21-victim171" type="gender" value="male"></interp>
