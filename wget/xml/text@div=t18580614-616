                        <interp inst="t18580614-616-verdict-1" type="verdictSubcategory" value="no_subcategory"></interp>GUILTY</rs>
                  </hi>.—<hi rend="italic">
                     <rs id="t18580614-616-punishment-14" type="punishmentDescription">
                        <interp inst="t18580614-616-punishment-14" type="punishmentCategory" value="imprison"></interp>
