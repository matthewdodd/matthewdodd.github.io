<div1 type="trialAccount" id="t17490411-8">
               <interp inst="t17490411-8" type="collection" value="BAILEY"></interp>
               <interp inst="t17490411-8" type="year" value="1749"></interp>
               <interp inst="t17490411-8" type="uri" value="sessionsPapers/17490411"></interp>
