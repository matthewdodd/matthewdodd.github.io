                     <interp inst="t18150215-41-victim366" type="gender" value="male"></interp>
                  </persName>
               
            , and 
               
                  <persName id="t18150215-41-victim368" type="victimName">
                     William 
                     Cartwright
                  <interp inst="t18150215-41-victim368" type="surname" value="Cartwright"></interp>
