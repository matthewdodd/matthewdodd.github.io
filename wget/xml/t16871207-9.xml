               <p>
            
               
                  <persName id="t16871207-9-defend44" type="defendantName">
                     John 
                     Bownes
                  <interp inst="t16871207-9-defend44" type="surname" value="Bownes"></interp>
                     <interp inst="t16871207-9-defend44" type="given" value="John"></interp>
                     <interp inst="t16871207-9-defend44" type="gender" value="male"></interp>
                  </persName>
               
            , Indicted for <rs id="t16871207-9-off33" type="offenceDescription">
                     <interp inst="t16871207-9-off33" type="offenceCategory" value="theft"></interp>
                     <interp inst="t16871207-9-off33" type="offenceSubcategory" value="grandLarceny"></interp>
