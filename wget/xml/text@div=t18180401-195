                  <persName id="t18180401-195-person1364">
                  ALFRED 
                  BRATHWAITE
               <interp inst="t18180401-195-person1364" type="surname" value="BRATHWAITE"></interp>
                     <interp inst="t18180401-195-person1364" type="given" value="ALFRED"></interp>
                     <interp inst="t18180401-195-person1364" type="gender" value="male"></interp>
