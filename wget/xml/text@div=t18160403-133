                  <persName id="t18160403-133-person1009">
                  GEORGE 
                  DAVENPORT
               <interp inst="t18160403-133-person1009" type="surname" value="DAVENPORT"></interp>
                     <interp inst="t18160403-133-person1009" type="given" value="GEORGE"></interp>
