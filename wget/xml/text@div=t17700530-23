            . I <rs id="t17700530-23-viclabel116" type="occupation">keep the <placeName id="t17700530-23-crimeloc117">Black-Bull, Gray's-Inn-Lane</placeName>
                     <interp inst="t17700530-23-crimeloc117" type="placeName" value="Black-Bull, Gray's-Inn-Lane"></interp>
                     <interp inst="t17700530-23-crimeloc117" type="type" value="crimeLocation"></interp>
                     <join result="offencePlace" targOrder="Y" targets="t17700530-23-off114 t17700530-23-crimeloc117"></join>
