                     <interp inst="t18091101-1-off1" type="offenceCategory" value="breakingPeace"></interp>
                     <interp inst="t18091101-1-off1" type="offenceSubcategory" value="wounding"></interp>
               he on the <rs id="t18091101-1-cd2" type="crimeDate">4th of January</rs>
                     <join result="offenceCrimeDate" targOrder="Y" targets="t18091101-1-off1 t18091101-1-cd2"></join>, upon 
                     
                        <persName id="t18091101-1-victim50" type="victimName">
                           Robert 
                           Nixon
                        <interp inst="t18091101-1-victim50" type="surname" value="Nixon"></interp>
