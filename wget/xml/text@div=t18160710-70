               <p>
            
                  <persName id="t18160710-70-person510">
                  ELIZABETH 
                  NICHOLS
               <interp inst="t18160710-70-person510" type="surname" value="NICHOLS"></interp>
                     <interp inst="t18160710-70-person510" type="given" value="ELIZABETH"></interp>
                     <interp inst="t18160710-70-person510" type="gender" value="female"></interp>
