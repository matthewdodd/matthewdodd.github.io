                     <interp inst="t17320525-12-defend162" type="gender" value="male"></interp>
                  </persName>
               
             (was a 2d time) and 
               
                  <persName id="t17320525-12-defend164" type="defendantName">
                     Michael 
                     Shaw 
                  <interp inst="t17320525-12-defend164" type="surname" value="Shaw"></interp>
                     <interp inst="t17320525-12-defend164" type="given" value="Michael"></interp>
                     <interp inst="t17320525-12-defend164" type="gender" value="male"></interp>
                  </persName>
                
            , were indicted for <rs id="t17320525-12-off98" type="offenceDescription">
                     <interp inst="t17320525-12-off98" type="offenceCategory" value="theft"></interp>
                     <interp inst="t17320525-12-off98" type="offenceSubcategory" value="burglary"></interp>
               breaking and entring the House of 
                     
                        <persName id="t17320525-12-victim166" type="victimName">
                           Elizabeth 
                           Webb 
                        <interp inst="t17320525-12-victim166" type="surname" value="Webb"></interp>
                        <interp inst="t17320525-12-victim166" type="given" value="Elizabeth"></interp>
