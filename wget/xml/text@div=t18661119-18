                        <interp inst="t18661119-18-verdict-1" type="verdictSubcategory" value="no_subcategory"></interp>GUILTY</rs>
                  </hi>.—<hi rend="italic">

                     <rs id="t18661119-18-punishment-16" type="punishmentDescription">
