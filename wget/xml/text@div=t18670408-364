               <p>
                  <hi rend="largeCaps">CHILDS</hi> 
                  <hi rend="smallCaps">
                     <rs id="t18670408-364-verdict-1" type="verdictDescription">
                        <interp inst="t18670408-364-verdict-1" type="verdictCategory" value="guilty"></interp>
                        <interp inst="t18670408-364-verdict-1" type="verdictSubcategory" value="pleadedGuilty"></interp>PLEADED GUILTY</rs>
                  </hi>.—<hi rend="italic">
                     <rs id="t18670408-364-punishment-15" type="punishmentDescription">
                        <interp inst="t18670408-364-punishment-15" type="punishmentCategory" value="imprison"></interp>
