                  <persName id="t18310908-85-person771">
                  JOHN 
                  EGAN
               <interp inst="t18310908-85-person771" type="surname" value="EGAN"></interp>
                     <interp inst="t18310908-85-person771" type="given" value="JOHN"></interp>
