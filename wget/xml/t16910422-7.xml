                     <persName id="t16910422-7-victim53" type="victimName">
                        George 
                        Cromwell
                     <interp inst="t16910422-7-victim53" type="surname" value="Cromwell"></interp>
                        <interp inst="t16910422-7-victim53" type="given" value="George"></interp>
                        <interp inst="t16910422-7-victim53" type="gender" value="male"></interp>
