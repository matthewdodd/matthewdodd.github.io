                     <interp inst="t17200602-13-off61" type="offenceSubcategory" value="highwayRobbery"></interp>
               assaulting 
                     
                        <persName id="t17200602-13-victim82" type="victimName">
                           Mary 
                           Baldwin 
                        <interp inst="t17200602-13-victim82" type="surname" value="Baldwin"></interp>
