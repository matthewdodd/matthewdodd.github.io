               <p>989. 
               <persName id="t18270405-237-defend1319" type="defendantName">
                  GEORGE 
                  FINCH
               <interp inst="t18270405-237-defend1319" type="surname" value="FINCH"></interp>
                     <interp inst="t18270405-237-defend1319" type="given" value="GEORGE"></interp>
