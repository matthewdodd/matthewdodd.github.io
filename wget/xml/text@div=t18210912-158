                  <persName id="t18210912-158-person1643">
                  CHARLES 
                  DALE
               <interp inst="t18210912-158-person1643" type="surname" value="DALE"></interp>
                     <interp inst="t18210912-158-person1643" type="given" value="CHARLES"></interp>
                     <interp inst="t18210912-158-person1643" type="gender" value="male"></interp>
