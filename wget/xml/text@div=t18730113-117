                        <interp inst="t18730113-117-verdict-2" type="verdictSubcategory" value="no_subcategory"></interp>GUILTY</rs>
                  </hi>—<hi rend="italic">
                     <rs id="t18730113-117-punishment-16" type="punishmentDescription">
