               <p>
                  <xptr type="pageFacsimile" doc="179207040021"></xptr>326. 
               
                  <persName id="t17920704-16-defend236" type="defendantName">
                     MARY 
                     STINSTON
                  <interp inst="t17920704-16-defend236" type="surname" value="STINSTON"></interp>
