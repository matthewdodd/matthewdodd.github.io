                     <interp inst="t18280221-192-off1021" type="offenceCategory" value="theft"></interp>
                     <interp inst="t18280221-192-off1021" type="offenceSubcategory" value="simpleLarceny"></interp>
               stealing, on<xptr type="pageFacsimile" doc="182802210112"></xptr>the <rs id="t18280221-192-cd1022" type="crimeDate">11th of January</rs>
                     <join result="offenceCrimeDate" targOrder="Y" targets="t18280221-192-off1021 t18280221-192-cd1022"></join>, 50 pieces of wood, value 5s.
            </rs>, the goods of 
               <persName id="t18280221-192-victim1141" type="victimName">
                  Samuel 
                  Puddefoot
               <interp inst="t18280221-192-victim1141" type="surname" value="Puddefoot"></interp>
                     <interp inst="t18280221-192-victim1141" type="given" value="Samuel"></interp>
