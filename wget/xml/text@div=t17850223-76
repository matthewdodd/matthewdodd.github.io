               <p>367. 
               <persName id="t17850223-76-defend760" type="defendantName">
                  
                     JAMES 
                     LAPIER
                  , otherwise <rs id="t17850223-76-alias-6" type="alias">
                        <join result="nameAlias" targOrder="Y" targets="t17850223-76-defend760 t17850223-76-alias-6"></join>CHURCH</rs>
