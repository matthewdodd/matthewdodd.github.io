             and 
               
                  <persName id="t18160710-125-defend874" type="defendantName">
                     GEORGE 
                     PHIPPS
                  <interp inst="t18160710-125-defend874" type="surname" value="PHIPPS"></interp>
                     <interp inst="t18160710-125-defend874" type="given" value="GEORGE"></interp>
