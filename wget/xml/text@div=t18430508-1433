                     <interp inst="t18430508-1433-offence-2" type="offenceSubcategory" value="burglary"></interp>1 gown, 1<hi rend="italic">s</hi>.; 1 apron, 1<hi rend="italic">s</hi>.; 1 petticoat, 1<hi rend="italic">s</hi>.; and 1 bed-gown, 1<hi rend="italic">s</hi>.; the goods of <persName id="t18430508-name-253" type="victimName">
                        <interp inst="t18430508-name-253" type="gender" value="female"></interp>
                        <interp inst="t18430508-name-253" type="surname" value="WILTON"></interp>
