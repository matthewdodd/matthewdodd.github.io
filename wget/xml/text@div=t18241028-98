                     <join result="offenceCrimeDate" targOrder="Y" targets="t18241028-98-off522 t18241028-98-cd523"></join>, at <placeName id="t18241028-98-crimeloc524">St. James's, Westminster</placeName>
                     <interp inst="t18241028-98-crimeloc524" type="placeName" value="St. James's, Westminster"></interp>
