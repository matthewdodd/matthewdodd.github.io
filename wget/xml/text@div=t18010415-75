               <persName id="t18010415-75-person820">
                  James 
                  Tims
               <interp inst="t18010415-75-person820" type="surname" value="Tims"></interp>
                     <interp inst="t18010415-75-person820" type="given" value="James"></interp>
                     <interp inst="t18010415-75-person820" type="gender" value="male"></interp>
