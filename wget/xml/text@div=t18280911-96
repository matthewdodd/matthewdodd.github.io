               <p>1653. 
               <persName id="t18280911-96-defend652" type="defendantName">
                  MICHAEL 
                  BURKE
               <interp inst="t18280911-96-defend652" type="surname" value="BURKE"></interp>
                     <interp inst="t18280911-96-defend652" type="given" value="MICHAEL"></interp>
