<div1 type="trialAccount" id="t17351210-74">
               <interp inst="t17351210-74" type="collection" value="BAILEY"></interp>
               <interp inst="t17351210-74" type="year" value="1735"></interp>
               <interp inst="t17351210-74" type="uri" value="sessionsPapers/17351210"></interp>
