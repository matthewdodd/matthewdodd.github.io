<div1 type="trialAccount" id="t17520116-4">
               <interp inst="t17520116-4" type="collection" value="BAILEY"></interp>
               <interp inst="t17520116-4" type="year" value="1752"></interp>
               <interp inst="t17520116-4" type="uri" value="sessionsPapers/17520116"></interp>
