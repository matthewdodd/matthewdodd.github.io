                  </persName> 
                  <hi rend="smallCaps">
                     <rs id="t18660409-426-verdict-1" type="verdictDescription">
                        <interp inst="t18660409-426-verdict-1" type="verdictCategory" value="guilty"></interp>
                        <interp inst="t18660409-426-verdict-1" type="verdictSubcategory" value="pleadedGuilty"></interp>PLEADED GUILTY</rs>
                  </hi>.—<hi rend="italic">
                     <rs id="t18660409-426-punishment-85" type="punishmentDescription">
                        <interp inst="t18660409-426-punishment-85" type="punishmentCategory" value="imprison"></interp>
