                     <interp inst="t16931012-42-off163" type="offenceCategory" value="theft"></interp>
                     <interp inst="t16931012-42-off163" type="offenceSubcategory" value="burglary"></interp>
               breaking the House of 
                     <persName id="t16931012-42-victim123" type="victimName">
                        William 
                        Marriott
                     <interp inst="t16931012-42-victim123" type="surname" value="Marriott"></interp>
