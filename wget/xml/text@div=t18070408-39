               <p>293. 
               
                  <persName id="t18070408-39-defend367" type="defendantName">
                     ELIZABETH 
                     BARNES
                  <interp inst="t18070408-39-defend367" type="surname" value="BARNES"></interp>
                     <interp inst="t18070408-39-defend367" type="given" value="ELIZABETH"></interp>
