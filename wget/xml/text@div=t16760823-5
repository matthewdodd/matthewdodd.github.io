                     <interp inst="t16760823-5-off16" type="offenceCategory" value="violentTheft"></interp>
                     <interp inst="t16760823-5-off16" type="offenceSubcategory" value="highwayRobbery"></interp>
               indicted for a Highway-man
            </rs>, and the 
               <persName id="t16760823-5-victim8" type="victimName">person<interp inst="t16760823-5-victim8" type="gender" value="male"></interp>
                     <join result="offenceVictim" targOrder="Y" targets="t16760823-5-off16 t16760823-5-victim8"></join>
