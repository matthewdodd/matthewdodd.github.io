                     <hi rend="largeCaps">ANN SMITH</hi>
                  </persName> and <hi rend="largeCaps">
                     <persName id="def2-1038-18420228" type="defendantName">
                        <interp inst="def2-1038-18420228" type="gender" value="female"></interp>
