                  <persName id="t18290409-214-person2023">
                  MARGARET 
                  NASH
               <interp inst="t18290409-214-person2023" type="surname" value="NASH"></interp>
                     <interp inst="t18290409-214-person2023" type="given" value="MARGARET"></interp>
                     <interp inst="t18290409-214-person2023" type="gender" value="female"></interp>
