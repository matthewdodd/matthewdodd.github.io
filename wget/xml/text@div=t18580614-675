               <join result="criminalCharge" id="t18580614-675-charge-4" targOrder="Y" targets="def1-675-18580614 t18580614-675-offence-2 t18580614-675-verdict-3"></join>
               <join result="criminalCharge" id="t18580614-675-charge-5" targOrder="Y" targets="def2-675-18580614 t18580614-675-offence-2 t18580614-675-verdict-3"></join>
