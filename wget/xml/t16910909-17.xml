                     <join result="defendantPunishment" targOrder="Y" targets="t16910909-17-defend59 t16910909-17-punish78"></join>
                     <join result="defendantPunishment" targOrder="Y" targets="t16910909-17-defend60 t16910909-17-punish78"></join>
               
                     <note>[Whipping. See summary.]</note>
               
                  </rs>
