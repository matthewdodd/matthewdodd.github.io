                     <interp inst="def1-501-18500204" type="given" value="HENRY"></interp>
                     <hi rend="largeCaps">HENRY FREEMAN</hi>
                  </persName> and <persName id="def2-501-18500204" type="defendantName">
                     <interp inst="def2-501-18500204" type="gender" value="male"></interp>
                     <interp inst="def2-501-18500204" type="age" value="19"></interp>
                     <interp inst="def2-501-18500204" type="surname" value="WALTER"></interp>
                     <interp inst="def2-501-18500204" type="given" value="ALFRED"></interp>
