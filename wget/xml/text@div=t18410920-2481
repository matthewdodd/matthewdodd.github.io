                     <interp inst="t18410920-2481-offence-1" type="offenceSubcategory" value="theftFromPlace"></interp> for stealing, on the <rs id="t18410920-cd-143" type="crimeDate">
                        <join result="offenceCrimeDate" targOrder="Y" targets="t18410920-2481-offence-1 t18410920-cd-143"></join>13th of August</rs>, 2 sheets, value 6<hi rend="italic">s</hi>.; 2 blankets, value 8<hi rend="italic">s</hi>.; 2 quilts, value 7<hi rend="italic">s</hi>.; 1 bolster, value 3<hi rend="italic">s</hi>.; 1 looking-glass and frame, value 2<hi rend="italic">s</hi>. 3<hi rend="italic">d</hi>.; 1 candlestick, value 9<hi rend="italic">d</hi>.; and 1 set of fire-irons, value 2<hi rend="italic">s</hi>. 6<hi rend="italic">d</hi>.; the goods of <persName id="t18410920-name-816" type="victimName">
                        <interp inst="t18410920-name-816" type="gender" value="male"></interp>
                        <interp inst="t18410920-name-816" type="surname" value="KENT"></interp>
