                  <persName id="t17990220-25-person226">
                  EDWARD
                   SMITH
               <interp inst="t17990220-25-person226" type="surname" value="SMITH"></interp>
                     <interp inst="t17990220-25-person226" type="given" value="EDWARD"></interp>
                     <interp inst="t17990220-25-person226" type="gender" value="male"></interp>
