                        <interp inst="t18680406-315-verdict-2" type="verdictSubcategory" value="no_subcategory"></interp>GUILTY</rs>
                  </hi>—<hi rend="italic">
                     <rs id="t18680406-315-punishment-14" type="punishmentDescription">
                        <interp inst="t18680406-315-punishment-14" type="punishmentCategory" value="imprison"></interp>
