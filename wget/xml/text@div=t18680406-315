               <interp inst="t18680406-315" type="date" value="18680406"></interp>
               <join result="criminalCharge" id="t18680406-315-charge-1" targOrder="Y" targets="def1-315-18680406 t18680406-315-offence-1 t18680406-315-verdict-1"></join>
