               <p>510. 
               
                  <persName id="t18200412-159-defend1572" type="defendantName">
                     JAMES 
                     TURNER
                  <interp inst="t18200412-159-defend1572" type="surname" value="TURNER"></interp>
                     <interp inst="t18200412-159-defend1572" type="given" value="JAMES"></interp>
