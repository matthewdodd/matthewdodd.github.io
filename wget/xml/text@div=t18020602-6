                  <persName id="t18020602-6-person95">
                  JOHN 
                  HEAPY
               <interp inst="t18020602-6-person95" type="surname" value="HEAPY"></interp>
                     <interp inst="t18020602-6-person95" type="given" value="JOHN"></interp>
                     <interp inst="t18020602-6-person95" type="gender" value="male"></interp>
                  </persName>
             sworn. - I am a <rs id="t18020602-6-viclabel33" type="occupation">shoe-maker</rs>
                  <join result="persNameOccupation" targOrder="Y" targets="t18020602-6-victim94 t18020602-6-viclabel33"></join> in <placeName id="t18020602-6-crimeloc34">Fore-street</placeName>
