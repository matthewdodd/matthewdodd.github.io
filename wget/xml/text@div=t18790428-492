                        <interp inst="t18790428-492-verdict-1" type="verdictSubcategory" value="no_subcategory"></interp>GUILTY</rs>
                  </hi>.—<hi rend="italic">
                     <rs id="t18790428-492-punishment-47" type="punishmentDescription">
                        <interp inst="t18790428-492-punishment-47" type="punishmentCategory" value="imprison"></interp>
