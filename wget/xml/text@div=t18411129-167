                        <interp inst="t18411129-name-933" type="given" value="WILLIAM"></interp>
                        <interp inst="t18411129-name-933" type="occupation" value="oilman"></interp>
                        <join result="offenceVictim" targOrder="Y" targets="t18411129-167-offence-1 t18411129-name-933"></join>William Skinner</persName>.</rs>
